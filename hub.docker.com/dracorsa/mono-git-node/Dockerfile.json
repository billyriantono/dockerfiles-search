{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "dracorsa/mono-git:latest",
      "SourceCode": "FROM dracorsa/mono-git:latest",
      "Platform": "",
      "From": {
        "Image": "dracorsa/mono-git:latest"
      },
      "Commands": [
        {
          "Name": "user",
          "User": "root"
        },
        {
          "Env": [
            {
              "Key": "CI",
              "Value": "true"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "NPM_VERSION",
              "Value": "latest"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get -q update \u0026\u0026     apt-get install -y apt-transport-https"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"deb https://dl.yarnpkg.com/debian/ stable main\" | tee /etc/apt/sources.list.d/yarn.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -sL https://deb.nodesource.com/setup_12.x | bash -"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get -q update \u0026\u0026     apt-get install -y --force-yes     build-essential     libsecret-1-dev     nodejs     yarn     \u0026\u0026 rm -rf /var/lib/apt/lists/* /tmp/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "npm install -g gulp-cli"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "npm install npm@$NPM_VERSION -g"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "gitlabci"
        }
      ]
    }
  ]
