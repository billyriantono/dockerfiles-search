{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:14.04",
      "SourceCode": "FROM ubuntu:14.04",
      "Platform": "",
      "From": {
        "Image": "ubuntu:14.04"
      },
      "Commands": [
        {
          "Maintainer": "IBM Swift Engineering at IBM Cloud",
          "Name": "maintainer"
        },
        {
          "Labels": [
            {
              "Key": "Description",
              "Value": "\"Linux Ubuntu 14 image with the latest Swift binaries.\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "SWIFT_SNAPSHOT",
              "Value": "swift-DEVELOPMENT-SNAPSHOT-2016-06-06-a"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "UBUNTU_VERSION",
              "Value": "ubuntu14.04"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "UBUNTU_VERSION_NO_DOTS",
              "Value": "ubuntu1404"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "/root"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "WORK_DIR",
              "Value": "/root"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "${WORK_DIR}"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y   automake   build-essential   clang   curl   gcc-4.8   git   g++-4.8   libblocksruntime-dev   libbsd-dev   libcurl4-gnutls-dev   libcurl3   libglib2.0-dev   libpython2.7   libicu-dev   libkqueue-dev   libtool   openssh-client   wget   binutils-gold"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget https://swift.org/builds/development/$UBUNTU_VERSION_NO_DOTS/$SWIFT_SNAPSHOT/$SWIFT_SNAPSHOT-$UBUNTU_VERSION.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar xzvf $SWIFT_SNAPSHOT-$UBUNTU_VERSION.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$WORK_DIR/$SWIFT_SNAPSHOT-$UBUNTU_VERSION/usr/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "swiftc -h"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm /usr/bin/ld \u0026\u0026 ln -s /usr/bin/ld.gold /usr/bin/ld"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "git clone -b experimental/foundation https://github.com/apple/swift-corelibs-libdispatch.git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd swift-corelibs-libdispatch \u0026\u0026 git submodule init \u0026\u0026 git submodule update \u0026\u0026 sh ./autogen.sh \u0026\u0026 CFLAGS=-fuse-ld=gold ./configure --with-swift-toolchain=$WORK_DIR/$SWIFT_SNAPSHOT-$UBUNTU_VERSION/usr --prefix=$WORK_DIR/$SWIFT_SNAPSHOT-$UBUNTU_VERSION/usr \u0026\u0026 make \u0026\u0026 make install"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}