{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "usijo/docker-php5fpm",
      "SourceCode": "FROM usijo/docker-php5fpm",
      "Platform": "",
      "From": {
        "Image": "usijo/docker-php5fpm"
      },
      "Commands": [
        {
          "Maintainer": "Hongshun Yang\u003cojisucom@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "LIBICONV_VERSION",
              "Value": "1.15"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LIBICONV_DOWNLOAD_URL",
              "Value": "http://ftp.gnu.org/pub/gnu/libiconv/libiconv-$LIBICONV_VERSION.tar.gz"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LIBDATRIE_VERSION",
              "Value": "0.2.9"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LIBDATRIE_DOWNLOAD_URL",
              "Value": "ftp://sourceforge.mirrorservice.org/sites/distfiles.finkmirrors.net/libdatrie-$LIBDATRIE_VERSION.tar.xz"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk add -U --virtual build-deps autoconf build-base php5-dev git \u0026\u0026 cd /tmp/ \u0026\u0026 wget -qO- $LIBICONV_DOWNLOAD_URL | tar xvz \u0026\u0026 cd libiconv-$LIBICONV_VERSION \u0026\u0026 ./configure \u0026\u0026 make \u0026\u0026 make install \u0026\u0026 cd /tmp/ \u0026\u0026 wget $LIBDATRIE_DOWNLOAD_URL \u0026\u0026 tar xvf libdatrie-$LIBDATRIE_VERSION.tar.xz \u0026\u0026 cd libdatrie-$LIBDATRIE_VERSION \u0026\u0026 ./configure \u0026\u0026 make \u0026\u0026 make install \u0026\u0026 cd /tmp/ \u0026\u0026 git clone https://github.com/wulijun/php-ext-trie-filter.git \u0026\u0026 cd php-ext-trie-filter \u0026\u0026 phpize \u0026\u0026 ./configure \u0026\u0026 make \u0026\u0026 make install \u0026\u0026 cd ~ \u0026\u0026 rm -rf /tmp/* \u0026\u0026 apk del -U build-deps \u0026\u0026 rm -rf /var/cache/apk/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"extension=trie_filter.so\" \u003e /etc/php5/conf.d/trie_filter.ini"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}