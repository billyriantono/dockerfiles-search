{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:18.04",
      "SourceCode": "FROM ubuntu:18.04",
      "Platform": "",
      "From": {
        "Image": "ubuntu:18.04"
      },
      "Commands": [
        {
          "Maintainer": "Tomasz Jankowski",
          "Name": "maintainer"
        },
        {
          "Name": "workdir",
          "Path": "/root"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026     apt-get install --no-install-recommends -yy build-essential perl libxml2-dev zlib1g-dev libxml2-utils clang llvm-dev bison flex wget python python3 git python3-pip ssh \u0026\u0026     apt-get clean \u0026\u0026     apt-get autoclean"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget --no-check-certificate https://www.omnetpp.org/omnetpp/send/30-omnet-releases/2327-omnetpp-5-3-core --referer=https://omnetpp.org/omnetpp -O omnetpp-5.3-src-core.tgz --progress=dot:giga; exit 0"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar xf omnetpp-5.3-src-core.tgz \u0026\u0026     rm omnetpp-5.3-src-core.tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget --no-check-certificate https://github.com/inet-framework/inet/releases/download/v3.6.4/inet-3.6.4-src.tgz -O inet.tgz; echo 0"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar xf inet.tgz \u0026\u0026     rm inet.tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/root/omnetpp-5.3"
        },
        {
          "CmdLine": [
            "echo \"CFLAGS_RELEASE='-O3 -DNDEBUG=1 -D_XOPEN_SOURCE'\" \u003e\u003e configure.user \u0026\u0026     echo \"CXXFLAGS='-std=c++14'\" \u003e\u003e configure.user \u0026\u0026     sed -i 's/PREFER_CLANG=yes/PREFER_CLANG=no/g' configure.user"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/root/omnetpp-5.3/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LD_LIBRARY_PATH",
              "Value": "/root/omnetpp-5.3/lib:$LD_LIBRARY_PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "./configure WITH_TKENV=no WITH_QTENV=no WITH_OSG=no WITH_OSGEARTH=no \u0026\u0026     make -j $(nproc) MODE=release VERBOSE=1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/root/inet"
        },
        {
          "CmdLine": [
            "make makefiles \u0026\u0026     make -j $(nproc) MODE=release VERBOSE=1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "travis.sh",
            "/root"
          ]
        },
        {
          "CmdLine": [
            "/root/travis.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}