{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8",
      "SourceCode": "FROM openjdk:8",
      "Platform": "",
      "From": {
        "Image": "openjdk:8"
      },
      "Commands": [
        {
          "Maintainer": "Lauri Kuittinen \u003clauri.kuittinen@gofore.com\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y --no-install-recommends       build-essential       libmalaga-dev       execstack     \u0026\u0026 rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "VOIKKO_PREFIX",
              "Value": "/usr/local/voikko"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "VOIKKO_VERSION",
              "Value": "3.7.1"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p /tmp/voikko \u0026\u0026     cd /tmp/voikko \u0026\u0026     curl -sL \"http://www.puimula.org/voikko-sources/libvoikko/libvoikko-${VOIKKO_VERSION}.tar.gz\"     | gunzip | tar -x -C /tmp/voikko \u0026\u0026     cd libvoikko-${VOIKKO_VERSION} \u0026\u0026     ./configure --prefix ${VOIKKO_PREFIX} --with-dictionary-path=${VOIKKO_DICTS} \u0026\u0026     make     \u0026\u0026 make install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rf /tmp/voikko"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "execstack -c ${VOIKKO_PREFIX}/lib/libvoikko.so"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -sv ${VOIKKO_PREFIX}/lib/libvoikko.so /usr/lib/libvoikko.so"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ldconfig"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "SBT_VERSION",
              "Value": "0.13.11"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SBT_HOME",
              "Value": "/usr/local/sbt"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "${PATH}:${SBT_HOME}/bin"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl -sL \"http://dl.bintray.com/sbt/native-packages/sbt/$SBT_VERSION/sbt-$SBT_VERSION.tgz\" | gunzip | tar -x -C /usr/local \u0026\u0026     echo -ne \"- with sbt $SBT_VERSION\\n\" \u003e\u003e /root/.built"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/app"
        }
      ]
    }
  ]
}