  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:14.04",
      "SourceCode": "FROM ubuntu:14.04",
      "Platform": "",
      "From": {
        "Image": "ubuntu:14.04"
      },
      "Commands": [
        {
          "Maintainer": "Denis Darii \u003cdenis.darii@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get upgrade -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install python2.7-dev wget -y sqlite3 libjpeg-dev"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget https://pypi.python.org/packages/source/d/django-lfs/django-lfs-installer-0.9.0a1.tar.gz \u0026\u0026 tar xzf django-lfs-installer-0.9.0a1.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/lfs-installer"
        },
        {
          "CmdLine": [
            "/usr/bin/python2.7 bootstrap.py"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bin/buildout -v"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed --in-place \"s/'ENGINE': 'django.db.backends.',/'ENGINE': 'django.db.backends.sqlite3',/\" /lfs-installer/lfs_project/settings.py"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed --in-place \"s/'NAME': '',/'NAME': '\\/lfs-installer\\/lfs_project\\/demo-lfs-db.sqlite3',/\" /lfs-installer/lfs_project/settings.py"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed --in-place \"s/path = open(os.path.join(os.path.dirname(__file__), \\\"..\\\", \\\"utils\\\", \\\"data\\\"))/path = os.path.join(os.path.dirname(__file__), \\\"..\\\", \\\"utils\\\", \\\"data\\\")/\" /lfs-installer/eggs/django_lfs-0.9.0a1-py2.7.egg/lfs/utils/generator.py"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bin/django syncdb --noinput"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"from django.contrib.auth.models import User; User.objects.create_superuser('admin', 'admin@example.com', 'admin')\" | bin/django shell"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bin/django migrate"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bin/django lfs_init"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bin/django collectstatic --noinput"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bin/django lfs_generate_products"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "8000"
          ]
        },
        {
          "CmdLine": [
            "bin/django",
            "runserver",
            "0.0.0.0:8000"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
