      "BaseName": "docker:git",
      "SourceCode": "FROM docker:git",
      "Platform": "",
      "From": {
        "Image": "docker:git"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "DOCKER_MACHINE_BUCKET",
              "Value": "github.com/docker/machine/releases/download"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DOCKER_MACHINE_VERSION",
              "Value": "v0.15.0"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DOCKER_MACHINE_SHA256",
              "Value": "44c5c62db13b6eb6a9d3e276c1181401c78327ff6303570936ba2cf5d137b7b5"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "set -ex \u0026\u0026 apk add --no-cache libcurl curl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "set -ex \u0026\u0026 curl -fSL \"https://${DOCKER_MACHINE_BUCKET}/${DOCKER_MACHINE_VERSION}/docker-machine-`uname -s`-`uname -m`\" -o docker-machine \u0026\u0026 echo \"${DOCKER_MACHINE_SHA256}  docker-machine\" | sha256sum -c - \u0026\u0026 mv docker-machine /usr/local/bin/ \u0026\u0026 chmod +x /usr/local/bin/docker-machine \u0026\u0026 docker-machine -v"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apk add --no-cache python python-dev libxml2-dev alpine-sdk openssl-dev libffi-dev"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -L https://bootstrap.pypa.io/get-pip.py -o /usr/local/bin/get-pip.py"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "python /usr/local/bin/get-pip.py"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "pip install pywinrm boto boto3 markupsafe jinja2 docker-py cryptography ansible awscli"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "docker --version \u0026\u0026 docker-machine --version \u0026\u0026 python --version \u0026\u0026 aws --version \u0026\u0026 ansible --version"
