{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "lavaliere/jenkins-base",
      "SourceCode": "FROM lavaliere/jenkins-base",
      "Platform": "",
      "From": {
        "Image": "lavaliere/jenkins-base"
      },
      "Commands": [
        {
          "Maintainer": "Tracy Kennedy",
          "Name": "maintainer"
        },
        {
          "Name": "user",
          "User": "jenkins"
        },
        {
          "Name": "workdir",
          "Path": "/usr/lib/jenkins"
        },
        {
          "CmdLine": [
            "curl -L -O -w \"Downloaded: %{url_effective}\\\\n\" \"http://nectar-downloads.cloudbees.com/nectar/latest/jenkins.war\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "22",
            "33848",
            "443",
            "8080",
            "8888",
            "9990"
          ]
        },
        {
          "Env": [
            {
              "Key": "JENKINS_HOME",
              "Value": "/var/lib/jenkins"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "CmdLine": [
            "java",
            "-jar",
            "jenkins.war",
            "--httpPort=8080"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "--prefix=/jenkins"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}