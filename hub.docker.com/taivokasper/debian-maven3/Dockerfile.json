{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "taivokasper/debian-oracle-jdk:latest",
      "SourceCode": "FROM taivokasper/debian-oracle-jdk:latest",
      "Platform": "",
      "From": {
        "Image": "taivokasper/debian-oracle-jdk:latest"
      },
      "Commands": [
        {
          "Maintainer": "Taivo KÃ¤sper \u003ctaivo.kasper@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get update -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/opt"
        },
        {
          "CmdLine": [
            "wget --no-check-certificate http://www-eu.apache.org/dist/maven/maven-3/3.3.9/binaries/apache-maven-3.3.9-bin.tar.gz -O maven.tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar -xzf maven.tgz \u0026\u0026 rm maven.tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "M2_HOME",
              "Value": "/opt/apache-maven-3.3.9"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "M2",
              "Value": "$M2_HOME/bin"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "MAVEN_OPTS",
              "Value": "-Xms256m -Xmx512m"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/root"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "${PATH}:${M2_HOME}/bin"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mvn -version"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}