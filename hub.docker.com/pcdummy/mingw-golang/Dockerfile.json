{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:zesty",
      "SourceCode": "FROM ubuntu:zesty",
      "Platform": "",
      "From": {
        "Image": "ubuntu:zesty"
      },
      "Commands": [
        {
          "CmdLine": [
            "dpkg --add-architecture i386 \u0026\u0026 apt-get update \u0026\u0026 apt-get install -y --no-install-recommends \t\tg++ \t\tgcc \t\tlibc6-dev \t\tmake \t\tpkg-config \t\tca-certificates \t\twget \t\tgit \t\tssh \t\tmingw-w64 \t\tnsis \t\twine-stable \t\twine32 \t\u0026\u0026 rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GOLANG_VERSION",
              "Value": "1.9"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "cd /usr/local \u0026\u0026 wget https://storage.googleapis.com/golang/go${GOLANG_VERSION}.linux-amd64.tar.gz \u0026\u0026     tar zxf go${GOLANG_VERSION}.linux-amd64.tar.gz \u0026\u0026 rm go${GOLANG_VERSION}.linux-amd64.tar.gz \u0026\u0026     ln -s /usr/local/go/bin/go /usr/bin/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GOPATH",
              "Value": "/go"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$GOPATH/bin:/usr/local/go/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p \"$GOPATH/src\" \"$GOPATH/bin\" \u0026\u0026 chmod -R 777 \"$GOPATH\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "$GOPATH"
        }
      ]
    }
  ]
