{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8u162-jdk",
      "SourceCode": "FROM openjdk:8u162-jdk",
      "Platform": "",
      "From": {
        "Image": "openjdk:8u162-jdk"
      },
      "Commands": [
        {
          "Maintainer": "Carlos Sanchez \u003ccarlos@apache.org\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "JENKINS_SWARM_VERSION",
              "Value": "3.12"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JENKINS_SWARM_DOWNLOAD_SITE",
              "Value": "https://repo.jenkins-ci.org/releases/org/jenkins-ci/plugins/swarm-client"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JENKINS_HOME",
              "Value": "/home/jenkins-slave"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JENKINS_USER",
              "Value": "jenkins-slave"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y net-tools \u0026\u0026 rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "useradd -c \"Jenkins Slave user\" -d \"${JENKINS_HOME}\" -U -s /sbin/nologin -m \"${JENKINS_USER}\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl --create-dirs -sSLo /usr/share/jenkins/swarm-client-${JENKINS_SWARM_VERSION}.jar   ${JENKINS_SWARM_DOWNLOAD_SITE}/${JENKINS_SWARM_VERSION}/swarm-client-${JENKINS_SWARM_VERSION}.jar   \u0026\u0026 chmod 755 /usr/share/jenkins"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "jenkins-slave.sh",
            "/usr/local/bin/jenkins-slave.sh"
          ]
        },
        {
          "Name": "user",
          "User": "\"${JENKINS_USER}\""
        },
        {
          "CmdLine": [
            "/usr/local/bin/jenkins-slave.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}