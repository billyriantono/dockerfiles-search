{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "centos:centos7",
      "SourceCode": "FROM centos:centos7",
      "Platform": "",
      "From": {
        "Image": "centos:centos7"
      },
      "Commands": [
        {
          "Maintainer": "Steve Kamerman \"https://github.com/kamermans\"",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:/opt/dell/srvadmin/bin:/opt/dell/srvadmin/sbin"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "TOMCATCFG",
              "Value": "/opt/dell/srvadmin/lib64/openmanage/apache-tomcat/conf/server.xml"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "TERM",
              "Value": "xterm"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "USER",
              "Value": "root"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PASS",
              "Value": "password"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SYSTEMCTL_SKIP_REDIRECT",
              "Value": "1"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p /run/lock/subsys     \u0026\u0026 cp /etc/redhat-release /etc/.redhat-release.actual     \u0026\u0026 echo 'Red Hat Enterprise Linux Server release 6.2 (Santiago)' \u003e /etc/redhat-release"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "yum -y update     \u0026\u0026 yum -y install gcc wget perl passwd which tar libstdc++.so.6 compat-libstdc++-33.i686 glibc.i686         net-snmp net-snmp-sysvinit nano dmidecode libxml2.i686 strace less     \u0026\u0026 yum clean all"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "resources/snmpd.conf",
            "/etc/snmp/snmpd.conf"
          ]
        },
        {
          "CmdLine": [
            "/etc/init.d/snmpd start"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"$USER:$PASS\" | chpasswd"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget -q -O - http://linux.dell.com/repo/hardware/dsu/bootstrap.cgi | bash"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "yum -y update     \u0026\u0026 yum -y install         srvadmin-all         dell-system-update         ipmitool     \u0026\u0026 yum clean all"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed -i         -e 's/SSL_DHE_RSA_WITH_3DES_EDE_CBC_SHA/TLS_ECDHE_RSA_WITH_AES_128_CBC_SHA256/'         -e 's/TLS_DHE_RSA_WITH_AES_128_CBC_SHA/TLS_ECDHE_RSA_WITH_AES_128_CBC_SHA/'         -e 's/TLS_DHE_DSS_WITH_AES_128_CBC_SHA/TLS_ECDHE_RSA_WITH_AES_256_CBC_SHA384/'         -e 's/SSL_DHE_DSS_WITH_3DES_EDE_CBC_SHA/TLS_ECDHE_RSA_WITH_AES_256_CBC_SHA/' $TOMCATCFG     \u0026\u0026 ln -sf /opt/dell/srvadmin/lib64/libstorelibir-3.so /opt/dell/srvadmin/lib64/libstorelibir.so.5"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"dmidecode -t1\" \u003e\u003e ~/.bashrc"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "resources/init.sh",
            "/init.sh"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/opt/dell/srvadmin/bin"
        },
        {
          "CmdLine": [
            "/init.sh"
          ],
          "Name": "cmd",
          "PrependShell": false
        },
        {
          "Name": "expose",
          "Ports": [
            "1311",
            "161",
            "162"
          ]
        }
      ]
    }
  ]
}