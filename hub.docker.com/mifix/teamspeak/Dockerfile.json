{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "docker.io/ubuntu:wily",
      "SourceCode": "FROM docker.io/ubuntu:wily",
      "Platform": "",
      "From": {
        "Image": "docker.io/ubuntu:wily"
      },
      "Commands": [
        {
          "Maintainer": "wth-kiste",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "TS_VERSION",
              "Value": "3.0.12.4"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "useradd -m teamspeak \u0026\u0026     mkdir -p /home/teamspeak/data \u0026\u0026     chown -R teamspeak:teamspeak  /home/teamspeak/data"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "DEBIAN_FRONTEND=noninteractive apt-get -q update \u0026\u0026 apt-get install -qy --no-install-recommends ca-certificates curl bzip2"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -LS http://dl.4players.de/ts/releases/${TS_VERSION}/teamspeak3-server_linux_amd64-${TS_VERSION}.tar.bz2 | tar jxC /tmp   \u0026\u0026 mv /tmp/teamspeak3-server_linux_amd64 /opt/teamspeak   \u0026\u0026 apt-get clean   \u0026\u0026 rm -rf /var/lib/apt /tmp/* /var/tmp/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "10011",
            "30033",
            "9987/udp"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/home/teamspeak/data"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/opt/teamspeak"
        },
        {
          "Name": "user",
          "User": "teamspeak"
        },
        {
          "CmdLine": [
            "ln -s /home/teamspeak/data/ts3server.sqlitedb /opt/teamspeak/ts3server.sqlitedb"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "LD_LIBRARY_PATH=\"/opt/teamspeak\" /opt/teamspeak/ts3server logpath=/home/teamspeak/data/logs"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}