          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KAFKA_ADVERTISED_PORT",
              "Value": "9094"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KAFKA_PORT",
              "Value": "9092"
