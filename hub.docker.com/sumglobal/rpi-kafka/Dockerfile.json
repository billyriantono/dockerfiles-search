        {
          "Key": "scala_version",
          "Name": "arg",
          "Value": "2.12"
        },
        {
          "Labels": [
            {
              "Key": "maintainer",
              "Value": "\"Charles Walker \u003ccwalker@sumglobal.com\u003e, Chip Dickson \u003ccdickson@sumglobal.com\u003e\""
            }
          ],
          "Name": "label"
        },
        {
