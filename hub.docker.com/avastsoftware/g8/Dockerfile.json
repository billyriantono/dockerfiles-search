      "Platform": "",
      "From": {
        "Image": "openjdk:8u151-jre-alpine"
      },
      "Commands": [
        {
          "Labels": [
            {
              "Key": "org.label-schema.schema-version",
              "Value": "= \"1.0\""
            }
          ],
          "Name": "label"
        },
        {
          "Labels": [
            {
              "Key": "org.label-schema.description",
              "Value": "= \"A simple containerization of giter8. https://github.com/moredip/giter8\""
            }
          ],
          "Name": "label"
        },
        {
          "Labels": [
            {
              "Key": "org.label-schema.vcs-url",
