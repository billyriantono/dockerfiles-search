{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "docker.bintray.io/jfrog/artifactory-oss:5.3.1",
      "SourceCode": "FROM docker.bintray.io/jfrog/artifactory-oss:5.3.1",
      "Platform": "",
      "From": {
        "Image": "docker.bintray.io/jfrog/artifactory-oss:5.3.1"
      },
      "Commands": [
        {
          "Maintainer": "philipp.goellner@xaltbc.de",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "TIME_ZONE",
              "Value": "Europe/Berlin"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "INIT_DIRECTORY",
              "Value": "/docker/artifactory/init"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "TEMP_DIRECTORY",
              "Value": "/docker/artifactory/temp"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p ${INIT_DIRECTORY}  \u0026\u0026 mkdir -p ${TEMP_DIRECTORY}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "init/*",
            "${INIT_DIRECTORY}/"
          ]
        },
        {
          "CmdLine": [
            "chmod 740 ${INIT_DIRECTORY}/*.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update --quiet  \u0026\u0026 apt-get install -y --quiet openssh-client                                rsync  \u0026\u0026 apt-get clean"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "${ARTIFACTORY_DATA}"
          ]
        },
        {
          "CmdLine": [
            "/docker/artifactory/init/configure-and-start.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}