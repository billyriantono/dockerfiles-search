          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"JAVA_OPTS=\\\"\\$JAVA_OPTS -javaagent:/opt/jboss/newrelic/newrelic.jar\\\"\" \u003e\u003e /opt/jboss/keycloak/bin/standalone.conf"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}