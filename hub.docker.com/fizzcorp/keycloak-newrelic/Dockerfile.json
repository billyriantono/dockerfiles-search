{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "jboss/keycloak:3.4.3.Final",
      "SourceCode": "FROM jboss/keycloak:3.4.3.Final",
      "Platform": "",
      "From": {
        "Image": "jboss/keycloak:3.4.3.Final"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "NR_AGENT_VERSION",
              "Value": "3.30.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
