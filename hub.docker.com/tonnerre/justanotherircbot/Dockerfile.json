{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "debian:stretch",
      "SourceCode": "FROM debian:stretch",
      "Platform": "",
      "From": {
        "Image": "debian:stretch"
      },
      "Commands": [
        {
          "Maintainer": "Juliane Clausen \u003cgithub@juliane-clausen.de\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "GOPATH",
              "Value": "/usr/share/gocode:/usr/local"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/etc/justanotherircbot"
          ]
        },
        {
          "CmdLine": [
            "apt-get -q -y update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get -q -y -o Dpkg::Options::=--force-confdef -o Dpkg::Options::=--force-confold --purge install golang-go golang-golang-x-net-dev golang-golang-x-tools golang-golang-x-tools-dev golang-goprotobuf-dev golang-x-text-dev git-core mercurial protobuf-compiler"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "go get github.com/thoj/go-ircevent"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /usr/local/src/github.com/julianec"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd /usr/local/src/github.com/julianec \u0026\u0026 git clone https://github.com/julianec/justanotherircbot.git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd /usr/local/src/github.com/julianec/justanotherircbot \u0026\u0026 protoc --go_out=. config.proto"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd /usr/local/src/github.com/julianec/justanotherircbot \u0026\u0026 go build"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd /usr/local/src/github.com/julianec/justanotherircbot \u0026\u0026 go install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "addgroup --quiet --system --force-badname _ircbot"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "adduser --quiet --system --ingroup _ircbot --disabled-login --disabled-password --home /nonexistent --no-create-home --force-badname --gecos \"Just another IRC bot\" _ircbot"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "_ircbot"
        },
        {
          "CmdLine": [
            "/usr/local/bin/justanotherircbot"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "--config=/etc/justanotherircbot/justanotherircbot.conf"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}