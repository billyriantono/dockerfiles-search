{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "alpine:3.2",
      "SourceCode": "FROM       alpine:3.2",
      "Platform": "",
      "From": {
        "Image": "alpine:3.2"
      },
      "Commands": [
        {
          "Maintainer": "David Cuadrado \u003cdacuad@facebook.com\u003e",
          "Name": "maintainer"
        },
        {
          "Name": "expose",
          "Ports": [
            "9001"
          ]
        },
        {
          "Env": [
            {
              "Key": "GOPATH",
              "Value": "/go"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "APPPATH",
              "Value": "$GOPATH/src/github.com/lowstz/mongodb_exporter"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            ".",
            "$APPPATH"
          ]
        },
        {
          "CmdLine": [
            "apk add --update -t build-deps go git mercurial libc-dev gcc libgcc     \u0026\u0026 cd $APPPATH \u0026\u0026 go get -d \u0026\u0026 go build -o /bin/mongodb_exporter     \u0026\u0026 apk del --purge build-deps \u0026\u0026 rm -rf $GOPATH"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/bin/mongodb_exporter"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}