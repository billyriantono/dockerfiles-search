{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openshift/base-centos7",
      "SourceCode": "FROM openshift/base-centos7",
      "Platform": "",
      "From": {
        "Image": "openshift/base-centos7"
      },
      "Commands": [
        {
          "Maintainer": "Rabbit \u003c380303318@qq.com\u003e",
          "Name": "maintainer"
        },
        {
          "Name": "expose",
          "Ports": [
            "8080"
          ]
        },
        {
          "Env": [
            {
              "Key": "MAVEN_VERSION",
              "Value": "3.5.2"
            },
            {
              "Key": "APP_ROOT",
              "Value": "\"/opt/app-root/\""
            }
          ],
          "Name": "env"
        },
        {
          "Labels": [
            {
              "Key": "io.k8s.description",
              "Value": "\"Platform for building and running SpringBoot applications\""
            },
            {
              "Key": "io.k8s.display-name",
              "Value": "\"SpringBoot Builder\""
            },
            {
              "Key": "io.openshift.expose-services",
              "Value": "\"8080:http\""
            },
            {
              "Key": "io.openshift.tags",
              "Value": "\"builder,springboot\""
            },
            {
              "Key": "io.openshift.s2i.destination",
              "Value": "\"/opt/s2i/destination\""
            }
          ],
          "Name": "label"
        },
        {
          "CmdLine": [
            "INSTALL_PKGS=\"tar java-1.8.0-openjdk java-1.8.0-openjdk-devel\" \u0026\u0026     yum install -y --enablerepo=centosplus $INSTALL_PKGS \u0026\u0026     rpm -V $INSTALL_PKGS \u0026\u0026     yum clean all -y \u0026\u0026     (curl -v http://mirrors.tuna.tsinghua.edu.cn/apache/maven/maven-3/$MAVEN_VERSION/binaries/apache-maven-$MAVEN_VERSION-bin.tar.gz |     tar -zx -C /usr/local) \u0026\u0026     ln -sf /usr/local/apache-maven-$MAVEN_VERSION/bin/mvn /usr/local/bin/mvn \u0026\u0026     mkdir -p $HOME/.m2 \u0026\u0026     mkdir -p $APP_ROOT \u0026\u0026     mkdir -p /opt/s2i/destination"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "./contrib/settings.xml",
            "$HOME/.m2/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "./s2i/bin/",
            "$STI_SCRIPTS_PATH"
          ]
        },
        {
          "CmdLine": [
            "chmod -R a+rw $HOME \u0026\u0026     chmod -R +x $STI_SCRIPTS_PATH \u0026\u0026     chmod -R g+rw /opt/s2i/destination"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"root:root\" | chpasswd"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "1001"
        },
        {
          "CmdLine": [
            "$STI_SCRIPTS_PATH/usage"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}