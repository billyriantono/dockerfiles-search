{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "frolvlad/alpine-oraclejdk8:slim",
      "SourceCode": "FROM frolvlad/alpine-oraclejdk8:slim",
      "Platform": "",
      "From": {
        "Image": "frolvlad/alpine-oraclejdk8:slim"
      },
      "Commands": [
        {
          "Maintainer": "Scott Fan \u003cfancp2007@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "MAVEN_VERSION",
              "Value": "3.3.9"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk upgrade --update \u0026\u0026     apk add --update curl tar \u0026\u0026     mkdir -p /usr/share/maven \u0026\u0026     curl -fsSL https://repo1.maven.org/maven2/org/apache/maven/apache-maven/$MAVEN_VERSION/apache-maven-$MAVEN_VERSION-bin.tar.gz | tar -xzC /usr/share/maven --strip-components=1 \u0026\u0026     ln -s /usr/share/maven/bin/mvn /usr/bin/mvn \u0026\u0026     apk del curl tar \u0026\u0026     rm -rf /tmp/* /var/cache/apk/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "MAVEN_HOME",
              "Value": "/usr/share/maven"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/root/.m2"
          ]
        },
        {
          "CmdLine": [
            "mvn"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
