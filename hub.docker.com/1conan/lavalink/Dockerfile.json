{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "adoptopenjdk/openjdk11:alpine-jre",
      "SourceCode": "FROM adoptopenjdk/openjdk11:alpine-jre",
      "Platform": "",
      "From": {
        "Image": "adoptopenjdk/openjdk11:alpine-jre"
      },
      "Commands": [
        {
          "Name": "workdir",
          "Path": "/opt/Lavalink"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/config"
          ]
        },
        {
          "Env": [
            {
              "Key": "LAVALINK_VERSION",
              "Value": "3.2.1"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apk add wget nss mpg123"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget \"https://github.com/Frederikam/Lavalink/releases/download/${LAVALINK_VERSION}/Lavalink.jar\" -P /opt/Lavalink"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -s /config/application.yml /opt/Lavalink/application.yml"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "2333"
          ]
        },
        {
          "CmdLine": [
            "java",
            "-jar",
            "Lavalink.jar"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}