        },
        {
          "Key": "JENKINS_SHA",
          "Name": "arg",
          "Value": "564e49fbd180d077a22a8c7bb5b8d4d58d2a18ce"
        },
        {
          "Key": "TINI_VERSION",
          "Name": "arg",
          "Value": "0.9.0"
        },
        {
          "Key": "TINI_SHA",
          "Name": "arg",
          "Value": "fa23d1e20732501c3bb8eeeca423c89ac80ed452"
        },
        {
          "Key": "user",
          "Name": "arg",
          "Value": "jenkins"
        },
        {
          "Key": "group",
          "Name": "arg",
          "Value": "jenkins"
        },
        {
          "Key": "uid",
          "Name": "arg",
          "Value": "1000"
        },
        {
          "Key": "gid",
          "Name": "arg",
          "Value": "1000"
        },
        {
          "CmdLine": [
            "apt-get update     \u0026\u0026 apt-get install -y git curl zip     \u0026\u0026 rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "groupadd -g ${gid} ${group}     \u0026\u0026 useradd -d \"$JENKINS_HOME\" -u ${uid} -g ${gid} -m -s /bin/bash ${user}"
