          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "${JENKINS_SLAVE_AGENT_PORT}"
          ]
        },
        {
          "Name": "user",
          "User": "${user}"
        },
        {
          "CmdLine": [
            "mkdir -p ${JENKINS_REF}/init.groovy.d"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
