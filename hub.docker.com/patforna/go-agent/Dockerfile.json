{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "dockerfile/java",
      "SourceCode": "FROM dockerfile/java",
      "Platform": "",
      "From": {
        "Image": "dockerfile/java"
      },
      "Commands": [
        {
          "Maintainer": "Patric Fornasier \u003cpatric.fornasier@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "GO_DOWNLOAD_URL",
              "Value": "http://download01.thoughtworks.com/go/14.1.0/ga/go-agent-14.1.0-18882.deb"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget -O /tmp/go-agent.deb ${GO_DOWNLOAD_URL}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "dpkg -i /tmp/go-agent.deb"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -f /tmp/go-agent.deb"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed -i '/.*GO_SERVER.*/d' /etc/default/go-agent"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed -i 's/DAEMON=Y/DAEMON=N/' /etc/default/go-agent"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed -i 's/su -/su -p/' /etc/init.d/go-agent"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GO_SERVER",
              "Value": "go-server"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "su go -c '/etc/init.d/go-agent start'"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}