{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:openjdk-8",
      "SourceCode": "FROM java:openjdk-8",
      "Platform": "",
      "From": {
        "Image": "java:openjdk-8"
      },
      "Commands": [
        {
          "Maintainer": "negokaz \u003cnegokaz@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "BACK_CHANNELING_VERSION",
              "Value": "0.2.0-beta1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DATOMIC_URL",
              "Value": "datomic:free://localhost:4334/bc"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl --silent --location     --url \"https://github.com/kawasima/back-channeling/releases/download/v${BACK_CHANNELING_VERSION}/back-channeling-${BACK_CHANNELING_VERSION}-dist.zip\"     --output back-channeling.zip     \u0026\u0026 unzip back-channeling.zip     \u0026\u0026 rm back-channeling.zip     \u0026\u0026 mv \"back-channeling-${BACK_CHANNELING_VERSION}\" \"/opt/back-channeling\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/opt/back-channeling"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "entrypoint.sh",
            "bin/"
          ]
        },
        {
          "CmdLine": [
            "chmod a+x bin/entrypoint.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/opt/datomic/data"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "3009"
          ]
        },
        {
          "CmdLine": [
            "bin/entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}