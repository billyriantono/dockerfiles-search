{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "pinterb/base:alpine",
      "SourceCode": "FROM pinterb/base:alpine",
      "Platform": "",
      "From": {
        "Image": "pinterb/base:alpine"
      },
      "Commands": [
        {
          "Maintainer": "Brad Pinter \"brad.pinter@gmail.com\"",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "CONSUL_VERSION",
              "Value": "0.6.4"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CONSUL_SHA256",
              "Value": "abdf0e1856292468e2c9971420d73b805e93888e006c76324ae39416edcf0627"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GLIBC_PKG_VERSION",
              "Value": "2.23-r3"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DUMB_INIT_VERSION",
              "Value": "1.1.3"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk --update add --no-cache ca-certificates \u0026\u0026   curl -Lo /etc/apk/keys/sgerrand.rsa.pub \"https://github.com/sgerrand/alpine-pkg-glibc/releases/download/${GLIBC_PKG_VERSION}/sgerrand.rsa.pub\" \u0026\u0026   curl -Lo glibc-${GLIBC_PKG_VERSION}.apk \"https://github.com/sgerrand/alpine-pkg-glibc/releases/download/${GLIBC_PKG_VERSION}/glibc-${GLIBC_PKG_VERSION}.apk\" \u0026\u0026   curl -Lo glibc-bin-${GLIBC_PKG_VERSION}.apk \"https://github.com/sgerrand/alpine-pkg-glibc/releases/download/${GLIBC_PKG_VERSION}/glibc-bin-${GLIBC_PKG_VERSION}.apk\" \u0026\u0026   curl -Lo glibc-i18n-${GLIBC_PKG_VERSION}.apk \"https://github.com/sgerrand/alpine-pkg-glibc/releases/download/${GLIBC_PKG_VERSION}/glibc-i18n-${GLIBC_PKG_VERSION}.apk\" \u0026\u0026   apk add glibc-${GLIBC_PKG_VERSION}.apk glibc-bin-${GLIBC_PKG_VERSION}.apk glibc-i18n-${GLIBC_PKG_VERSION}.apk \u0026\u0026   rm -rf /var/cache/apk/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://releases.hashicorp.com/consul/${CONSUL_VERSION}/consul_${CONSUL_VERSION}_linux_amd64.zip",
            "/tmp/consul.zip"
          ]
        },
        {
          "CmdLine": [
            "echo \"${CONSUL_SHA256}  /tmp/consul.zip\" \u003e /tmp/consul.sha256   \u0026\u0026 sha256sum -c /tmp/consul.sha256   \u0026\u0026 cd /bin   \u0026\u0026 unzip /tmp/consul.zip   \u0026\u0026 chmod +x /bin/consul   \u0026\u0026 rm /tmp/consul.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -Lo /usr/local/bin/dumb-init \"https://github.com/Yelp/dumb-init/releases/download/v${DUMB_INIT_VERSION}/dumb-init_${DUMB_INIT_VERSION}_amd64\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod +x /usr/local/bin/dumb-init"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /consul/data \u0026\u0026     mkdir -p /consul/config"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/consul/data",
            "/consul/config"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "8300"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "8301",
            "8301/udp",
            "8302",
            "8302/udp"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "8400",
            "8500",
            "8600",
            "8600/udp"
          ]
        },
        {
          "CmdLine": [
            "/bin/consul"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "--help"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}