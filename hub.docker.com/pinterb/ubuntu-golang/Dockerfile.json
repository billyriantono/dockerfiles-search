{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "pinterb/ubuntu-base:0.0.10",
      "SourceCode": "FROM pinterb/ubuntu-base:0.0.10",
      "Platform": "",
      "From": {
        "Image": "pinterb/ubuntu-base:0.0.10"
      },
      "Commands": [
        {
          "Maintainer": "Brad Pinter brad.pinter@gmail.com",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "curl -sSL http://golang.org/dl/go1.3.src.tar.gz \t| tar -v -C /usr/src -xz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/usr/src/go"
        },
        {
          "Env": [
            {
              "Key": "GOLANG_CROSSPLATFORMS",
              "Value": "darwin/386 darwin/amd64 \tdragonfly/386 dragonfly/amd64 \tfreebsd/386 freebsd/amd64 freebsd/arm \tlinux/386 linux/amd64 linux/arm \tnacl/386 nacl/amd64p32 \tnetbsd/386 netbsd/amd64 netbsd/arm \topenbsd/386 openbsd/amd64 \tplan9/386 plan9/amd64 \tsolaris/amd64 \twindows/386 windows/amd64"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GOARM",
              "Value": "5"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "bash -xec '\t\tcd src; \t\tfor platform in $GOLANG_CROSSPLATFORMS; do \t\t\tGOOS=${platform%/*} \t\t\tGOARCH=${platform##*/} \t\t\t./make.bash --no-clean 2\u003e\u00261; \t\tdone \t'"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/usr/src/go/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p /go/src"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GOPATH",
              "Value": "/go"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/go/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/go"
        },
        {
          "CmdLine": [
            "/bin/bash"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}