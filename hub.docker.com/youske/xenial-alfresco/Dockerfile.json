{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "youske/ubuntu-stdmachine:base",
      "SourceCode": "FROM youske/ubuntu-stdmachine:base",
      "Platform": "",
      "From": {
        "Image": "youske/ubuntu-stdmachine:base"
      },
      "Commands": [
        {
          "Maintainer": "youske miyakoshi \u003cyouske@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "ALFRESCO_VERSION",
              "Value": "201605"
            },
            {
              "Key": "ALFRESCO_BUILD",
              "Value": "00010"
            },
            {
              "Key": "ALFRESCO_BASE",
              "Value": "/alfresco-community"
            },
            {
              "Key": "LANGUAGE",
              "Value": "ja_JP.UTF-8"
            },
            {
              "Key": "SHARE_PORT",
              "Value": "8080"
            },
            {
              "Key": "FTP_PORT",
              "Value": "21"
            },
            {
              "Key": "ADMIN_PASSWORD",
              "Value": "admin"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "PACKAGE=\"fontconfig libice6 libsm6 libxt6 libxrender1 libfontconfig1 libglu1-mesa libdbus-glib-1-2 libcups2 libxinerama1 xvfb imagemagick ghostscript xfonts-base\" \u0026\u0026     apt-get update \u0026\u0026 apt-get -y -q upgrade \u0026\u0026     apt-get install -y -q wget language-pack-ja ${PACKAGE}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "DOWNLOAD_URL=http://dl.alfresco.com/release/community/${ALFRESCO_VERSION}-build-${ALFRESCO_BUILD}/alfresco-community-installer-${ALFRESCO_VERSION}-linux-x64.bin \u0026\u0026     wget -q ${DOWNLOAD_URL} -O /tmp/alfresco-community.bin \u0026\u0026     chmod +x /tmp/alfresco-community.bin \u0026\u0026 /tmp/alfresco-community.bin --mode text --installer-language en     --prefix ${ALFRESCO_BASE}     --postgres_port 5432     --tomcat_server_port ${SHARE_PORT}     --tomcat_server_shutdown_port 8005     --tomcat_server_ssl_port 8443     --tomcat_server_ajp_port 8009     --libreoffice_port 8100     --alfresco_ftp_port ${FTP_PORT}     --alfresco_admin_password ${ADMIN_PASSWORD}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ALFRESCO_AMPS=${ALFRESCO_BASE}/amps \u0026\u0026     ALFRESCO_AMPSSHARE=${ALFRESCO_BASE}/amps_share \u0026\u0026     ALFRESCO_LIBREOFFICE=${ALFRESCO_BASE}/libreoffice \u0026\u0026     wget -qP ${ALFRESCO_AMPS} https://github.com/share-extras/js-console/releases/download/v0.6.0-rc1/javascript-console-repo-0.6.0.amp \u0026\u0026     wget -qP ${ALFRESCO_AMPS} https://github.com/Alfresco/alfresco-support-tools/releases/download/1.10/support-tools-1.10.amp \u0026\u0026     wget -qP ${ALFRESCO_AMPS} https://github.com/ntmcminn/alfresco-pdf-toolkit/releases/download/1.3-Beta3/pdf-toolkit-repo.amp \u0026\u0026     wget -qP ${ALFRESCO_AMPSSHARE} https://github.com/ntmcminn/alfresco-pdf-toolkit/releases/download/1.3-Beta3/pdf-toolkit-share.amp \u0026\u0026     wget -qP ${ALFRESCO_AMPSSHARE} https://github.com/teqnology/alfresco-header-share/blob/master/target/alfresco-header-share.amp?raw=true \u0026\u0026     ${ALFRESCO_BASE}/bin/apply_amps.sh \u0026\u0026     ln -s -f ${ALFRESCO_LIBREOFFICE}/scripts/libreoffice_ctl.sh ${ALFRESCO_LIBREOFFICE}/scripts/ctl.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "HEAPSIZE",
              "Value": "1024M"
            },
            {
              "Key": "MEMSIZE",
              "Value": "3096M"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "scripts/",
            "/alfresco_scripts"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "setenv.sh.tmpl",
            "${ALFRESCO_BASE}/tomcat/bin"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "enable.d/",
            "/tmp"
          ]
        },
        {
          "CmdLine": [
            "cd ${ALFRESCO_BASE}/tomcat/shared/classes/ \u0026\u0026     echo \"\\n\" \u003e\u003e alfresco-global.properties \u0026\u0026     cat /tmp/cifs \u003e\u003e alfresco-global.properties \u0026\u0026     cat /tmp/ftp \u003e\u003e alfresco-global.properties \u0026\u0026     cat /tmp/rmi \u003e\u003e alfresco-global.properties"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get autoremove \u0026\u0026 apt-get autoclean \u0026\u0026 rm -rf /tmp/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "${FTP_PORT}",
            "${SHARE_PORT}",
            "5432",
            "56000-56009",
            "8005",
            "8100",
            "8443"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/"
        },
        {
          "CmdLine": [
            "render",
            "/alfresco-community/tomcat/bin/setenv.sh",
            "--",
            "/init"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "/bin/bash",
            "/alfresco_scripts/alfresco_run.sh"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}