{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "youske/alpine-conda:latest",
      "SourceCode": "FROM youske/alpine-conda:latest",
      "Platform": "",
      "From": {
        "Image": "youske/alpine-conda:latest"
      },
      "Commands": [
        {
          "Maintainer": "youske miyakoshi \u003cyouske@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "LANG",
              "Value": "C.UTF-8"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "APPENGINE_VERSION",
              "Value": "1.9.38"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SDK",
              "Value": "https://storage.googleapis.com/appengine-sdks/featured/google_appengine_${APPENGINE_VERSION}.zip"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PACKAGE",
              "Value": "\"unzip git tmux \""
            },
            {
              "Key": "BUILD_PACKAGE",
              "Value": "\"build-base python-dev musl-dev linux-headers\""
            },
            {
              "Key": "LANGUAGE",
              "Value": "${LANG}"
            },
            {
              "Key": "TARGETDIR",
              "Value": "/google_appengine"
            },
            {
              "Key": "WORK",
              "Value": "/home/admin/projects"
            },
            {
              "Key": "PORT",
              "Value": "8080-8089"
            },
            {
              "Key": "PATH",
              "Value": "${TARGETDIR}:${PATH}"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk update --no-cache \u0026\u0026 apk upgrade --no-cache \u0026\u0026     apk add ${PACKAGE} --no-cache \u0026\u0026 apk add ${BUILD_PACKAGE} --no-cache -t buildpack"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget -q ${SDK} -O /tmp/gae.zip  \u0026\u0026  unzip -q /tmp/gae.zip -d /tmp/gtemp \u0026\u0026 mv /tmp/gtemp/google_appengine /google_appengine"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "requirements.txt",
            "/tmp"
          ]
        },
        {
          "CmdLine": [
            "cd /tmp \u0026\u0026 pip install -U  -r requirements.txt"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apk del buildpack --no-cache \u0026\u0026 rm -rf /tmp/* /var/cache/apk/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "admin"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "notes/",
            "/home/admin/notes"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "${PORT}"
          ]
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "Name": "workdir",
          "Path": "${WORK}"
        },
        {
          "CmdLine": [
            "switch",
            "shell=su admin -c /bin/bash",
            "--",
            "switch",
            "root=/bin/bash",
            "--",
            "/init"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "su",
            "admin",
            "-c",
            "/bin/bash"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}