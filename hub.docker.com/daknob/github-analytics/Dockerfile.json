{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "alpine:latest",
      "SourceCode": "FROM alpine:latest",
      "Platform": "",
      "From": {
        "Image": "alpine:latest"
      },
      "Commands": [
        {
          "Maintainer": "Antonios A. Chariton \u003cdaknob@daknob.net\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apk add --update python py-pip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "pip install gunicorn"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir /gh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            ".",
            "/gh/."
          ]
        },
        {
          "Name": "workdir",
          "Path": "/gh"
        },
        {
          "CmdLine": [
            "pip install -r requirements.txt"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "80"
          ]
        },
        {
          "Env": [
            {
              "Key": "GH_USER",
              "Value": "devstaff-crete"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GH_REPO",
              "Value": "devstaff-heraklion"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GH_ISSUE_LABEL",
              "Value": "topics"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GH_ISSUE_STATE",
              "Value": "open"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GH_ISSUE_REACTION",
              "Value": "+1"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "gunicorn",
            "-w",
            "8",
            "-b",
            "0.0.0.0:80",
            "analytics:app",
            "gunicorn-scripts"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}