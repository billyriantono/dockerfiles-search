{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ysaotome/ubuntu:14.04",
      "SourceCode": "FROM ysaotome/ubuntu:14.04",
      "Platform": "",
      "From": {
        "Image": "ysaotome/ubuntu:14.04"
      },
      "Commands": [
        {
          "Maintainer": "Yuichi Saotome \u003cy@sotm.jp\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "GOLANG_VERSION",
              "Value": "1.4.2"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget -qO- http://golang.org/dl/go${GOLANG_VERSION}.linux-amd64.tar.gz | tar -C /usr/local -xzf -"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "/root"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GOPATH",
              "Value": "${HOME}/go"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "${PATH}:${GOPATH}/bin:/usr/local/go/bin"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "touch ${HOME}/.zshrc"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "${HOME}"
        },
        {
          "CmdLine": [
            "go get github.com/revel/revel"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "go get github.com/revel/cmd/revel"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "9000"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "${GOPATH}/src/dev"
          ]
        },
        {
          "Name": "workdir",
          "Path": "${GOPATH}/src/dev"
        }
      ]
    }
  ]
}