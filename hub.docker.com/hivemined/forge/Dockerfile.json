{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "scratch",
      "SourceCode": "FROM scratch",
      "Platform": "",
      "From": {
        "Scratch": true
      },
      "Commands": [
        {
          "Maintainer": "\"Ryan - faceless.saint@gmail.com\"",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "MINECRAFT_VERSION",
              "Value": "1.7.10"
            },
            {
              "Key": "FORGE_VERSION",
              "Value": "10.13.4.1448"
            }
          ],
          "Name": "env"
        },
        {
          "Labels": [
            {
              "Key": "hivemined.comb",
              "Value": "hivemined.minecraft.version=${MINECRAFT_VERSION} hivemined.forge.version=${FORGE_VERSION}"
            }
          ],
          "Name": "label"
        },
        {
          "Expression": "LABEL hivemined.comb hivemined.minecraft.version=${MINECRAFT_VERSION} hivemined.forge.version=${FORGE_VERSION}",
          "Name": "onbuild"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "src/",
            "/mnt/minecraft/"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/mnt/minecraft"
          ]
        }
      ]
    }
  ]
}