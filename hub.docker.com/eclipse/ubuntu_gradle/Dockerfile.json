{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "eclipse/ubuntu_jdk8",
      "SourceCode": "FROM eclipse/ubuntu_jdk8",
      "Platform": "",
      "From": {
        "Image": "eclipse/ubuntu_jdk8"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "GRADLE_VERSION",
              "Value": "2.3"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GRADLE_HOME",
              "Value": "/home/user/gradle-$GRADLE_VERSION"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$GRADLE_HOME/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget -P /home/user/ --quiet https://services.gradle.org/distributions/gradle-$GRADLE_VERSION-bin.zip \u0026\u0026     cd /home/user/ \u0026\u0026 unzip gradle-$GRADLE_VERSION-bin.zip \u0026\u0026 rm gradle-$GRADLE_VERSION-bin.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "9000"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/projects"
        }
      ]
    }
  ]
}