      "SourceCode": "FROM uweloydl/robot-base:latest",
      "Platform": "",
      "From": {
        "Image": "uweloydl/robot-base:latest"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "DEBIAN_FRONTEND",
              "Value": "noninteractive"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DEBCONF_NONINTERACTIVE_SEEN",
              "Value": "true"
