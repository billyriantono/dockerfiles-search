          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "TZ",
              "Value": "\"Europe/Berlin\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo \"${TZ}\" \u003e /etc/timezone   \u0026\u0026 dpkg-reconfigure --frontend noninteractive tzdata"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update -qqy   \u0026\u0026 apt-get -qqy install     xvfb   \u0026\u0026 rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "entry.sh",
            "/opt/bin/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "functions.sh",
            "/opt/bin/"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /opt/bin/entry.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "SCREEN_WIDTH",
              "Value": "1360"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SCREEN_HEIGHT",
              "Value": "1020"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SCREEN_DEPTH",
