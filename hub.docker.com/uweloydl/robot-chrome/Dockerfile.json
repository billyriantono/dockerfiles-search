          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "chrome_launcher.sh",
            "/opt/google/chrome/google-chrome"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /opt/google/chrome/google-chrome"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "seluser"
        },
        {
          "Env": [
