{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "densuke/trusty-jp:latest",
      "SourceCode": "FROM densuke/trusty-jp:latest",
      "Platform": "",
      "From": {
        "Image": "densuke/trusty-jp:latest"
      },
      "Commands": [
        {
          "Maintainer": "densuke",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y openjdk-7-jre-headless git wget"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir /var/jenkins_home"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "Name": "workdir",
          "Path": "/"
        },
        {
          "Env": [
            {
              "Key": "JENKINS_HOME",
              "Value": "/var/jenkins_home"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/usr/local/bin:/usr/bin:/bin"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget -nv -O /usr/local/bin/jenkins.war http://mirrors.jenkins-ci.org/war/latest/jenkins.war"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "8080"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "50000"
          ]
        },
        {
          "CmdLine": [
            "/usr/bin/java",
            "-jar",
            "/usr/local/bin/jenkins.war"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}