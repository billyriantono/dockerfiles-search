{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "callforamerica/debian",
      "SourceCode": "FROM callforamerica/debian",
      "Platform": "",
      "From": {
        "Image": "callforamerica/debian"
      },
      "Commands": [
        {
          "Maintainer": "Joe Black \u003cjoeblack949@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Key": "KAMAILIO_VERSION",
          "Name": "arg",
          "Value": null
        },
        {
          "Key": "KAMAILIO_INSTALL_MODS",
          "Name": "arg",
          "Value": null
        },
        {
          "Key": "KAZOO_CONFIGS_BRANCH",
          "Name": "arg",
          "Value": null
        },
        {
          "Env": [
            {
              "Key": "KAMAILIO_VERSION",
              "Value": "${KAMAILIO_VERSION:-4.4.5}"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KAMAILIO_INSTALL_MODS",
              "Value": "${KAMAILIO_INSTALL_MODS:-extra,kazoo,outbound,presence,tls,websocket}"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KAZOO_CONFIGS_BRANCH",
              "Value": "${KAZOO_CONFIGS_BRANCH:-4.0}"
            }
          ],
          "Name": "env"
        },
        {
          "Labels": [
            {
              "Key": "app.kamailio.version",
              "Value": "$KAMAILIO_VERSION"
            }
          ],
          "Name": "label"
        },
        {
          "Labels": [
            {
              "Key": "app.kamailio.mods",
              "Value": "$KAMAILIO_INSTALL_MODS"
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "APP",
              "Value": "kamailio"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "USER",
              "Value": "$APP"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "/opt/$APP"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "build.sh",
            "/tmp/"
          ]
        },
        {
          "CmdLine": [
            "/tmp/build.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "entrypoint",
            "/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "build/kamailio.limits.conf",
            "/etc/security/limits.d/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "build/sync-freeswitch",
            "/usr/local/bin/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "build/50-kamailio-functions.sh",
            "/etc/profile.d/"
          ]
        },
        {
          "Env": [
            {
              "Key": "KAMAILIO_LOG_LEVEL",
              "Value": "info"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KAMAILIO_ENABLE_ROLES",
              "Value": "websockets,message"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "expose",
          "Ports": [
            "5060",
            "5060/udp",
            "5061"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "5064",
            "5064/udp",
            "5065",
            "5065/udp"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "7000",
            "7000/udp",
            "7001"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/volumes/kamailio/dbtext",
            "/volumes/kamailio/tls"
          ]
        },
        {
          "Name": "workdir",
          "Path": "$HOME"
        },
        {
          "Name": "shell",
          "Shell": [
            "/bin/bash"
          ]
        },
        {
          "Health": {
            "Interval": 15000000000,
            "Test": [
              "CMD-SHELL",
              "kamctl monitor 1 | grep -q ^Up || exit 1"
            ],
            "Timeout": 5000000000
          },
          "Name": "healthcheck"
        },
        {
          "CmdLine": [
            "/dumb-init",
            "--"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "/entrypoint"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}