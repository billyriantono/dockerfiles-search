{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "pkannan/scala:latest",
      "SourceCode": "FROM pkannan/scala:latest",
      "Platform": "",
      "From": {
        "Image": "pkannan/scala:latest"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "KAFKAMANAGERVERSION",
              "Value": "1.3.3.15"
            },
            {
              "Key": "USER",
              "Value": "kafka-manager"
            },
            {
              "Key": "USER_ID",
              "Value": "1000"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget -q -O - \"https://github.com/yahoo/kafka-manager/archive/$KAFKAMANAGERVERSION.tar.gz\" | tar -zxf - -C /tmp \t\u0026\u0026 cd \"/tmp/kafka-manager-$KAFKAMANAGERVERSION\"     \t\u0026\u0026 sbt clean dist \t\u0026\u0026 cd \"/tmp/kafka-manager-$KAFKAMANAGERVERSION/target/universal\"     \t\u0026\u0026 jar xf \"kafka-manager-$KAFKAMANAGERVERSION.zip\"     \t\u0026\u0026 mv \"kafka-manager-$KAFKAMANAGERVERSION\" /opt/kafka-manager     \t\u0026\u0026 groupadd -r $USER --gid=$USER_ID \u0026\u0026 useradd -r -g $USER --uid=$USER_ID $USER     \t\u0026\u0026 cd /opt/kafka-manager     \t\u0026\u0026 chown -R $USER:$USER  /opt/kafka-manager     \t\u0026\u0026 chmod +x /opt/kafka-manager/bin/kafka-manager     \t\u0026\u0026 rm -rf \"/tmp/kafka-manager-$KAFKAMANAGERVERSION\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "${USER}"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "\"/opt/kafka-manager/bin:${PATH}\""
            }
          ],
          "Name": "env"
        },
        {
          "Name": "expose",
          "Ports": [
            "9000"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/opt/kafka-manager"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/opt/kafka-manager/logs"
          ]
        },
        {
          "CmdLine": [
            "bin/kafka-manager",
            "-Dconfig.file=conf/application.conf",
            "-Dapplication.home=/opt/kafka-manager"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}