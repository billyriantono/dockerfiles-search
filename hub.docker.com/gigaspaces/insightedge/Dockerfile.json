{
  "MetaArgs": [
    {
      "Key": "JAVA_IMAGE",
      "DefaultValue": "openjdk",
      "ProvidedValue": null,
      "Value": "openjdk"
    },
    {
      "Key": "JAVA_TAG",
      "DefaultValue": "8-alpine",
      "ProvidedValue": null,
      "Value": "8-alpine"
    }
  ],
  "Stages": [
    {
      "Name": "",
      "BaseName": "${JAVA_IMAGE}:${JAVA_TAG}",
      "SourceCode": "FROM ${JAVA_IMAGE}:${JAVA_TAG}",
      "Platform": "",
      "From": {
        "Image": "${JAVA_IMAGE}:${JAVA_TAG}"
      },
      "Commands": [
        {
          "Key": "GS_PRODUCT",
          "Name": "arg",
          "Value": "insightedge"
        },
        {
          "Key": "GS_VERSION",
          "Name": "arg",
          "Value": "14.2.0"
        },
        {
          "Key": "GS_MILESTONE",
          "Name": "arg",
          "Value": "ga"
        },
        {
          "Key": "GS_BUILD",
          "Name": "arg",
          "Value": "20400"
        },
        {
          "Key": "GS_NAME",
          "Name": "arg",
          "Value": "gigaspaces-${GS_PRODUCT}-${GS_VERSION}-${GS_MILESTONE}-b${GS_BUILD}"
        },
        {
          "Key": "GS_URL",
          "Name": "arg",
          "Value": "https://gigaspaces-releases-eu.s3.amazonaws.com/${GS_PRODUCT}/${GS_VERSION}/${GS_NAME}.zip"
        },
        {
          "Env": [
            {
              "Key": "GS_HOME",
              "Value": "/opt/gigaspaces"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "${GS_HOME}"
        },
        {
          "CmdLine": [
            "apk --no-cache add openssl wget unzip bash libc6-compat \u0026\u0026    wget --progress=bar:force -O /tmp/gigaspaces.zip ${GS_URL} \u0026\u0026    unzip -q /tmp/gigaspaces.zip -d /tmp \u0026\u0026    mkdir -p ${GS_HOME} \u0026\u0026    mv /tmp/${GS_NAME}/* ${GS_HOME}/ \u0026\u0026    rm -f /tmp/gigaspaces.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "entrypoint.sh",
            "/opt/entrypoint.sh"
          ]
        },
        {
          "CmdLine": [
            "apk add --no-cache nss"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "XAP_NIC_ADDRESS",
              "Value": "\"#eth0:ip#\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "XAP_LOOKUP_PORT",
              "Value": "4174"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "XAP_LRMI_PORT",
              "Value": "8200-8300"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "XAP_WEBSTER_HTTP_PORT",
              "Value": "8199"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "XAP_RMI_REGISTRY_PORT",
              "Value": "10098"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "XAP_REST_PORT",
              "Value": "8089"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SPARK_MASTER_PORT",
              "Value": "7077"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SPARK_MASTER_WEBUI_PORT",
              "Value": "8080"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SPARK_MASTER_REST_PORT",
              "Value": "6066"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ZEPPELIN_PORT",
              "Value": "9090"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "expose",
          "Ports": [
            "${SPARK_MASTER_PORT}",
            "${SPARK_MASTER_REST_PORT}",
            "${SPARK_MASTER_REST_PORT}",
            "${SPARK_MASTER_WEBUI_PORT}",
            "${SPARK_MASTER_WEBUI_PORT}",
            "${XAP_LOOKUP_PORT}",
            "${XAP_LRMI_PORT}",
            "${XAP_REST_PORT}",
            "${XAP_RMI_REGISTRY_PORT}",
            "${XAP_WEBSTER_HTTP_PORT}",
            "${ZEPPELIN_PORT}",
            "${ZEPPELIN_PORT}"
          ]
        },
        {
          "Env": [
            {
              "Key": "EXT_JAVA_OPTIONS",
              "Value": "\"-Dcom.gs.multicast.discoveryPort=${XAP_LOOKUP_PORT} -Dcom.gs.transport_protocol.lrmi.bind-port=${XAP_LRMI_PORT} -Dcom.gigaspaces.start.httpPort=${XAP_WEBSTER_HTTP_PORT} -Dcom.gigaspaces.start.rmi.registryPort=${XAP_RMI_REGISTRY_PORT}  -Dzeppelin.server.port=${ZEPPELIN_PORT}\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "/opt/entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "demo"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}