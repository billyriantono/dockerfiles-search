{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:xenial",
      "SourceCode": "FROM ubuntu:xenial",
      "Platform": "",
      "From": {
        "Image": "ubuntu:xenial"
      },
      "Commands": [
        {
          "Labels": [
            {
              "Key": "ravendb-version",
              "Value": "\"40016-beta\""
            }
          ],
          "Name": "label"
        },
        {
          "Labels": [
            {
              "Key": "operating-system",
              "Value": "\"ubuntu\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "RAVEN_Port",
              "Value": "8080"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "RAVEN_Url",
              "Value": "\"http://0.0.0.0:8080\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "RAVEN_Hostname",
              "Value": "localhost"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "RAVEN_TCP_Port",
              "Value": "38888"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "RAVEN_TCP_Url",
              "Value": "\"tcp://0.0.0.0:38888\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "RAVEN_DataDir",
              "Value": "/var/lib/ravendb/"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "RAVEN_AllowAnonymous",
              "Value": "true"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "RAVEN_LogsMode",
              "Value": "Information"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update     \u0026\u0026 apt-get install -y --no-install-recommends     \u0026\u0026 apt-get install bzip2 libunwind8 libicu55 libcurl3 ca-certificates -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "${RAVEN_Port}",
            "${RAVEN_TCP_Port}"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "${RAVEN_DataDir}"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "http://hibernatingrhinos.com/downloads/RavenDB%20for%20Ubuntu%2016.04%20x64/40016-Beta",
            "/home/Raven.tar.bz2"
          ]
        },
        {
          "CmdLine": [
            "cd /home     \u0026\u0026 mkdir Raven     \u0026\u0026 tar xf Raven.tar.bz2 -C Raven     \u0026\u0026 rm /home/Raven.tar.bz2     \u0026\u0026 apt-get remove bzip2 -y     \u0026\u0026 apt-get autoremove -y     \u0026\u0026 rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/home/Raven/Server"
        },
        {
          "CmdLine": [
            "./Raven.Server         /Raven/ServerUrl=${RAVEN_Url}         /Raven/ServerUrl/Tcp=${RAVEN_TCP_Url}         /Raven/HostName=${RAVEN_Hostname}         /Raven/AllowAnonymousUserToAccessTheServer=${RAVEN_AllowAnonymous}         /Raven/DataDir=${RAVEN_DataDir} \t\t/Raven/Logs.Mode=${RAVEN_LogsMode}         --daemon         --print-id"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}