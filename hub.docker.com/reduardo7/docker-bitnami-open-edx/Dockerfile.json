          ]
        },
        {
          "CmdLine": [
            "chmod a+x bitnami-edx-linux-x64-installer.run"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"${EDX_SETUP_DEMO_COURSES}\\ny\\n${EDX_SETUP_INSTALL_FOLDER}\\n${EDX_SETUP_REAL_NAME}\\n${EDX_SETUP_EMAIL}\\n${EDX_SETUP_USER}\\n${EDX_SETUP_PASSWORD}\\n${EDX_SETUP_PASSWORD}\\n${EDX_SETUP_EMAIL_SUPPORT}\\n${EDX_SETUP_HOSTNAME}\\ny\\n\" | ./bitnami-edx-linux-x64-installer.run"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get autoremove -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get clean \u0026\u0026 rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp /setup-files"
          ],
          "Name": "run",
          "PrependShell": true
        },
