{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "frolvlad/alpine-glibc",
      "SourceCode": "FROM frolvlad/alpine-glibc",
      "Platform": "",
      "From": {
        "Image": "frolvlad/alpine-glibc"
      },
      "Commands": [
        {
          "Maintainer": "Jorn Wijnands \u003cjwijnands@ebay.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "HEARTBEAT_VERSION",
              "Value": "5.3.1"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk add --no-cache       ca-certificates       curl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -L -O https://artifacts.elastic.co/downloads/beats/heartbeat/heartbeat-${HEARTBEAT_VERSION}-linux-x86_64.tar.gz \u0026\u0026     tar -xvvf heartbeat-${HEARTBEAT_VERSION}-linux-x86_64.tar.gz \u0026\u0026     mv heartbeat-${HEARTBEAT_VERSION}-linux-x86_64/ /heartbeat \u0026\u0026     mv /heartbeat/heartbeat.yml /heartbeat/heartbeat.example.yml \u0026\u0026     mv /heartbeat/heartbeat /bin/heartbeat \u0026\u0026     chmod +x /bin/heartbeat \u0026\u0026     mkdir -p /heartbeat/config /heartbeat/data \u0026\u0026     rm heartbeat-${HEARTBEAT_VERSION}-linux-x86_64.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/heartbeat"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "docker-entrypoint.sh",
            "/heartbeat/docker-entrypoint.sh"
          ]
        },
        {
          "CmdLine": [
            "/heartbeat/docker-entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": true
        }
      ]
    }
  ]
}