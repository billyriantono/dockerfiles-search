{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:7",
      "SourceCode": "FROM openjdk:7",
      "Platform": "",
      "From": {
        "Image": "openjdk:7"
      },
      "Commands": [
        {
          "Maintainer": "Sathwik B P",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "NODE_VERSION",
              "Value": "6.6.0"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "MAVEN_VERSION",
              "Value": "3.3.9"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PHANTOSJS_VERSION",
              "Value": "2.1.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "WORKSPACE",
              "Value": "/workspace"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/usr/local/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p /opt/node     \u0026\u0026 curl -SLO \"https://nodejs.org/dist/v$NODE_VERSION/node-v$NODE_VERSION-linux-x64.tar.xz\"     \u0026\u0026 tar -Jxf \"node-v$NODE_VERSION-linux-x64.tar.xz\" -C /opt/node --strip-components=1     \u0026\u0026 rm \"node-v$NODE_VERSION-linux-x64.tar.xz\"     \u0026\u0026 update-alternatives --install /usr/local/bin/node node /opt/node/bin/node 1     \u0026\u0026 update-alternatives --install /usr/local/bin/npm npm /opt/node/bin/npm 1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/opt/node/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "npm install -g minimatch@3.0.2   \u0026\u0026 npm install -g bower   \u0026\u0026 npm install -g gulp"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "update-alternatives --install /usr/local/bin/bower bower /opt/node/bin/bower 1     \u0026\u0026 update-alternatives --install /usr/local/bin/gulp gulp /opt/node/bin/gulp 1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -SLO https://bitbucket.org/ariya/phantomjs/downloads/phantomjs-$PHANTOSJS_VERSION-linux-x86_64.tar.bz2   \u0026\u0026 tar -jxf \"phantomjs-$PHANTOSJS_VERSION-linux-x86_64.tar.bz2\" -C /usr/local --strip-components=1   \u0026\u0026 rm \"phantomjs-$PHANTOSJS_VERSION-linux-x86_64.tar.bz2\"   \u0026\u0026 ln -s /usr/local/phantomjs /usr/bin/phatomjs"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /opt/maven /opt/maven/ref   \u0026\u0026 curl -SLO http://apache.osuosl.org/maven/maven-3/$MAVEN_VERSION/binaries/apache-maven-$MAVEN_VERSION-bin.tar.gz   \u0026\u0026 tar -xf \"apache-maven-$MAVEN_VERSION-bin.tar.gz\" -C /opt/maven --strip-components=1   \u0026\u0026 rm \"apache-maven-$MAVEN_VERSION-bin.tar.gz\"   \u0026\u0026 update-alternatives --install /usr/local/bin/mvn mvn /opt/maven/bin/mvn 1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/opt/maven/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "MAVEN_HOME",
              "Value": "/opt/maven"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "chmod 777 -R /tmp \u0026\u0026 chmod o+t -R /tmp"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/workspace"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/workspace"
        },
        {
          "CmdLine": [
            "gulp"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}