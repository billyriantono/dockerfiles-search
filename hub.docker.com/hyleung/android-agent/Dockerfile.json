{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "hyleung/jenkins-agent",
      "SourceCode": "FROM hyleung/jenkins-agent",
      "Platform": "",
      "From": {
        "Image": "hyleung/jenkins-agent"
      },
      "Commands": [
        {
          "Maintainer": "H.Y. Leung \u003chy.leung@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get install -y curl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir ~/temp"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl http://dl.google.com/android/android-sdk_r23.0.2-linux.tgz -o ~/temp/android-sdk.tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get -y install lib32z1 lib32ncurses5 lib32bz2-1.0 lib32stdc++6"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar zvxf ~/temp/android-sdk.tgz -C /usr/local"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chown -R root:root /usr/local/android-sdk-linux"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod g+w -R /usr/local/android-sdk-linux"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "ANDROID_HOME",
              "Value": "/usr/local/android-sdk-linux"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "${PATH}:${ANDROID_HOME}/tools:${ANDROID_HOME}/platform-tools"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo y | /usr/local/android-sdk-linux/tools/android update sdk --no-ui --filter platform-tools,build-tools"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | /usr/local/android-sdk-linux/tools/android update sdk --no-ui -a --filter build-tools-19.1.0"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | /usr/local/android-sdk-linux/tools/android update sdk --no-ui -a --filter build-tools-20.0.0"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | /usr/local/android-sdk-linux/tools/android update sdk --no-ui --filter android-20,android-19,android-18,android-17,android-16"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | /usr/local/android-sdk-linux/tools/android update sdk --no-ui --filter extra-android-support"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | /usr/local/android-sdk-linux/tools/android update sdk --no-ui --filter extra-android-m2repository"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | /usr/local/android-sdk-linux/tools/android update sdk --no-ui --filter extra-google-m2repository"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | /usr/local/android-sdk-linux/tools/android update sdk --no-ui --filter extra-google-google_play_services"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod -R 777  /tmp/"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}