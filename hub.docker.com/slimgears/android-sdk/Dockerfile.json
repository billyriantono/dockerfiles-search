{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8-jdk-slim",
      "SourceCode": "FROM openjdk:8-jdk-slim",
      "Platform": "",
      "From": {
        "Image": "openjdk:8-jdk-slim"
      },
      "Commands": [
        {
          "Labels": [
            {
              "Key": "maintainer",
              "Value": "\"Denis Itskovich \u003cdenis.itskovich@gmail.com\u003e\""
            }
          ],
          "Name": "label"
        },
        {
          "Name": "shell",
          "Shell": [
            "/bin/bash",
            "-c"
          ]
        },
        {
          "CmdLine": [
            "apt-get --quiet update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get --quiet -y install git wget tar unzip qemu-system-arm libglu1-mesa"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "ANDROID_HOME",
              "Value": "/android-sdk"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_COMPILE_SDK",
              "Value": "28"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_SDK_TOOLS",
              "Value": "4333796"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_BUILD_TOOLS",
              "Value": "28.0.3"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_VER",
              "Value": "25"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_TARGET_CPU",
              "Value": "armeabi-v7a"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$ANDROID_HOME/platform-tools/:$ANDROID_HOME/emulator:${PATH}"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p $ANDROID_HOME"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget --quiet --output-document=./android-sdk.zip https://dl.google.com/android/repository/sdk-tools-linux-$ANDROID_SDK_TOOLS.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget --quiet --output-document=android-wait-for-emulator https://raw.githubusercontent.com/travis-ci/travis-cookbooks/0f497eb71291b52a703143c5cd63a217c8766dc9/community-cookbooks/android-sdk/files/default/android-wait-for-emulator"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod +x android-wait-for-emulator"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "unzip -d $ANDROID_HOME android-sdk.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm android-sdk.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | $ANDROID_HOME/tools/bin/sdkmanager \"platforms;android-$ANDROID_COMPILE_SDK\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | $ANDROID_HOME/tools/bin/sdkmanager \"platform-tools\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | $ANDROID_HOME/tools/bin/sdkmanager \"emulator\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | $ANDROID_HOME/tools/bin/sdkmanager \"system-images;android-$ANDROID_VER;google_apis;$ANDROID_TARGET_CPU\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "set +o pipefail;yes | $ANDROID_HOME/tools/bin/sdkmanager --licenses;set -o pipefail"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo no | $ANDROID_HOME/tools/bin/avdmanager create avd --name emulator -k \"system-images;android-$ANDROID_VER;google_apis;$ANDROID_TARGET_CPU\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "5554/tcp",
            "5555/tcp"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "emulator.sh",
            "/"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /emulator.sh"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}