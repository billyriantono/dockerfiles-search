{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openresty/openresty:alpine",
      "SourceCode": "FROM openresty/openresty:alpine",
      "Platform": "",
      "From": {
        "Image": "openresty/openresty:alpine"
      },
      "Commands": [
        {
          "Maintainer": "Onni Hakala onni@keksi.io",
          "Name": "maintainer"
        },
        {
          "Key": "BUILD_DEPS",
          "Name": "arg",
          "Value": "\"gettext curl wget\""
        },
        {
          "Key": "RUNTIME_DEPS",
          "Name": "arg",
          "Value": "\"libintl openssl ca-certificates bash\""
        },
        {
          "Key": "DOCKER_GEN_VERSION",
          "Name": "arg",
          "Value": "0.7.3"
        },
        {
          "CmdLine": [
            "apk add --update $RUNTIME_DEPS     \u0026\u0026 apk add --virtual build_deps $BUILD_DEPS     \u0026\u0026 wget --quiet https://github.com/jwilder/docker-gen/releases/download/$DOCKER_GEN_VERSION/docker-gen-alpine-linux-amd64-$DOCKER_GEN_VERSION.tar.gz \t\u0026\u0026 tar -C /usr/local/bin -xvzf docker-gen-alpine-linux-amd64-$DOCKER_GEN_VERSION.tar.gz \t\u0026\u0026 rm /docker-gen-alpine-linux-amd64-$DOCKER_GEN_VERSION.tar.gz     \u0026\u0026 cp /usr/bin/envsubst /usr/local/bin/envsubst      \u0026\u0026 apk del build_deps"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -s /usr/local/openresty/nginx/conf /etc/nginx  \t\u0026\u0026 mkdir /etc/nginx/conf.d"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://github.com/jwilder/forego/releases/download/v0.16.1/forego",
            "/usr/local/bin/forego"
          ]
        },
        {
          "CmdLine": [
            "chmod u+x /usr/local/bin/forego"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            ".",
            "/app/"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/app/"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "nginx/",
            "/etc/nginx/"
          ]
        },
        {
          "Env": [
            {
              "Key": "DOCKER_HOST",
              "Value": "unix:///tmp/docker.sock"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "NGINX_PROXY_TIMEOUT",
              "Value": "600"
            },
            {
              "Key": "NGINX_MAX_UPLOAD",
              "Value": "2G"
            },
            {
              "Key": "HOSTMACHINE_IP",
              "Value": "172.17.42.1"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/etc/nginx/certs"
          ]
        },
        {
          "CmdLine": [
            "/app/docker-entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "forego",
            "start",
            "-r"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}