{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "crowdcode/vue-cli:latest",
      "SourceCode": "FROM crowdcode/vue-cli:latest",
      "Platform": "",
      "From": {
        "Image": "crowdcode/vue-cli:latest"
      },
      "Commands": [
        {
          "Labels": [
            {
              "Key": "maintainer",
              "Value": "\"development@crowdcode.io\""
            },
            {
              "Key": "description",
              "Value": "\"Simple vuepress docker container\""
            }
          ],
          "Name": "label"
        },
        {
          "Key": "VUEPRESS_VERSION",
          "Name": "arg",
          "Value": "0.14.8"
        },
        {
          "Env": [
            {
              "Key": "VUEPRESS_VERSION",
              "Value": "${VUEPRESS_VERSION}"
            }
          ],
          "Name": "env"
        },
        {
          "Key": "USER_ID",
          "Name": "arg",
          "Value": "1000"
        },
        {
          "Key": "USER_HOME_DIR",
          "Name": "arg",
          "Value": "\"/build\""
        },
        {
          "Key": "WORKSPACE_DIR",
          "Name": "arg",
          "Value": "\"/workspace\""
        },
        {
          "Env": [
            {
              "Key": "NPM_CONFIG_LOGLEVEL",
              "Value": "info"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "\"$USER_HOME_DIR\""
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "${USER_HOME_DIR}"
        },
        {
          "CmdLine": [
            "set -xe     \u0026\u0026 npm install -g  vuepress@${VUEPRESS_VERSION}     \u0026\u0026 npm cache clean --force"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "$WORKSPACE_DIR"
        },
        {
          "Name": "expose",
          "Ports": [
            "8080"
          ]
        },
        {
          "CmdLine": [
            "/usr/bin/dumb-init",
            "--"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "Name": "user",
          "User": "${USER_ID}"
        }
      ]
    }
  ]
}