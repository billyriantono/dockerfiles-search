{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "nginx:stable",
      "SourceCode": "FROM nginx:stable",
      "Platform": "",
      "From": {
        "Image": "nginx:stable"
      },
      "Commands": [
        {
          "Maintainer": "yumaoshu@gmail.com",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get update     \u0026\u0026 apt-get install --no-install-recommends -y curl apt-transport-https ca-certificates gnupg1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -sSL https://deb.nodesource.com/setup_6.x | bash -"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y nodejs --no-install-recommends"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install --no-install-recommends -y unzip sed"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GITLAB_CE_PAGES_HOME",
              "Value": "\"/home/pages\""
            },
            {
              "Key": "GITLAB_CE_PAGES_USER",
              "Value": "\"pages\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GITLAB_CE_PAGES_PUBLIC_DIR",
              "Value": "\"${GITLAB_CE_PAGES_HOME}/public\""
            },
            {
              "Key": "GITLAB_CE_PAGES_WEBHOOK_DIR",
              "Value": "\"${GITLAB_CE_PAGES_HOME}/webhook\""
            },
            {
              "Key": "GITLAB_CE_PAGES_CNAME_DIR",
              "Value": "\"${GITLAB_CE_PAGES_HOME}/cname\""
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "${GITLAB_CE_PAGES_WEBHOOK_DIR}"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "src/package.json",
            "${GITLAB_CE_PAGES_WEBHOOK_DIR}/"
          ]
        },
        {
          "CmdLine": [
            "npm install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "npm install -g nodemon"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "TINI_VERSION",
              "Value": "v0.10.0"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://github.com/krallin/tini/releases/download/${TINI_VERSION}/tini",
            "/tini"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /tini"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/tini",
            "--"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "entrypoint.sh",
            "/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/nginx.conf",
            "/etc/nginx/nginx.conf"
          ]
        },
        {
          "CmdLine": [
            "rm -f /etc/nginx/conf.d/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "src/",
            "${GITLAB_CE_PAGES_WEBHOOK_DIR}/"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "80/tcp"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "${GITLAB_CE_PAGES_PUBLIC_DIR}"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "${GITLAB_CE_PAGES_CNAME_DIR}"
          ]
        },
        {
          "CmdLine": [
            "/entrypoint.sh",
            "/usr/bin/npm",
            "start"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}