{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "wwek/openresty:alpine",
      "SourceCode": "FROM wwek/openresty:alpine",
      "Platform": "",
      "From": {
        "Image": "wwek/openresty:alpine"
      },
      "Commands": [
        {
          "Labels": [
            {
              "Key": "version",
              "Value": "\"1.0\""
            },
            {
              "Key": "description",
              "Value": "\"openresty alpine\""
            },
            {
              "Key": "maintainer",
              "Value": "\"wwek\u003clicoolgo@gmail.com\u003e\""
            }
          ],
          "Name": "label"
        },
        {
          "Key": "OPENRESTY_DIR",
          "Name": "arg",
          "Value": "/usr/local/openresty"
        },
        {
          "Key": "OPENRESTY_CONF_DIR",
          "Name": "arg",
          "Value": "${OPENRESTY_DIR}/nginx/conf"
        },
        {
          "CmdLine": [
            "adduser -D -H -u 1000 -s /bin/bash www-data     \u0026\u0026 mkdir -p ${OPENRESTY_CONF_DIR}/conf.d ${OPENRESTY_CONF_DIR}/vhost"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "nginx.conf",
            "${OPENRESTY_CONF_DIR}"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "sites/",
            "${OPENRESTY_CONF_DIR}/sites-available"
          ]
        },
        {
          "Key": "PHP_UPSTREAM_CONTAINER",
          "Name": "arg",
          "Value": "php-fpm"
        },
        {
          "Key": "PHP_UPSTREAM_PORT",
          "Name": "arg",
          "Value": "9000"
        },
        {
          "CmdLine": [
            "echo \"upstream php-upstream { server ${PHP_UPSTREAM_CONTAINER}:${PHP_UPSTREAM_PORT}; }\" \u003e ${OPENRESTY_CONF_DIR}/conf.d/upstream.conf"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "443",
            "80"
          ]
        },
        {
          "CmdLine": [
            "/usr/local/openresty/bin/openresty"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
