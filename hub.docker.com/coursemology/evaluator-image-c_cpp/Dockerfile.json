{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "coursemology/evaluator-image-base:deb8",
      "SourceCode": "FROM coursemology/evaluator-image-base:deb8",
      "Platform": "",
      "From": {
        "Image": "coursemology/evaluator-image-base:deb8"
      },
      "Commands": [
        {
          "Maintainer": "Coursemology \u003ccoursemology@googlegroups.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "/home/coursemology"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y --force-yes \tbuild-essential git   \u0026\u0026 git clone https://github.com/google/googletest.git $HOME/googletest   \u0026\u0026 apt-get remove -y git   \u0026\u0026 apt-get autoremove -y   \u0026\u0026 apt-get install -y wget   \u0026\u0026 cd $HOME/   \u0026\u0026 mkdir boost   \u0026\u0026 cd boost   \u0026\u0026 wget https://sourceforge.net/projects/boost/files/boost/1.64.0/boost_1_64_0.tar.gz/download   \u0026\u0026 tar xvfz download   \u0026\u0026 rm download   \u0026\u0026 cd boost_1_64_0/   \u0026\u0026 ./bootstrap.sh   \u0026\u0026 ./b2 stage threading=multi link=static   \u0026\u0026 ./b2 install threading=multi link=static   \u0026\u0026 cd ../../   \u0026\u0026 rm -Rf boost/   \u0026\u0026 apt-get remove -y wget   \u0026\u0026 rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "BOOST_DIR",
              "Value": "/usr/local/lib"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GTEST_DIR",
              "Value": "$HOME/googletest/googletest"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "cd $GTEST_DIR/   \u0026\u0026 g++ -isystem ${GTEST_DIR}/include -I${GTEST_DIR}      -pthread -c ${GTEST_DIR}/src/gtest-all.cc      -std=c++11   \u0026\u0026 ar -rv libgtest.a gtest-all.o"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GTEST_OUTPUT",
              "Value": "xml:./report.xml"
            }
          ],
          "Name": "env"
        }
      ]
    }
  ]
}