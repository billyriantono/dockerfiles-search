{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "dankempster/php:fpm",
      "SourceCode": "FROM dankempster/php:fpm",
      "Platform": "",
      "From": {
        "Image": "dankempster/php:fpm"
      },
      "Commands": [
        {
          "Maintainer": "Dan Kempster \u003cdev@dankempster.co.uk\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "mkdir -p /symfony/cache;     mkdir -p /symfony/vendor;         chown -R www-data:www-data /symfony;     chmod -R 2775 /symfony;"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "DOCKER",
              "Value": "1"
            },
            {
              "Key": "COMPOSER_VENDOR_DIR",
              "Value": "/symfony/vendor"
            },
            {
              "Key": "PATH",
              "Value": "/symfony/vendor/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/symfony/cache",
            "/symfony/vendor"
          ]
        }
      ]
    }
  ]
}