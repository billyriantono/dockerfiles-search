{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:8",
      "SourceCode": "FROM java:8",
      "Platform": "",
      "From": {
        "Image": "java:8"
      },
      "Commands": [
        {
          "Maintainer": "Jeffrey Bird \u003cjeff@jbird.org\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "LUCEE_VERSION",
              "Value": "4.5.1.000"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/app/lucee"
        },
        {
          "CmdLine": [
            "curl -Lo lucee.zip  https://bitbucket.org/lucee/lucee/downloads/lucee-$LUCEE_VERSION-express.zip \u0026\u0026     unzip lucee.zip \u0026\u0026 rm -f lucee.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "8009",
            "8888"
          ]
        },
        {
          "CmdLine": [
            "/app/lucee/bin/catalina.sh run"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}