{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:16.04",
      "SourceCode": "FROM ubuntu:16.04",
      "Platform": "",
      "From": {
        "Image": "ubuntu:16.04"
      },
      "Commands": [
        {
          "Maintainer": "Brandon Risberg \u003cbrandon.risberg@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "APTLIST",
              "Value": "\"git libav-tools libffi-dev libopus-dev libssl-dev python3.5-dev unzip ffmpeg wget ca-certificates build-essential\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PIPLIST",
              "Value": "\"git+https://github.com/Rapptz/discord.py@master#egg=discord.py[voice] imgurpython youtube_dl beautifulsoup4\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y --no-install-recommends $APTLIST \u0026\u0026 apt-get clean"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -s /usr/bin/python3.5 /usr/bin/python"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget --no-check-certificate -O /tmp/get-pip.py http://bootstrap.pypa.io/get-pip.py \u0026\u0026 python3.5 /tmp/get-pip.py"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "pip install --trusted-host pypi.python.org $PIPLIST"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            ".",
            "/app"
          ]
        },
        {
          "CmdLine": [
            "chmod 0744 /app/entrypoint.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mv /app/cogs /app/cogs_backup"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/app/cogs"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/app/data"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/app"
        },
        {
          "CmdLine": [
            "/app/entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}