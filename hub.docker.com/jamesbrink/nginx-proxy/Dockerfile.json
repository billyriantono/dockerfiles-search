{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "nginx:1.9.0",
      "SourceCode": "FROM nginx:1.9.0",
      "Platform": "",
      "From": {
        "Image": "nginx:1.9.0"
      },
      "Commands": [
        {
          "Maintainer": "Jason Wilder jwilder@litl.com",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get update  \u0026\u0026 apt-get install -y -q --no-install-recommends     ca-certificates     wget  \u0026\u0026 apt-get clean  \u0026\u0026 rm -r /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"daemon off;\" \u003e\u003e /etc/nginx/nginx.conf  \u0026\u0026 sed -i 's/^http {/\u0026\\n    server_names_hash_bucket_size 128;/g' /etc/nginx/nginx.conf"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget -P /usr/local/bin https://godist.herokuapp.com/projects/ddollar/forego/releases/current/linux-amd64/forego  \u0026\u0026 chmod u+x /usr/local/bin/forego"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "DOCKER_GEN_VERSION",
              "Value": "0.3.9"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget https://github.com/jwilder/docker-gen/releases/download/$DOCKER_GEN_VERSION/docker-gen-linux-amd64-$DOCKER_GEN_VERSION.tar.gz  \u0026\u0026 tar -C /usr/local/bin -xvzf docker-gen-linux-amd64-$DOCKER_GEN_VERSION.tar.gz  \u0026\u0026 rm /docker-gen-linux-amd64-$DOCKER_GEN_VERSION.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update   \u0026\u0026 apt-get install -y python python-daemon python-setproctitle    \u0026\u0026 wget http://nginx.org/packages/mainline/debian/pool/nginx/n/nginx-nr-agent/nginx-nr-agent_2.0.0-6_all.deb   \u0026\u0026 dpkg --install nginx-nr-agent_2.0.0-6_all.deb   \u0026\u0026 rm nginx-nr-agent_2.0.0-6_all.deb   \u0026\u0026 apt-get clean   \u0026\u0026 rm -r /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            ".",
            "/app/"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/app/"
        },
        {
          "Env": [
            {
              "Key": "DOCKER_HOST",
              "Value": "unix:///tmp/docker.sock"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/etc/nginx/certs"
          ]
        },
        {
          "CmdLine": [
            "forego",
            "start",
            "-r"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}