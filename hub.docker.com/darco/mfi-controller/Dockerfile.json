{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:7-jre",
      "SourceCode": "FROM java:7-jre",
      "Platform": "",
      "From": {
        "Image": "java:7-jre"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "MFI_VERSION",
              "Value": "2.1.11"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://www.ubnt.com/downloads/mfi/$MFI_VERSION/mFi.unix.zip",
            "/"
          ]
        },
        {
          "CmdLine": [
            "unzip mFi.unix.zip \u0026\u0026 rm mFi.unix.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /mFi/logs \u0026\u0026 ln -s /dev/stderr /mFi/logs/mongod.log \u0026\u0026 ln -s /dev/stderr /mFi/logs/server.log"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "DEBIAN_FRONTEND=noninteractive apt-key adv --keyserver keyserver.ubuntu.com --recv C0A52C50"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "DEBIAN_FRONTEND=noninteractive apt-get -y update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "DEBIAN_FRONTEND=noninteractive apt-get install -y -q --no-install-recommends libtcnative-1 mongodb-server unzip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "2323/tcp",
            "6080/tcp",
            "6443/tcp",
            "6843/tcp",
            "6880/tcp"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "10001/udp",
            "1900/udp",
            "3478/udp"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/mFi/data"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/mFi"
        },
        {
          "CmdLine": [
            "java",
            "-jar",
            "/mFi/lib/ace.jar",
            "start"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}