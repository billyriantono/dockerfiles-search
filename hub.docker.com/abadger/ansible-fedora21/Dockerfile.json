{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "fedora:21",
      "SourceCode": "FROM fedora:21",
      "Platform": "",
      "From": {
        "Image": "fedora:21"
      },
      "Commands": [
        {
          "Maintainer": "Toshio Kuratomi \u003ctkuratomi@ansible.com\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "yum -y update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "yum -y install PyYAML python-jinja2 python-httplib2 python-keyczar python-paramiko python-setuptools git python-pip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir /etc/ansible/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "touch /etc/ansible/hosts"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir /opt/ansible/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "git clone http://github.com/ansible/ansible.git /opt/ansible/ansible"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/opt/ansible/ansible"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/opt/ansible/ansible/bin:/bin:/usr/bin:/sbin:/usr/sbin"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PYTHONPATH",
              "Value": "/opt/ansible/ansible/lib"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANSIBLE_LIBRARY",
              "Value": "/opt/ansible/ansible/library"
            }
          ],
          "Name": "env"
        }
      ]
    }
  ]
}