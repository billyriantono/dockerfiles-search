{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "jupyter/scipy-notebook",
      "SourceCode": "FROM jupyter/scipy-notebook",
      "Platform": "",
      "From": {
        "Image": "jupyter/scipy-notebook"
      },
      "Commands": [
        {
          "Name": "user",
          "User": "$NB_USER"
        },
        {
          "CmdLine": [
            "conda install --quiet --yes anaconda-client anaconda mkl-rt boto3 pymongo biopython sqlalchemy rdflib psycopg2 nltk pytables lxml autopep8 yapf nodejs  pandas openjdk maven pytest mysql-connector-python elasticsearch ipyparallel pyld"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "conda install --quiet --yes --channel conda-forge widgetsnbextension jupyterlab ipywidgets beakerx"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "conda install --quiet --yes --channel https://conda.anaconda.org/openbabel openbabel"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "conda install --quiet --yes --channel https://conda.anaconda.org/bioconda rdflib-jsonld"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "conda update --all --quiet --yes"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "beakerx install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "TINI_VERSION",
              "Value": "v0.6.0"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://github.com/krallin/tini/releases/download/${TINI_VERSION}/tini",
            "/usr/bin/tini"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /usr/bin/tini"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ipcluster nbextension enable"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "$NB_USER"
        },
        {
          "CmdLine": [
            "pip install jupyter_contrib_nbextensions"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "jupyter contrib nbextension install --user"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "jupyter nbextensions_configurator enable --user"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bash -c \"jupyter-nbextension enable codefolding/main ; jupyter-nbextension enable code_prettify/code_pretiffy ; jupyter-nbextension enable execute_time/ExecuteTime ; jupyter-nbextension enable gist_it/main ; jupyter-nbextension enable snippets/main ; jupyter-nbextension enable snippets_menu/main ; jupyter-nbextension enable autopep8 ; jupyter-nbextension enable codefolding/edit ; jupyter-nbextension enable collapsible_headings/main ; jupyter-nbextension enable splitcell/splitcell ;  jupyter-nbextension enable toc2/main\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "jupyter labextension install @jupyter-widgets/jupyterlab-manager"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/usr/bin/tini",
            "--"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "bash -c \"jupyter lab --no-browser --port=8888\""
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
