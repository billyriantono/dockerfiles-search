{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "nginx:latest",
      "SourceCode": "FROM nginx:latest",
      "Platform": "",
      "From": {
        "Image": "nginx:latest"
      },
      "Commands": [
        {
          "Maintainer": "Keith Chambers",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "DEBIAN_FRONTEND=noninteractive     apt-get update -qq \u0026\u0026     apt-get -y install curl runit \u0026\u0026     rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "CT_URL",
              "Value": "https://github.com/hashicorp/consul-template/releases/download/v0.5.1/consul-template_0.5.1_linux_amd64.tar.gz"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl -L $CT_URL | tar -C /usr/local/bin --strip-components 1 -zxf -"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "nginx.service",
            "/etc/service/nginx/run"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "consul-template.service",
            "/etc/service/consul-template/run"
          ]
        },
        {
          "CmdLine": [
            "rm -v /etc/nginx/conf.d/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "nginx.conf",
            "/etc/consul-templates/nginx.conf"
          ]
        },
        {
          "CmdLine": [
            "/usr/bin/runsvdir",
            "/etc/service"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}