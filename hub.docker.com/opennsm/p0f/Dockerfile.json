{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "opennsm/debian",
      "SourceCode": "FROM       opennsm/debian",
      "Platform": "",
      "From": {
        "Image": "opennsm/debian"
      },
      "Commands": [
        {
          "Maintainer": "Syed Huq \u003cdeedarhuq@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Labels": [
            {
              "Key": "organization",
              "Value": "opennsm"
            }
          ],
          "Name": "label"
        },
        {
          "Labels": [
            {
              "Key": "program",
              "Value": "p0f"
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "VIRTUSER",
              "Value": "opennsm"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PROGRAM",
              "Value": "p0f"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "VERS",
              "Value": "3.06b"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y gcc libpcap0.8-dev"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "opennsm"
        },
        {
          "Name": "workdir",
          "Path": "/home/$VIRTUSER"
        },
        {
          "CmdLine": [
            "git clone https://github.com/p0f/p0f.git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/home/$VIRTUSER/$PROGRAM"
        },
        {
          "CmdLine": [
            "git checkout v$VERS"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "./build.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "CmdLine": [
            "install -m 755 -o root -g root ./$PROGRAM /usr/local/bin/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "install -m 644 -o root -g root ./$PROGRAM.fp /home/$VIRTUSER"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/home/$VIRTUSER"
        },
        {
          "Name": "user",
          "User": "opennsm"
        }
      ]
    }
  ]
}