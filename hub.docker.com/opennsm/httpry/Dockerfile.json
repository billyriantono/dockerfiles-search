{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "opennsm/debian",
      "SourceCode": "FROM       opennsm/debian",
      "Platform": "",
      "From": {
        "Image": "opennsm/debian"
      },
      "Commands": [
        {
          "Maintainer": "Syed Huq \u003cdeedarhuq@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Labels": [
            {
              "Key": "organization",
              "Value": "opennsm"
            }
          ],
          "Name": "label"
        },
        {
          "Labels": [
            {
              "Key": "program",
              "Value": "httpry"
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "VIRTUSER",
              "Value": "opennsm"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PROGRAM",
              "Value": "httpry"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "VERS",
              "Value": "0.0.6"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y make gcc libpcap0.8-dev"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /usr/man/man1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /usr/local/man/man1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "opennsm"
        },
        {
          "Name": "workdir",
          "Path": "/home/$VIRTUSER"
        },
        {
          "CmdLine": [
            "git clone https://github.com/jbittel/httpry"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/home/$VIRTUSER/httpry"
        },
        {
          "CmdLine": [
            "git checkout $PROGRAM-$VERS"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed -i 's/ -Werror//'  Makefile"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "make"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "Name": "workdir",
          "Path": "/home/$VIRTUSER/httpry"
        },
        {
          "CmdLine": [
            "make install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod u+s /usr/sbin/$PROGRAM"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/home/$VIRTUSER"
        },
        {
          "Name": "user",
          "User": "opennsm"
        }
      ]
    }
  ]
}