{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "debian:jessie",
      "SourceCode": "FROM debian:jessie",
      "Platform": "",
      "From": {
        "Image": "debian:jessie"
      },
      "Commands": [
        {
          "Maintainer": "Jacek Kowalski \u003cjkowalsk@student.agh.edu.pl\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "MAVEN_INSTALL_VERSION",
              "Value": "3.3.9"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GRADLE_INSTALL_VERSION",
              "Value": "2.9"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo deb http://httpredir.debian.org/debian jessie-backports main \u003e\u003e /etc/apt/sources.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get -y update \t\u0026\u0026 apt-get -y install cvs subversion git mercurial openjdk-7-jdk openjdk-8-jdk ant unzip wget xvfb \t\u0026\u0026 apt-get -y clean"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd /tmp \t\u0026\u0026 wget ftp://mirror.reverse.net/pub/apache/maven/maven-3/${MAVEN_INSTALL_VERSION}/binaries/apache-maven-${MAVEN_INSTALL_VERSION}-bin.tar.gz \t\u0026\u0026 tar xf apache-maven-${MAVEN_INSTALL_VERSION}-bin.tar.gz -C /opt \t\u0026\u0026 rm -f apache-maven-${MAVEN_INSTALL_VERSION}-bin.tar.gz \t\u0026\u0026 ln -s /opt/apache-maven-${MAVEN_INSTALL_VERSION} /opt/apache-maven"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd /tmp \t\u0026\u0026 wget \"https://services.gradle.org/distributions/gradle-${GRADLE_INSTALL_VERSION}-bin.zip\" \t\u0026\u0026 unzip gradle-${GRADLE_INSTALL_VERSION}-bin.zip -d /opt \t\u0026\u0026 rm gradle-${GRADLE_INSTALL_VERSION}-bin.zip \t\u0026\u0026 ln -s /opt/gradle-${GRADLE_INSTALL_VERSION} /opt/gradle"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get -y install npm"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get -y install chromedriver"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -s /usr/lib/jvm/java-1.7.0-openjdk-amd64 /usr/lib/jvm/java-1.7.0-openjdk \t\u0026\u0026 ln -s /usr/lib/jvm/java-1.8.0-openjdk-amd64 /usr/lib/jvm/java-1.8.0-openjdk"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "groupadd -r -g 900 bamboo-agent \t\u0026\u0026 useradd -r -m -u 900 -g 900 bamboo-agent"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "bamboo-agent.sh",
            "/"
          ]
        },
        {
          "Name": "user",
          "User": "bamboo-agent"
        },
        {
          "CmdLine": [
            "/bamboo-agent.sh"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}