{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8-alpine",
      "SourceCode": "FROM openjdk:8-alpine",
      "Platform": "",
      "From": {
        "Image": "openjdk:8-alpine"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "PLAY_VERSION",
              "Value": "\"1.4.6\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PLAY_HOME",
              "Value": "/opt/play/play-${PLAY_VERSION}"
            }
          ],
          "Name": "env"
        },
        {
          "Labels": [
            {
              "Key": "maintainer.name",
              "Value": "\"Anthony PERIQUET\""
            }
          ],
          "Name": "label"
        },
        {
          "Labels": [
            {
              "Key": "maintainer.email",
              "Value": "\"anthony@periquet.net\""
            }
          ],
          "Name": "label"
        },
        {
          "Labels": [
            {
              "Key": "version",
              "Value": "${PLAY_VERSION}"
            }
          ],
          "Name": "label"
        },
        {
          "Labels": [
            {
              "Key": "description",
              "Value": "\"Play Framework v${PLAY_VERSION}\""
            }
          ],
          "Name": "label"
        },
        {
          "CmdLine": [
            "apk add --no-cache --quiet curl python \u0026\u0026 \tmkdir -p /opt/play \u0026\u0026 cd /opt/play \u0026\u0026 \tcurl --fail --silent --show-error --location --remote-name https://downloads.typesafe.com/play/${PLAY_VERSION}/play-${PLAY_VERSION}.zip \u0026\u0026 \tunzip play-${PLAY_VERSION}.zip \u0026\u0026 rm play-${PLAY_VERSION}.zip \u0026\u0026 \tchmod +x /opt/play/play-${PLAY_VERSION}/play"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /app"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/app"
        },
        {
          "Name": "expose",
          "Ports": [
            "9000"
          ]
        },
        {
          "Health": {
            "Interval": 30000000000,
            "Test": [
              "CMD-SHELL",
              "curl --fail http://localhost:9000/ || exit 1"
            ],
            "Timeout": 3000000000
          },
          "Name": "healthcheck"
        },
        {
          "CmdLine": [
            "/opt/play/play-1.4.6/play"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "run",
            "/app",
            "--deps",
            "--http.port=9000"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}