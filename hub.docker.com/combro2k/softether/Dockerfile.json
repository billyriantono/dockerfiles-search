{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "combro2k/debian-debootstrap:8",
      "SourceCode": "FROM combro2k/debian-debootstrap:8",
      "Platform": "",
      "From": {
        "Image": "combro2k/debian-debootstrap:8"
      },
      "Commands": [
        {
          "Maintainer": "Martijn van Maurik \u003cdocker@vmaurik.nl\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "VERSION",
              "Value": "v4.21-9613-beta-2016.04.24"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026     apt-get -y -q install gcc make curl dnsmasq supervisor \u0026\u0026     curl -L http://www.softether-download.com/files/softether/${VERSION}-tree/Linux/SoftEther_VPN_Server/64bit_-_Intel_x64_or_AMD64/softether-vpnserver-${VERSION}-linux-x64-64bit.tar.gz |     tar -xzv -C /opt"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/opt/vpnserver"
        },
        {
          "CmdLine": [
            "make i_read_and_agree_the_license_agreement \u0026\u0026     apt-get purge -y -q --auto-remove gcc make \u0026\u0026     apt-get clean \u0026\u0026     rm -fr /var/lib/apt"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "resources/bin/",
            "/usr/local/bin/"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "resources/etc/",
            "/etc/"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /usr/local/bin/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/opt/vpnserver/vpnserver_config"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "1194/tcp",
            "1194/udp",
            "443/tcp",
            "5555/tcp",
            "992/tcp"
          ]
        },
        {
          "CmdLine": [
            "/usr/local/bin/run"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}