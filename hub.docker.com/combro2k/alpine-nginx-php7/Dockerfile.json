{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "combro2k/alpine-base:latest",
      "SourceCode": "FROM combro2k/alpine-base:latest",
      "Platform": "",
      "From": {
        "Image": "combro2k/alpine-base:latest"
      },
      "Commands": [
        {
          "Expression": "ARG BUILD_DATE=now",
          "Name": "onbuild"
        },
        {
          "Expression": "ARG VCS_REF=master",
          "Name": "onbuild"
        },
        {
          "Expression": "ARG VCS_URL=https://github.com/combro2k/dockerfiles/tree/master/alpine-nginx-php7",
          "Name": "onbuild"
        },
        {
          "Expression": "ARG DOCKERFILE=alpine-nginx-php7/Dockerfile",
          "Name": "onbuild"
        },
        {
          "Labels": [
            {
              "Key": "org.label-schema.build-date",
              "Value": "$BUILD_DATE"
            },
            {
              "Key": "org.label-schema.vcs-ref",
              "Value": "$VCS_REF"
            },
            {
              "Key": "org.label-schema.vcs-url",
              "Value": "$VCS_URL"
            },
            {
              "Key": "org.label-scheme.dockerfile",
              "Value": "$DOCKERFILE"
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "WWW_UID",
              "Value": "82"
            },
            {
              "Key": "WWW_GID",
              "Value": "82"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "set -xe \u0026\u0026 apk add --quiet --no-cache --virtual=.run-deps nginx git ca-certificates curl tar php7 php7-fpm php7-curl php7-dom php7-gd php7-iconv php7-xml   php7-json php7-mcrypt php7-pcntl php7-pdo php7-pdo_pgsql php7-pgsql php7-posix php7-mysqli php7-session php7-ctype \u0026\u0026 adduser -u 82 -D -S -G www-data www-data \u0026\u0026   ln -sf /dev/stdout /var/log/nginx/access.log \u0026\u0026 ln -sf /dev/stderr /var/log/nginx/error.log \u0026\u0026 \tapk del --quiet --no-cache --purge \u0026\u0026 rm -rf /var/cache/apk/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "resources/",
            "/"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "80/tcp"
          ]
        },
        {
          "CmdLine": [
            "set -xe \u0026\u0026 chmod +x /usr/bin/composer"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}