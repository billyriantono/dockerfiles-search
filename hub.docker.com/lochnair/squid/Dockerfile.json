{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "alpine:3.4",
      "SourceCode": "FROM alpine:3.4",
      "Platform": "",
      "From": {
        "Image": "alpine:3.4"
      },
      "Commands": [
        {
          "Maintainer": "Lochnair \u003clochnair@care.army\u003e",
          "Name": "maintainer"
        },
        {
          "Labels": [
            {
              "Key": "Description",
              "Value": "\"Docker image for Squid based on the offical Alpine Linux image (3.4)\""
            }
          ],
          "Name": "label"
        },
        {
          "CmdLine": [
            "apk add     --no-cache     --update     squid"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed -i 's|#cache_dir ufs /var/cache/squid 100 16 256|cache_dir aufs /var/cache/squid 100 16 256|' /etc/squid/squid.conf"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed -i 's|#http_access deny to_localhost|http_access deny to_localhost|' /etc/squid/squid.conf"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "3128"
          ]
        },
        {
          "CmdLine": [
            "/usr/sbin/squid -Nz -f /etc/squid/squid.conf"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/etc/squid"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/var/cache/squid"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/var/log/squid"
          ]
        },
        {
          "CmdLine": [
            "/usr/sbin/squid",
            "-N",
            "-Y",
            "-C",
            "-d",
            "1",
            "-f",
            "/etc/squid/squid.conf"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}