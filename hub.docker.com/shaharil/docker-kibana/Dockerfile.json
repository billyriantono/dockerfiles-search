{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "kibana:5.1",
      "SourceCode": "FROM kibana:5.1",
      "Platform": "",
      "From": {
        "Image": "kibana:5.1"
      },
      "Commands": [
        {
          "Maintainer": "Guillaume Simonneau \u003csimonneaug@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Labels": [
            {
              "Key": "Description",
              "Value": "\"kibana\""
            }
          ],
          "Name": "label"
        },
        {
          "CmdLine": [
            "/usr/share/kibana/bin/kibana-plugin install https://github.com/floragunncom/search-guard-kibana-plugin/releases/download/v5.1.2-alpha/searchguard-kibana-alpha-5.1.2.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update -y \u0026\u0026  apt-get install curl supervisor python-pip nginx -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "pip install envtpl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "KOPF_VERSION",
              "Value": "2.1.3"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl -s -L \"https://github.com/lshahar/elasticsearch-kopf/archive/v${KOPF_VERSION}.tar.gz\" |     tar xz -C /tmp \u0026\u0026 mv \"/tmp/elasticsearch-kopf-${KOPF_VERSION}\" /kopf"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /.backup/kibana"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/kibana.yml",
            "/.backup/kibana/kibana.yml"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/supervisord.conf",
            "/etc/supervisord.conf"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/nginx.conf.tpl",
            "/etc/nginx/conf.d/nginx.conf.tpl"
          ]
        },
        {
          "CmdLine": [
            "rm -f /etc/nginx/sites-enabled/default"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -f /etc/kibana/kibana.yml"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "KIBANA_PWD",
              "Value": "\"changeme\""
            },
            {
              "Key": "ELASTICSEARCH_HOST",
              "Value": "\"elasticsearch\""
            },
            {
              "Key": "ELASTICSEARCH_PORT",
              "Value": "\"9200\""
            },
            {
              "Key": "ELASTICSEARCH_PROTOCOL",
              "Value": "\"http\""
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "./src/",
            "/run/"
          ]
        },
        {
          "CmdLine": [
            "chmod +x -R /run/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/etc/kibana"
          ]
        },
        {
          "CmdLine": [
            "chown 999:999 -R /usr/share/kibana"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "supervisord",
            "--nodaemon",
            "--configuration",
            "/etc/supervisord.conf"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}