  "Stages": [
    {
      "Name": "",
      "BaseName": "chai65/alpine-phalcon-nginx:lastest",
      "SourceCode": "FROM chai65/alpine-phalcon-nginx:lastest",
      "Platform": "",
      "From": {
        "Image": "chai65/alpine-phalcon-nginx:lastest"
      },
      "Commands": [
        {
          "Maintainer": "Lê Thành \u003cstash.leth@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "MONGODB_VERSION",
              "Value": "1.2.10"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "REDIS_VERSION",
              "Value": "3.1.3"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo \"http://dl-cdn.alpinelinux.org/alpine/edge/community\" \u003e\u003e /etc/apk/repositories \u0026\u0026     echo \"http://dl-cdn.alpinelinux.org/alpine/edge/testing\" \u003e\u003e /etc/apk/repositories \u0026\u0026     apk --update add php7-json php7-mongodb php7-redis \u0026\u0026     mv /usr/lib/php7/modules/* /usr/local/lib/php/extensions/no-debug-non-zts-20160303 \u0026\u0026     echo \"extension=redis.so\" \u003e\u003e /usr/local/etc/php/conf.d/redis.ini \u0026\u0026     echo \"extension=mongodb.so\" \u003e\u003e /usr/local/etc/php/conf.d/mongodb.ini"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/usr/bin/supervisord",
            "--nodaemon",
            "--configuration",
            "/etc/supervisor/conf.d/supervisord.conf",
            "--logfile",
            "/var/log/supervisor/supervisord.log",
            "--pidfile",
            "/var/run/supervisor/supervisord.pid"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
