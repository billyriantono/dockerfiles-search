{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openzipkin/zipkin-builder",
      "SourceCode": "FROM openzipkin/zipkin-builder",
      "Platform": "",
      "From": {
        "Image": "openzipkin/zipkin-builder"
      },
      "Commands": [
        {
          "Name": "workdir",
          "Path": "/code"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            ".",
            "/code"
          ]
        },
        {
          "CmdLine": [
            "mvn -B --no-transfer-progress package -DskipTests=true -pl module/collector-sqs,module/collector-kinesis,module/storage-elasticsearch-aws,module/storage-xray -am"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/zipkin-aws"
        },
        {
          "CmdLine": [
            "cp /code/module/collector-sqs/target/zipkin-module-collector-sqs-*-module.jar sqs.jar \u0026\u0026   (mkdir sqs \u0026\u0026 cd sqs \u0026\u0026 jar -xf ../sqs.jar) \u0026\u0026   rm sqs.jar"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cp /code/module/collector-kinesis/target/zipkin-module-collector-kinesis-*-module.jar kinesis.jar \u0026\u0026   (mkdir kinesis \u0026\u0026 cd kinesis \u0026\u0026 jar -xf ../kinesis.jar) \u0026\u0026   rm kinesis.jar"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cp /code/module/storage-elasticsearch-aws/target/zipkin-module-storage-elasticsearch-aws-*-module.jar elasticsearch-aws.jar \u0026\u0026   (mkdir elasticsearch-aws \u0026\u0026 cd elasticsearch-aws \u0026\u0026 jar -xf ../elasticsearch-aws.jar) \u0026\u0026   rm elasticsearch-aws.jar"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cp /code/module/storage-xray/target/zipkin-module-storage-xray-*-module.jar xray.jar \u0026\u0026   echo \u003e .xray_profile \u0026\u0026   (mkdir xray \u0026\u0026 cd xray \u0026\u0026 jar -xf ../xray.jar) \u0026\u0026   rm xray.jar"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    },
    {
      "Name": "",
      "BaseName": "openzipkin/zipkin:master",
      "SourceCode": "FROM openzipkin/zipkin:master",
      "Platform": "",
      "From": {
        "Image": "openzipkin/zipkin:master"
      },
      "Commands": [
        {
          "Maintainer": "Zipkin \"https://zipkin.io/\"",
          "Name": "maintainer"
        },
        {
          "Chown": "",
          "From": "0",
          "Name": "copy",
          "SourcesAndDest": [
            "/zipkin-aws/",
            "/zipkin/"
          ]
        },
        {
          "Env": [
            {
              "Key": "MODULE_OPTS",
              "Value": "\"-Dloader.path=sqs,kinesis,elasticsearch-aws,xray -Dspring.profiles.active=sqs,kinesis,elasticsearch-aws,xray\""
            }
          ],
          "Name": "env"
        }
      ]
    }
  ]
}