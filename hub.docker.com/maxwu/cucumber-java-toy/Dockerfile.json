        "Image": "siomiz/chrome"
      },
      "Commands": [
        {
          "Maintainer": "maxwu \"maxwunj@gmail.com\"",
          "Name": "maintainer"
        },
        {
          "Key": "MAVEN_VERSION",
          "Name": "arg",
          "Value": "3.3.9"
        },
        {
          "Key": "USER_HOME_DIR",
          "Name": "arg",
          "Value": "\"/root\""
        },
        {
          "Env": [
            {
              "Key": "DEBIAN_FRONTEND",
              "Value": "noninteractive"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y curl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y software-properties-common python-software-properties"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "add-apt-repository ppa:git-core/ppa"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo oracle-java8-installer shared/accepted-oracle-license-v1-1 select true | debconf-set-selections \u0026\u0026   add-apt-repository -y ppa:webupd8team/java \u0026\u0026   apt-get update \u0026\u0026   apt-get install -y oracle-java8-installer \u0026\u0026   rm -rf /var/lib/apt/lists/* \u0026\u0026   rm -rf /var/cache/oracle-jdk8-installer"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "JAVA_HOME",
              "Value": "/usr/lib/jvm/java-8-oracle"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p /usr/share/maven /usr/share/maven/ref   \u0026\u0026 curl -fsSL http://apache.osuosl.org/maven/maven-3/$MAVEN_VERSION/binaries/apache-maven-$MAVEN_VERSION-bin.tar.gz     | tar -xzC /usr/share/maven --strip-components=1   \u0026\u0026 ln -s /usr/share/maven/bin/mvn /usr/bin/mvn"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "MAVEN_HOME",
              "Value": "/usr/share/maven"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "MAVEN_CONFIG",
              "Value": "\"$USER_HOME_DIR/.m2\""
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "\"$USER_HOME_DIR/.m2\""
          ]
        },
        {
          "CmdLine": [
            "uname -a"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \" \\n                   /__/__/__/__/__/| \\n                  /__/__/__/__/__/|/ \\n              jgs |__'__'__'__'__|/  \\n  \\n\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /usr/src/app"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/usr/src/app"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            ".",
            "/usr/src/app"
          ]
        },
        {
          "CmdLine": [
            "export DISPLAY=:1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "google-chrome --no-sandbox --version"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "alias google-chrome='google-chrome --no-sandbox'"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mvn clean test"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
