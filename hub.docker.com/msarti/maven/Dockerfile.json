{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "msarti/java:jdk7",
      "SourceCode": "FROM msarti/java:jdk7",
      "Platform": "",
      "From": {
        "Image": "msarti/java:jdk7"
      },
      "Commands": [
        {
          "Maintainer": "Marco Sarti \u003cmarco.sarti@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "MAVEN_VERSION",
              "Value": "3.3.9"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl -fsSL https://archive.apache.org/dist/maven/maven-3/$MAVEN_VERSION/binaries/apache-maven-$MAVEN_VERSION-bin.tar.gz | tar xzf - -C /usr/share   \u0026\u0026 mv /usr/share/apache-maven-$MAVEN_VERSION /usr/share/maven   \u0026\u0026 ln -s /usr/share/maven/bin/mvn /usr/bin/mvn"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "MAVEN_HOME",
              "Value": "/usr/share/maven"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/root/.m2"
          ]
        },
        {
          "CmdLine": [
            "/bin/bash"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}