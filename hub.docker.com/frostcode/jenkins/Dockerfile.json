{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:8",
      "SourceCode": "FROM java:8",
      "Platform": "",
      "From": {
        "Image": "java:8"
      },
      "Commands": [
        {
          "Maintainer": "Heribert Hirth \"docker@frostcode.de\"",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get install -q -y git unzip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "MAVEN_VERSION",
              "Value": "3.2.5"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "M2_HOME",
              "Value": "/opt/maven"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "M2",
              "Value": "$M2_HOME/bin"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "MAVEN_OPTS",
              "Value": "-Xms256m -Xmx512m"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$M2:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "http://apache.openmirror.de/maven/maven-3/$MAVEN_VERSION/binaries/apache-maven-$MAVEN_VERSION-bin.tar.gz",
            "/tmp/"
          ]
        },
        {
          "CmdLine": [
            "mkdir -p $M2_HOME"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar -xzf /tmp/apache-maven-$MAVEN_VERSION-bin.tar.gz -C $M2_HOME/ --strip 1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm /tmp/apache-maven-$MAVEN_VERSION-bin.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GRADLE_VERSION",
              "Value": "2.3"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GRADLE_HOME",
              "Value": "/opt/gradle"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$GRADLE_HOME/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://services.gradle.org/distributions/gradle-$GRADLE_VERSION-bin.zip",
            "/tmp/"
          ]
        },
        {
          "CmdLine": [
            "unzip -q /tmp/gradle-$GRADLE_VERSION-bin.zip -d /opt/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mv /opt/gradle-$GRADLE_VERSION $GRADLE_HOME"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod u+x $GRADLE_HOME/bin/gradle"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm /tmp/gradle-$GRADLE_VERSION-bin.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "ANDROID_SDK_VERSION",
              "Value": "r24.1.2"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_BUILD_TOOLS_VERSION",
              "Value": "22.0.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_HOME",
              "Value": "/opt/android"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -q -y lib32z1 lib32ncurses5 lib32stdc++6"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "http://dl.google.com/android/android-sdk_$ANDROID_SDK_VERSION-linux.tgz",
            "/tmp/"
          ]
        },
        {
          "CmdLine": [
            "mkdir -p $ANDROID_HOME"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar -xzf /tmp/android-sdk_$ANDROID_SDK_VERSION-linux.tgz -C $ANDROID_HOME/ -o"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm /tmp/android-sdk_$ANDROID_SDK_VERSION-linux.tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:$ANDROID_HOME/android-sdk-linux/tools:$ANDROID_HOME/android-sdk-linux/platform-tools"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo y | $ANDROID_HOME/android-sdk-linux/tools/android update sdk --filter platform-tools,build-tools-$ANDROID_BUILD_TOOLS_VERSION,android-22,sys-img-x86-android-22 --no-ui --force -a"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "http://mirrors.jenkins-ci.org/war-stable/latest/jenkins.war",
            "/opt/"
          ]
        },
        {
          "CmdLine": [
            "chmod 644 /opt/jenkins.war"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "JENKINS_HOME",
              "Value": "/var/jenkins"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "java",
            "-jar",
            "/opt/jenkins.war"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "Name": "expose",
          "Ports": [
            "8080"
          ]
        },
        {
          "CmdLine": [
            ""
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}