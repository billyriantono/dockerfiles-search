{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "centos/systemd",
      "SourceCode": "FROM centos/systemd",
      "Platform": "",
      "From": {
        "Image": "centos/systemd"
      },
      "Commands": [
        {
          "Name": "workdir",
          "Path": "/data"
        },
        {
          "Key": "version",
          "Name": "arg",
          "Value": "2019.3.1"
        },
        {
          "Env": [
            {
              "Key": "pkg_tabcmd",
              "Value": "https://downloads.tableau.com/esdalt/\\${version}/tableau-tabcmd-\\${version//./-}.noarch.rpm"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "pkg_server",
              "Value": "https://downloads.tableau.com/esdalt/\\${version}/tableau-server-\\${version//./-}.x86_64.rpm"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "eval curl -#Lo ./tableau-tabcmd.rpm \"${pkg_tabcmd}\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "eval curl -#Lo ./tableau-server.rpm \"${pkg_server}\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /run/systemd/system"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "touch /usr/local/bin/sysctl \t\u0026\u0026 chmod +x /usr/local/bin/sysctl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo 'seq 0 9 | xargs -I% -- echo %,%' \t| tee /usr/local/bin/lscpu \t\u0026\u0026 chmod +x /usr/local/bin/lscpu"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo cpu: $(lscpu -p | grep -E -v '^#' | sort -u -t, -k 2,4 | wc -l)"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo 'echo; echo mem: 32768' \t| tee /usr/local/bin/free \t\u0026\u0026 chmod +x /usr/local/bin/free"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo mem: $(free -m | awk 'NR == 2 { print $2; }')"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "yum install -y iputils iproute sudo"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "yum install -y \t./tableau-tabcmd.rpm \t./tableau-server.rpm \t|| rpm -i \t--nodigest \t--noscripts \t--notriggers \t--nosignature \t--nofiledigest \t./tableau-server.rpm \t|| echo something fucked up"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -svf /opt/tableau/tabcmd/bin/tabcmd \t/usr/local/bin"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -svf /opt/tableau/tableau_server/packages/scripts.*/initialize-tsm \t/usr/local/bin"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "find /opt/tableau/tableau_server/packages/customer-bin.* \t-not -type d -exec ln -svf {} /usr/local/bin/ \\;"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Key": "USER",
          "Name": "arg",
          "Value": "tableau"
        },
        {
          "CmdLine": [
            "( \t\techo \"${USER} ALL=(ALL) NOPASSWD:ALL\" ; \t\techo \"Defaults:${USER} !requiretty\"   ; \t\techo \"Defaults secure_path = $PATH\"   ; \t) | tee -a /etc/sudoers.d/sudo"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "useradd -g users -m \"${USER}\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "( \t\techo \"runuser -l ${USER} -c 'sudo $(realpath $(which initialize-tsm)) --accepteula'\"; \t) | tee -a /etc/rc.local"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"${USER}:${USER}\" | chpasswd"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "systemctl enable rc-local || chmod +x /etc/rc.d/rc.local"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "443",
            "80",
            "8316",
            "8381",
            "8731",
            "8749",
            "8780",
            "8850"
          ]
        }
      ]
    }
  ]
}