          "SourcesAndDest": [
            "./.s2i/bin/",
            "/usr/libexec/s2i"
          ]
        },
        {
          "CmdLine": [
            "chown -R 1001:1001 /opt/app-root"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "1001"
        },
        {
