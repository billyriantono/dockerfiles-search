        "Image": "openshift/base-centos7"
      },
      "Commands": [
        {
          "Maintainer": "Dinesh Elangovan (dineshsanthi05@gmail.com)",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "LOGSTASH_VERSION",
              "Value": "2.3.0"
            },
            {
              "Key": "ELASTICSEARCH_SERVICE_HOST",
