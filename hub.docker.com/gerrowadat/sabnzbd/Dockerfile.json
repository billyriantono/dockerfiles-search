{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:wily",
      "SourceCode": "FROM ubuntu:wily",
      "Platform": "",
      "From": {
        "Image": "ubuntu:wily"
      },
      "Commands": [
        {
          "Maintainer": "Dave O Connor \u003cdocker@mail.andvari.net\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "DEBIAN_FRONTEND",
              "Value": "\"noninteractive\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo \"deb http://archive.ubuntu.com/ubuntu wily multiverse\" \u003e\u003e /etc/apt/sources.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get -q update \u0026\u0026 apt-get -qy --force-yes dist-upgrade"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -qy sabnzbdplus \u0026\u0026 apt-get clean"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "useradd --system --uid 2002 -M --shell /usr/bin/nologin media"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "media"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/config",
            "/data"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "8085"
          ]
        },
        {
          "CmdLine": [
            "/usr/bin/sabnzbdplus",
            "--config-file",
            "/config/sabnzbd.ini",
            "--server",
            "0.0.0.0:8085",
            "--console"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}