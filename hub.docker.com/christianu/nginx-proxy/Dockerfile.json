{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "nginx:1.14",
      "SourceCode": "FROM nginx:1.14",
      "Platform": "",
      "From": {
        "Image": "nginx:1.14"
      },
      "Commands": [
        {
          "Labels": [
            {
              "Key": "maintainer",
              "Value": "\"Jason Wilder mail@jasonwilder.com\""
            }
          ],
          "Name": "label"
        },
        {
          "CmdLine": [
            "apt-get update  \u0026\u0026 apt-get install -y -q --no-install-recommends     ca-certificates     wget  \u0026\u0026 apt-get clean  \u0026\u0026 rm -r /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"daemon off;\" \u003e\u003e /etc/nginx/nginx.conf  \u0026\u0026 sed -i 's/worker_processes  1/worker_processes  auto/' /etc/nginx/nginx.conf"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://github.com/jwilder/forego/releases/download/v0.16.1/forego",
            "/usr/local/bin/forego"
          ]
        },
        {
          "CmdLine": [
            "chmod u+x /usr/local/bin/forego"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "DOCKER_GEN_VERSION",
              "Value": "0.7.4"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget https://github.com/jwilder/docker-gen/releases/download/$DOCKER_GEN_VERSION/docker-gen-linux-amd64-$DOCKER_GEN_VERSION.tar.gz  \u0026\u0026 tar -C /usr/local/bin -xvzf docker-gen-linux-amd64-$DOCKER_GEN_VERSION.tar.gz  \u0026\u0026 rm /docker-gen-linux-amd64-$DOCKER_GEN_VERSION.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "network_internal.conf",
            "/etc/nginx/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            ".",
            "/app/"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/app/"
        },
        {
          "Env": [
            {
              "Key": "DOCKER_HOST",
              "Value": "unix:///tmp/docker.sock"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/etc/nginx/certs",
            "/etc/nginx/dhparam"
          ]
        },
        {
          "CmdLine": [
            "/app/docker-entrypoint.sh"
