          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo yes | $ANDROID_HOME/tools/bin/sdkmanager --licenses"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo yes | $ANDROID_HOME/tools/bin/sdkmanager \"tools\" \"platform-tools\" \"emulator\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo yes | $ANDROID_HOME/tools/bin/sdkmanager \"build-tools;${ANDROID_BUILD_TOOLS}\""
