      "Platform": "",
      "From": {
        "Image": "openjdk:8-jdk"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "SDK_HOME",
              "Value": "/usr/local"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get --quiet update --yes"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get --quiet install --yes wget tar unzip lib32stdc++6 lib32z1 git --no-install-recommends"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get --quiet install --yes libqt5widgets5 usbutils"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GRADLE_VERSION",
              "Value": "5.1.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GRADLE_SDK_URL",
              "Value": "https://services.gradle.org/distributions/gradle-${GRADLE_VERSION}-bin.zip"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl -sSL \"${GRADLE_SDK_URL}\" -o gradle-${GRADLE_VERSION}-bin.zip  \t\u0026\u0026 unzip gradle-${GRADLE_VERSION}-bin.zip -d ${SDK_HOME}  \t\u0026\u0026 rm -rf gradle-${GRADLE_VERSION}-bin.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
