            {
              "Key": "ANDROID_BUILD_TOOLS",
              "Value": "\"28.0.3\""
            },
            {
              "Key": "ANDROID_SDK_TOOLS",
              "Value": "\"3859397\""
            },
            {
              "Key": "ANDROID_IMAGES",
              "Value": "\"system-images;android-22;google_apis;armeabi-v7a\""
            }
          ],
          "Name": "env"
