          "CmdLine": [
            "echo d56f5187479451eabf01fb78af6dfcb131a6481e \u003e\u003e $ANDROID_HOME/licenses/android-sdk-license"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
