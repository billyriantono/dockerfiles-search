{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "iswanj/tc-react-native:latest",
      "SourceCode": "FROM iswanj/tc-react-native:latest",
      "Platform": "",
      "From": {
        "Image": "iswanj/tc-react-native:latest"
      },
      "Commands": [
        {
          "CmdLine": [
            "apt-get update -y \u0026\u0026 apt-get install python-dev libssl-dev autoconf automake libtool -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install --reinstall make -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "git clone https://github.com/facebook/watchman.git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd watchman \u0026\u0026 git checkout v4.7.0 \u0026\u0026 ./autogen.sh \u0026\u0026 ./configure \u0026\u0026 make \u0026\u0026 make install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rf watchman"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "8081"
          ]
        },
        {
          "Env": [
            {
              "Key": "USERNAME",
              "Value": "dev"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "adduser --disabled-password --gecos '' $USERNAME"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "TINI_VERSION",
              "Value": "v0.10.0"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://github.com/krallin/tini/releases/download/${TINI_VERSION}/tini",
            "/tini"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /tini"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "$USERNAME"
        },
        {
          "Name": "workdir",
          "Path": "/home/$USERNAME/app"
        },
        {
          "Env": [
            {
              "Key": "GRADLE_USER_HOME",
              "Value": "/home/$USERNAME/app/android/gradle_deps"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "/tini",
            "--"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}