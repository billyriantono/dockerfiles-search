{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "python:3.6.1-alpine3.6",
      "SourceCode": "FROM            python:3.6.1-alpine3.6",
      "Platform": "",
      "From": {
        "Image": "python:3.6.1-alpine3.6"
      },
      "Commands": [
        {
          "Name": "workdir",
          "Path": "/usr/src/app/"
        },
        {
          "CmdLine": [
            "apk --no-cache add --virtual .build-deps                     build-base                     openssl-dev                     libffi-dev                     zlib-dev"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Expression": "ARG     PIP_DISABLE_PIP_VERSION_CHECK=on",
          "Name": "onbuild"
        },
        {
          "Expression": "ARG     PIP_FIND_LINKS=",
          "Name": "onbuild"
        },
        {
          "Expression": "ARG     BUILD_DEPS=",
          "Name": "onbuild"
        },
        {
          "Expression": "ARG     BYPASS_PIP_INSTALL_ERRORS=",
          "Name": "onbuild"
        },
        {
          "Expression": "RUN     echo $BUILD_DEPS | xargs -r apk --no-cache add --virtual .build-deps",
          "Name": "onbuild"
        },
        {
          "Expression": "COPY    requirements.txt .",
          "Name": "onbuild"
        },
        {
          "Expression": "RUN     PIP_FIND_LINKS=$PIP_FIND_LINKS pip install                     -r requirements.txt",
          "Name": "onbuild"
        },
        {
          "Expression": "RUN     find /usr/local/lib -type f -name \"*.pyc\" -delete \u0026\u0026                 find /usr/local/lib -type f -name \"*.pyo\" -delete \u0026\u0026                 find /usr/local/lib -type d -name \"__pycache__\" -delete \u0026\u0026                 find /usr/local/lib -type d -name \"tests\" -exec rm -rf '{}' +",
          "Name": "onbuild"
        },
        {
          "Expression": "RUN     apk del .build-deps",
          "Name": "onbuild"
        }
      ]
    }
  ]
