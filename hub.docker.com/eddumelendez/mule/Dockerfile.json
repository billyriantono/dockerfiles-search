{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:8",
      "SourceCode": "FROM java:8",
      "Platform": "",
      "From": {
        "Image": "java:8"
      },
      "Commands": [
        {
          "Maintainer": "Eddú Meléndez \u003ceddu.melendez@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "MULE_HOME",
              "Value": "/opt/mule"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "MULE_VERSION",
              "Value": "3.6.0"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "set -x         \u0026\u0026 cd /opt         \u0026\u0026 curl -o mule.zip https://repository.mulesoft.org/nexus/content/repositories/releases/org/mule/distributions/mule-standalone/$MULE_VERSION/mule-standalone-$MULE_VERSION.zip         \u0026\u0026 unzip mule.zip         \u0026\u0026 mv mule-standalone-$MULE_VERSION mule         \u0026\u0026 rm mule.zip*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "$MULE_HOME"
        },
        {
          "Name": "volume",
          "Volumes": [
            "$MULE_HOME/apps"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "$MULE_HOME/conf"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "$MULE_HOME/domains"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "$MULE_HOME/logs"
          ]
        },
        {
          "CmdLine": [
            "./bin/mule"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}