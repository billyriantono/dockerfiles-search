        "Image": "radanalyticsio/base-notebook"
      },
      "Commands": [
        {
          "Name": "user",
          "User": "root"
        },
        {
          "CmdLine": [
            "mkdir /data"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "NB_USER",
              "Value": "nbuser"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "NB_UID",
              "Value": "1011"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "expose",
          "Ports": [
            "8888"
          ]
        },
        {
          "Name": "user",
          "User": "$NB_UID"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "wikieod.parquet",
            "/data/wikieod.parquet"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "msgs.parquet",
            "/data/msgs.parquet"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "var.ipynb",
            "/notebooks/var.ipynb"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "pyspark.ipynb",
            "/notebooks/pyspark.ipynb"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "ml-basics.ipynb",
            "/notebooks/ml-basics.ipynb"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "auto-mpg.json",
            "/notebooks/data/"
          ]
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "CmdLine": [
            "chown -R $NB_USER:root /home/$NB_USER /data     \u0026\u0026 find /home/$NB_USER -type d -exec chmod g+rwx,o+rx {} \\;     \u0026\u0026 find /home/$NB_USER -type f -exec chmod g+rw {} \\;     \u0026\u0026 find /data -type d -exec chmod g+rwx,o+rx {} \\;     \u0026\u0026 find /data -type f -exec chmod g+rw {} \\;     \u0026\u0026 chmod -f g+rw /notebooks/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "$NB_UID"
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "/home/$NB_USER"
            }
          ],
          "Name": "env"
        },
        {
          "Labels": [
            {
              "Key": "io.k8s.description",
