{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:openjdk-8u91-jdk",
      "SourceCode": "FROM java:openjdk-8u91-jdk",
      "Platform": "",
      "From": {
        "Image": "java:openjdk-8u91-jdk"
      },
      "Commands": [
        {
          "Name": "volume",
          "Volumes": [
            "/tmp"
          ]
        },
        {
          "Env": [
            {
              "Key": "MAVEN_VERSION",
              "Value": "3.3.9"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "/root"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p /usr/share/maven   \u0026\u0026 curl -fsSL http://apache.osuosl.org/maven/maven-3/$MAVEN_VERSION/binaries/apache-maven-$MAVEN_VERSION-bin.tar.gz     | tar -xzC /usr/share/maven --strip-components=1   \u0026\u0026 ln -s /usr/share/maven/bin/mvn /usr/bin/mvn"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "MAVEN_HOME",
              "Value": "/usr/share/maven"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            ".",
            "/tmp"
          ]
        },
        {
          "CmdLine": [
            "cd /tmp \u0026\u0026 mvn package \u0026\u0026 mv target/release.jar /app.jar"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "8080"
          ]
        },
        {
          "CmdLine": [
            "bash -c 'touch /app.jar'"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "java",
            "-Djava.security.egd=file:/dev/./urandom",
            "-jar",
            "/app.jar"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}