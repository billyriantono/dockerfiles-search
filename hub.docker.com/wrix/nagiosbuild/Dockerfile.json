{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "wrix/nagiosbase:latest",
      "SourceCode": "FROM wrix/nagiosbase:latest",
      "Platform": "",
      "From": {
        "Image": "wrix/nagiosbase:latest"
      },
      "Commands": [
        {
          "Maintainer": "rbjoergensen \u003crasmusbojorgensen@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "MAIL_APIKEY",
              "Value": "default"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DEFAULT_MAILTO",
              "Value": "default"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DEFAULT_MAILFROM",
              "Value": "default"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CONTACT_SMS",
              "Value": "default"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CONTACT_MAIL",
              "Value": "default"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "rm -rf /usr/local/nagios/etc/nagios.cfg \u0026\u0026     rm -rf /usr/local/nagios/etc/resource.cfg \u0026\u0026     rm -rf /usr/local/nagios/etc/cgi.cfg \u0026\u0026     rm -rf /usr/local/nagios/etc/objects/commands.cfg \u0026\u0026     rm -rf /usr/local/nagios/etc/objects/timeperiods.cfg \u0026\u0026 \t  rm -rf /usr/local/nagios/etc/objects/contacts.cfg"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/nagios.cfg",
            "/usr/local/nagios/etc/nagios.cfg"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/resource.cfg",
            "/usr/local/nagios/etc/resource.cfg"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/cgi.cfg",
            "/usr/local/nagios/etc/cgi.cfg"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/commands.cfg",
            "/usr/local/nagios/etc/objects/commands.cfg"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/contacts.cfg",
            "/usr/local/nagios/etc/objects/contacts.cfg"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/nrpe_commands.cfg",
            "/usr/local/nagios/etc/objects/nrpe_commands.cfg"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/timeperiods.cfg",
            "/usr/local/nagios/etc/objects/timeperiods.cfg"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "plugins/check_nrpe",
            "/usr/local/nagios/libexec/check_nrpe"
          ]
        },
        {
          "CmdLine": [
            "chmod 777                   /usr/local/nagios/libexec/check_nrpe"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rf                      /usr/local/nagios/etc/objects/localhost.cfg"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config/localhost.cfg",
            "/usr/local/nagios/etc/objects/localhost.cfg"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "hostconfig/",
            "/usr/local/nagios/etc/servers"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "scripts/sendmail.sh",
            "/utilities/sendmail.sh"
          ]
        },
        {
          "CmdLine": [
            "chmod 777                   /utilities/sendmail.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod +x                    /utilities/sendmail.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "80"
          ]
        },
        {
          "CmdLine": [
            "echo $MAIL_APIKEY \u003e\u003e /utilities/MAIL_APIKEY \u0026\u0026            echo $DEFAULT_MAILTO \u003e\u003e /utilities/MAILTO \u0026\u0026            echo $DEFAULT_MAILFROM \u003e\u003e /utilities/MAILFROM \u0026\u0026            sed -i 's/nagiosadminmail_default/'$CONTACT_MAIL'/g' /usr/local/nagios/etc/objects/contacts.cfg \u0026\u0026            sed -i 's/nagiosadminsms_default/'$CONTACT_SMS'/g' /usr/local/nagios/etc/objects/contacts.cfg \u0026\u0026            cat /usr/local/nagios/etc/objects/contacts.cfg \u0026\u0026            service apache2 restart \u0026\u0026            service nagios restart \u0026\u0026 \t\t       cat /usr/local/nagios/etc/htpasswd.users \u0026\u0026 \t\t       tail -f /usr/local/nagios/var/nagios.log"
          ],
          "Name": "entrypoint",
          "PrependShell": true
        }
      ]
    }
  ]
}