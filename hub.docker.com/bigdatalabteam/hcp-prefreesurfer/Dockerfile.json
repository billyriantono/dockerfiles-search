{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "centos:centos5.11",
      "SourceCode": "FROM centos:centos5.11",
      "Platform": "",
      "From": {
        "Image": "centos:centos5.11"
      },
      "Commands": [
        {
          "Maintainer": "laletscaria@yahoo.co.in",
          "Name": "maintainer"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "./yum.repos/libselinux.repo",
            "/etc/yum.repos.d"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "./yum.repos/CentOS-Base.repo",
            "/etc/yum.repos.d"
          ]
        },
        {
          "CmdLine": [
            "yum update -y \u0026\u0026 yum install -y     bc     curl-devel     epel-release     expat-devel     fontconfig.x86_64     freetype.x86_64     gettext-devel     git     libpng.x86_64     libSM.x86_64     libstdc++.x86_64     libXrender.x86_64     libXext.x86_64     openssl-devel     perl-ExtUtils-MakeMaker     tar     unzip     wget     zlib-devel"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rpm --rebuilddb ;     yum clean all \u0026\u0026 yum install -y     numpy.x86_64"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "./myFslInstallerScript.sh",
            "/myFslInstallerScript.sh"
          ]
        },
        {
          "CmdLine": [
            "echo -e \"/usr/local/src\" | ./myFslInstallerScript.sh 5.0.6 CentOS5 /usr/local/src /usr/local/etc/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget https://github.com/Washington-University/Pipelines/archive/v3.19.0.tar.gz -O pipelines.tar.gz;     tar zxvf /pipelines.tar.gz -C /usr/local/src/;     mv /usr/local/src/Pipelines-* /usr/local/src/pipelines;     rm -rf pipelines.tar.gz;     mkdir -p /usr/local/src/tools;     wget https://ftp.humanconnectome.org/workbench/workbench-rh_linux64-v1.2.3.zip -O workbench.zip;     unzip workbench.zip -d /usr/local/src/tools;     rm -rf workbench.zip;"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "FSLDIR",
              "Value": "/usr/local/src/fsl"
            },
            {
              "Key": "FSLOUTPUTTYPE",
              "Value": "NIFTI_GZ"
            },
            {
              "Key": "FSLGECUDAQ",
              "Value": "cuda.q"
            },
            {
              "Key": "CARET7DIR",
              "Value": "/usr/local/src/tools/workbench/"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "${FSLDIR}/bin:${PATH}"
            },
            {
              "Key": "FSLTCLSH",
              "Value": "${FSLDIR}/bin/fsltclsh"
            },
            {
              "Key": "FSLWISH",
              "Value": "${FSLDIR}/bin/fslwish"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "/usr/local/src"
            },
            {
              "Key": "PATH",
              "Value": "${PATH}:/usr/local/src/pipelines/Examples/Scripts"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir /usr/local/src/projects;     ln -s /usr/local/src/pipelines /usr/local/src/projects/Pipelines;"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "./monitor.sh",
            "/bin"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "./create-execution-dir.sh",
            "/bin"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "./checksums.sh",
            "/bin"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "./command-line-script.sh",
            "/bin"
          ]
        }
      ]
    }
  ]
}