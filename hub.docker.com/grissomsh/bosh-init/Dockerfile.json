{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:14.04",
      "SourceCode": "FROM ubuntu:14.04",
      "Platform": "",
      "From": {
        "Image": "ubuntu:14.04"
      },
      "Commands": [
        {
          "Maintainer": "grissomsh wang.tianqing.cn@gmail.com",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "REFRESH_TIME",
              "Value": "2015-7-7"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "rm /bin/sh \u0026\u0026 ln -s /bin/bash /bin/sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y wget"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget http://storage.googleapis.com/golang/go1.4.2.linux-amd64.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar -xzf go1.4.2.linux-amd64.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mv go /usr/local/go"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget https://s3.amazonaws.com/bosh-init-artifacts/bosh-init-0.0.70-linux-amd64"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod +x bosh-init-0.0.70-linux-amd64"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mv bosh-init-0.0.70-linux-amd64 /usr/local/bin/bosh-init"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y build-essential zlibc zlib1g-dev openssl libxslt-dev libxml2-dev libssl-dev libreadline6 libreadline6-dev libyaml-dev libsqlite3-dev sqlite3"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y ruby1.9.3 rubygems-integration"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GOROOT",
              "Value": "/usr/local/go"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GOBIN",
              "Value": "$GOROOT/bin"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GOPKG",
              "Value": "$GOROOT//pkg/tool/linux_amd64"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GOARCH",
              "Value": "amd64"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GOOS",
              "Value": "linux"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:$GOBIN:$GOPKG"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "rm -rf  go1.4.2.linux-amd64.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "go",
            "help"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}