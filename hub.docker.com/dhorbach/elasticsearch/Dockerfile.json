{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "dhorbach/oraclejdk7",
      "SourceCode": "FROM dhorbach/oraclejdk7",
      "Platform": "",
      "From": {
        "Image": "dhorbach/oraclejdk7"
      },
      "Commands": [
        {
          "Maintainer": "Dzmitry Horbach",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "ES_VERSION",
              "Value": "1.4.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ES",
              "Value": "elasticsearch-$ES_VERSION"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget -nv --output-document=/opt/$ES.tgz https://download.elasticsearch.org/elasticsearch/elasticsearch/$ES.tar.gz \u0026\u0026     tar -C /opt --extract --file /opt/$ES.tgz \u0026\u0026     rm -f $ES_PKG_NAME.tar.gz \u0026\u0026     ln -s /opt/$ES /opt/elasticsearch"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/data"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "elasticsearch.yml",
            "/opt/elasticsearch/config/elasticsearch.yml"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/opt/elasticsearch"
        },
        {
          "Name": "expose",
          "Ports": [
            "9200"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "9300"
          ]
        },
        {
          "CmdLine": [
            "/opt/elasticsearch/bin/elasticsearch"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}