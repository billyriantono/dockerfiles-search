{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "centos:centos7.3.1611",
      "SourceCode": "FROM centos:centos7.3.1611",
      "Platform": "",
      "From": {
        "Image": "centos:centos7.3.1611"
      },
      "Commands": [
        {
          "Maintainer": "TUTORize \u003cdeveloper@tutorize.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "RUBY_MAJOR",
              "Value": "2.3"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "RUBY_VERSION",
              "Value": "2.3.4"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DEFAULT_PREFIX",
              "Value": "/usr/local"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "BUNDLER_VERSION",
              "Value": "1.15.3"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "yum update -y \u0026\u0026     yum install -y gcc-c++                    openssl-devel                    readline-devel                    make \u0026\u0026     yum clean all -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /usr/local/etc \u0026\u0026     echo 'install: --no-document' \u003e\u003e /usr/local/etc/gemrc \u0026\u0026     echo 'update: --no-document'  \u003e\u003e /usr/local/etc/gemrc"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://cache.ruby-lang.org/pub/ruby/$RUBY_MAJOR/ruby-$RUBY_VERSION.tar.gz",
            "/tmp"
          ]
        },
        {
          "CmdLine": [
            "tar -xzf /tmp/ruby-$RUBY_VERSION.tar.gz -C /tmp"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd /tmp/ruby-$RUBY_VERSION \u0026\u0026 ./configure --prefix=$DEFAULT_PREFIX --disable-install-doc"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd /tmp/ruby-$RUBY_VERSION \u0026\u0026 make"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd /tmp/ruby-$RUBY_VERSION \u0026\u0026 make install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rf /tmp/ruby-$RUBY_VERSION"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "yum remove -y gcc-c++ readline-devel openssl-devel"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "gem install bundler --version \"$BUNDLER_VERSION\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GEM_HOME",
              "Value": "/usr/local/bundle"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "BUNDLE_PATH",
              "Value": "$GEM_HOME"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "BUNDLE_BIN",
              "Value": "$GEM_HOME/bin"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "BUNDLE_SILENCE_ROOT_WARNING",
              "Value": "1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "BUNDLE_APP_CONFIG",
              "Value": "$GEM_HOME"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$BUNDLE_BIN:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p \"$GEM_HOME\" \"$BUNDLE_BIN\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod 777 \"$GEM_HOME\" \"$BUNDLE_BIN\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "irb"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}