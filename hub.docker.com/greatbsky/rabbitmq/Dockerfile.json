{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "greatbsky/centos7:latest",
      "SourceCode": "FROM greatbsky/centos7:latest",
      "Platform": "",
      "From": {
        "Image": "greatbsky/centos7:latest"
      },
      "Commands": [
        {
          "Maintainer": "architect.bian",
          "Name": "maintainer"
        },
        {
          "Labels": [
            {
              "Key": "name",
              "Value": "\"RabbitMQ\""
            },
            {
              "Key": "license",
              "Value": "\"MIT\""
            },
            {
              "Key": "build-date",
              "Value": "\"20190305\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "VERSION",
              "Value": "3.7.12"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "yum update -y \u0026\u0026 cd /data/softs \u0026\u0026 wget https://dl.bintray.com/rabbitmq-erlang/rpm/erlang/21/el/7/x86_64/erlang-21.2.6-1.el7.x86_64.rpm \u0026\u0026 rpm -ivh erlang-21.2.6-1.el7.x86_64.rpm \u0026\u0026 yum install -y socat logrotate \u0026\u0026 wget https://dl.bintray.com/rabbitmq/all/rabbitmq-server/$VERSION/rabbitmq-server-generic-unix-$VERSION.tar.xz \u0026\u0026 xz -d rabbitmq-server-generic-unix-$VERSION.tar.xz \u0026\u0026 tar -vxf rabbitmq-server-generic-unix-$VERSION.tar \u0026\u0026 mv rabbitmq_server-$VERSION /data/env/rabbitmq_server \u0026\u0026 /data/env/rabbitmq_server/sbin/rabbitmq-plugins enable rabbitmq_management \u0026\u0026 rm -rf /data/softs/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "RABBITMQ_HOME",
              "Value": "/data/env/rabbitmq_server"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "\"${PATH}:${RABBITMQ_HOME}/sbin\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "rabbitmq-server"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}