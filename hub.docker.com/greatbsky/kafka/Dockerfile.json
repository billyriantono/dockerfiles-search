{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "greatbsky/openjdk:latest",
      "SourceCode": "FROM greatbsky/openjdk:latest",
      "Platform": "",
      "From": {
        "Image": "greatbsky/openjdk:latest"
      },
      "Commands": [
        {
          "Maintainer": "architect.bian",
          "Name": "maintainer"
        },
        {
          "Labels": [
            {
              "Key": "name",
              "Value": "\"kafka\""
            },
            {
              "Key": "license",
              "Value": "\"MIT\""
            },
            {
              "Key": "build-date",
              "Value": "\"20180125\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "VERSION",
              "Value": "1.0.0"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KAFKA_HOME",
              "Value": "/data/env/kafka"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KAFKA_URL",
              "Value": "http://www-us.apache.org/dist/kafka/$VERSION/kafka_2.11-$VERSION.tgz"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:$KAFKA_HOME/bin:"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "yum update -y \u0026\u0026 cd /data/softs \u0026\u0026 wget \"$KAFKA_URL\" \u0026\u0026 tar -zvxf kafka_2.11-$VERSION.tgz \u0026\u0026 mv /data/softs/kafka_2.11-$VERSION /data/env/kafka \u0026\u0026 rm -rf /data/softs/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "kafka-server-start.sh",
            "/data/env/kafka/config/server.properties"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}