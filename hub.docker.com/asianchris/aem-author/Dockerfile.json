{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "asianchris/aem-base",
      "SourceCode": "FROM asianchris/aem-base",
      "Platform": "",
      "From": {
        "Image": "asianchris/aem-base"
      },
      "Commands": [
        {
          "Maintainer": "asianchris",
          "Name": "maintainer"
        },
        {
          "Expression": "ADD aem-author-4502.jar /aem/aem-author-4502.jar",
          "Name": "onbuild"
        },
        {
          "Expression": "ADD license.properties /aem/license.properties",
          "Name": "onbuild"
        },
        {
          "Expression": "ADD https://raw.githubusercontent.com/asianchris/aem-author/master/postInstallHook.py /aem/postInstallHook.py",
          "Name": "onbuild"
        },
        {
          "Expression": "WORKDIR /aem",
          "Name": "onbuild"
        },
        {
          "Expression": "RUN java -XX:MaxPermSize=256m -Xmx1024M -jar aem-author-4502.jar -unpack -r nosamplecontent",
          "Name": "onbuild"
        },
        {
          "Expression": "ADD https://raw.githubusercontent.com/asianchris/aem-author/master/org.apache.sling.commons.log.LogManager.config /aem/crx-quickstart/install/",
          "Name": "onbuild"
        },
        {
          "Expression": "RUN [\"python\",\"aemInstaller.py\",\"-i\",\"aem-author-4502.jar\",\"-r\",\"author\",\"-p\",\"4502\"]",
          "Name": "onbuild"
        },
        {
          "Name": "expose",
          "Ports": [
            "4502",
            "8000"
          ]
        },
        {
          "CmdLine": [
            "/aem/crx-quickstart/bin/quickstart"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}