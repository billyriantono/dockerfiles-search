{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:12.04",
      "SourceCode": "FROM ubuntu:12.04",
      "Platform": "",
      "From": {
        "Image": "ubuntu:12.04"
      },
      "Commands": [
        {
          "Maintainer": "anita annamale",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "PREMSEQ_PATH",
              "Value": "https://github.com/upmcgenomics/PREMSEQ/releases/download/v1.0"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PREMSEQ_ZIP",
              "Value": "premseq_v1.0.tar.gz"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir /PREMSEQ"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir /PREMSEQ/data"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/PREMSEQ/data"
          ]
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install --yes openjdk-7-jre-headless python-argparse perl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "${PREMSEQ_PATH}/${PREMSEQ_ZIP}",
            "/tmp/"
          ]
        },
        {
          "CmdLine": [
            "cd /usr/local ; tar xvzf /tmp/${PREMSEQ_ZIP}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm /tmp/${PREMSEQ_ZIP}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "python",
            "/usr/local/premseq_v1.0/premseq.py"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}