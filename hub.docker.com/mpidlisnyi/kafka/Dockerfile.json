{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:openjdk-8-jre",
      "SourceCode": "FROM java:openjdk-8-jre",
      "Platform": "",
      "From": {
        "Image": "java:openjdk-8-jre"
      },
      "Commands": [
        {
          "Maintainer": "Maksym Pidlisnyi \u003cmaksim@nightbook.info\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "KAFKA_VERSION",
              "Value": "2.0.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KAFKA_HOME",
              "Value": "/usr/local/kafka"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SCALA_VERSION",
              "Value": "2.12"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "BASENAME",
              "Value": "kafka_${SCALA_VERSION}-${KAFKA_VERSION}"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "MIRROR_URL",
              "Value": "http://archive.apache.org"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PACKAGE_URL",
              "Value": "${MIRROR_URL}/dist/kafka/${KAFKA_VERSION}/${BASENAME}.tgz"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "${PATH}:${KAFKA_HOME}/bin"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p ${KAFKA_HOME} \u0026\u0026     wget -q ${PACKAGE_URL} -O /tmp/kafka.tgz \u0026\u0026     tar xf /tmp/kafka.tgz --strip-components=1 -C ${KAFKA_HOME} \u0026\u0026     rm /tmp/kafka.tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "entrypoint.sh",
            "/entrypoint"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /entrypoint"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "${KAFKA_HOME}"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "log4j.properties",
            "config/log4j.properties"
          ]
        },
        {
          "CmdLine": [
            "/entrypoint"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}