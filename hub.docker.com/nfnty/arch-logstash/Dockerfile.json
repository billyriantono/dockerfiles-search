{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "nfnty/arch-jre8:latest",
      "SourceCode": "FROM nfnty/arch-jre8:latest",
      "Platform": "",
      "From": {
        "Image": "nfnty/arch-jre8:latest"
      },
      "Commands": [
        {
          "Maintainer": "nfnty \u003cdocker@nfnty.se\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "ugid='880013' \u0026\u0026     groupadd --gid \"${ugid}\" contgrp \u0026\u0026     useradd --uid \"${ugid}\" --gid \"${ugid}\" --shell /usr/bin/false contusr"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "VERSION_LOGSTASH",
              "Value": "'6.5.4'"
            },
            {
              "Key": "JAVA_HOME",
              "Value": "'/usr/lib/jvm/default-runtime'"
            },
            {
              "Key": "SINCEDB_DIR",
              "Value": "'/var/lib/logstash'"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir /opt/logstash \u0026\u0026     set -o pipefail \u0026\u0026     curl --speed-limit 256000         \"https://artifacts.elastic.co/downloads/logstash/logstash-${VERSION_LOGSTASH}.tar.gz\" |         tar --extract --file=- --gzip --strip-components=1 --directory='/opt/logstash' \u0026\u0026     chmod --recursive 'u=rwX,g=rX,o=rX' /opt/logstash \u0026\u0026     install --directory --owner=contusr --group=contgrp --mode=700 /var/lib/logstash /var/log/logstash"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "contusr"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/var/lib/logstash"
          ]
        },
        {
          "CmdLine": [
            "/opt/logstash/bin/logstash",
            "--config",
            "/etc/logstash/conf.d"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}