{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "nfnty/arch-mini:latest",
      "SourceCode": "FROM nfnty/arch-mini:latest",
      "Platform": "",
      "From": {
        "Image": "nfnty/arch-mini:latest"
      },
      "Commands": [
        {
          "Maintainer": "nfnty \u003cdocker@nfnty.se\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "ugid='880016' \u0026\u0026     groupadd --gid \"${ugid}\" contgrp \u0026\u0026     useradd --uid \"${ugid}\" --gid \"${ugid}\" --shell /usr/bin/false contusr"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "VERSION_NGINX_MAINLINE",
              "Value": "'1.15.7-1'"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "pacman --sync --noconfirm --refresh --sysupgrade \u0026\u0026     pacman --sync --noconfirm \"nginx-mainline=${VERSION_NGINX_MAINLINE}\" \u0026\u0026     find /var/cache/pacman/pkg -mindepth 1 -delete \u0026\u0026     setcap 'cap_net_bind_service=ep' /usr/bin/nginx \u0026\u0026     chown --recursive contusr:contgrp /var/lib/nginx /var/log/nginx \u0026\u0026     install --directory --owner=contusr --group=contgrp --mode=700 /run/nginx"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "contusr"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/var/lib/nginx"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "443/tcp",
            "80/tcp"
          ]
        },
        {
          "CmdLine": [
            "/usr/bin/nginx",
            "-g",
            "daemon off;"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}