{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "jetty:9-jre8",
      "SourceCode": "FROM jetty:9-jre8",
      "Platform": "",
      "From": {
        "Image": "jetty:9-jre8"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "JETTY_WEBAPPS",
              "Value": "$JETTY_BASE/webapps"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DIGILIB_VERSION_MAJOR",
              "Value": "2.3"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DIGILIB_VERSION_MINOR",
              "Value": "2.3.6"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DIGILIB_VERSION_URL",
              "Value": "http://sourceforge.net/projects/digilib/files/releases/$DIGILIB_VERSION_MAJOR/digilib-webapp-$DIGILIB_VERSION_MINOR-srv3.war"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "digilib-webapp-2.3-SNAPSHOT-srv3.war",
            "tmp.war"
          ]
        },
        {
          "CmdLine": [
            "unzip -q tmp.war -d \"$JETTY_WEBAPPS\"/ROOT/ \u0026\u0026 rm tmp.war"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "jetty.xml",
            "$JETTY_BASE/etc/jetty.xml"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/var/lib/jetty/webapps/ROOT/sample-images"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "8080"
          ]
        }
      ]
    }
  ]
}