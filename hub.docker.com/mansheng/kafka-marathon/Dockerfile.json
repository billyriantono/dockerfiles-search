{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "anapsix/alpine-java:jdk8",
      "SourceCode": "FROM anapsix/alpine-java:jdk8",
      "Platform": "",
      "From": {
        "Image": "anapsix/alpine-java:jdk8"
      },
      "Commands": [
        {
          "Maintainer": "Mansheng Yang",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apk add --update wget jq coreutils"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "KAFKA_VERSION",
              "Value": "\"0.10.0.0\""
            },
            {
              "Key": "SCALA_VERSION",
              "Value": "\"2.11\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KAFKA_RELEASE_ARCHIVE",
              "Value": "kafka_${SCALA_VERSION}-${KAFKA_VERSION}.tgz"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/tmp"
        },
        {
          "CmdLine": [
            "wget https://dist.apache.org/repos/dist/release/kafka/${KAFKA_VERSION}/${KAFKA_RELEASE_ARCHIVE} \u0026\u0026   tar -xz -C /opt -f ${KAFKA_RELEASE_ARCHIVE} \u0026\u0026   rm ${KAFKA_RELEASE_ARCHIVE}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "KAFKA_HOME",
              "Value": "/opt/kafka_${SCALA_VERSION}-${KAFKA_VERSION}"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "start-kafka.sh",
            "/usr/bin/start-kafka.sh"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/data"
          ]
        },
        {
          "CmdLine": [
            "start-kafka.sh"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}