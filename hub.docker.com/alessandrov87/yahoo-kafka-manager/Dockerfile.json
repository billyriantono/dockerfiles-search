{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8-jdk",
      "SourceCode": "FROM openjdk:8-jdk",
      "Platform": "",
      "From": {
        "Image": "openjdk:8-jdk"
      },
      "Commands": [
        {
          "Labels": [
            {
              "Key": "maintainer",
              "Value": "\"alessandrov87 (https://github.com/AlessandroVaccarino)\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "ZK_HOSTS",
              "Value": "localhost:2181"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KM_VERSION",
              "Value": "2.0.0.2"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026     apt-get -y install apt-transport-https"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"deb https://dl.bintray.com/sbt/debian /\" | tee -a /etc/apt/sources.list.d/sbt.list \u0026\u0026     apt-key adv --keyserver hkps://keyserver.ubuntu.com:443 --recv 2EE0EA64E40A89B84B2DF73499E82A75642AC823 \u0026\u0026     apt-get update \u0026\u0026     apt-get -y install sbt"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /tmp \u0026\u0026     cd /tmp \u0026\u0026     wget https://github.com/yahoo/kafka-manager/archive/${KM_VERSION}.tar.gz \u0026\u0026     tar xxf ${KM_VERSION}.tar.gz \u0026\u0026     cd /tmp/kafka-manager-${KM_VERSION} \u0026\u0026     sbt clean dist \u0026\u0026     unzip  -d / ./target/universal/kafka-manager-${KM_VERSION}.zip \u0026\u0026     rm -fr /tmp/${KM_VERSION} /tmp/kafka-manager-${KM_VERSION}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/kafka-manager-${KM_VERSION}"
        },
        {
          "Name": "expose",
          "Ports": [
            "9000"
          ]
        },
        {
          "CmdLine": [
            "./bin/kafka-manager",
            "-Dconfig.file=conf/application.conf"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}