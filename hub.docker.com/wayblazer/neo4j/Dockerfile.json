{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "c12e/debian",
      "SourceCode": "FROM c12e/debian",
      "Platform": "",
      "From": {
        "Image": "c12e/debian"
      },
      "Commands": [
        {
          "Maintainer": "Ricky Stillwell ricky@wayblazer.com",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026    apt-get install -y supervisor wget"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "NEO4j_VERSION",
              "Value": "enterprise-2.2.5"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget -q -O /tmp/neo4j-$NEO4j_VERSION-unix.tar.gz http://dist.neo4j.org/neo4j-$NEO4j_VERSION-unix.tar.gz \u0026\u0026    tar -zxf /tmp/neo4j-$NEO4j_VERSION-unix.tar.gz -C /opt \u0026\u0026    ln -s /opt/neo4j-$NEO4j_VERSION /opt/neo4j \u0026\u0026    wget -q -O /opt/neo4j/lib/gson-2.2.4.jar http://search.maven.org/remotecontent?filepath=com/google/code/gson/gson/2.2.4/gson-2.2.4.jar"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "neo4j-server.properties",
            "/opt/neo4j/conf/neo4j-server.properties"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "neo4j-wrapper.conf",
            "/opt/neo4j/conf/neo4j-wrapper.conf"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "neo4j.properties",
            "/opt/neo4j/conf/neo4j.properties"
          ]
        },
        {
          "CmdLine": [
            "mkdir -p /data"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /logs"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/data"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/logs"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/config"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "neo4j_supervisor.conf",
            "/etc/supervisor/supervisord.conf"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "1337",
            "5001",
            "6001",
            "6362",
            "7474"
          ]
        },
        {
          "CmdLine": [
            "/usr/bin/supervisord"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}