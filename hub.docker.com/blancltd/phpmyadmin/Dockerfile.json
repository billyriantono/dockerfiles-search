          ]
        },
        {
          "CmdLine": [
            "sed -i     -e 's/.*blowfish.*//g'     -e \"s/'cookie'/'http'/g\"     -e 's|?\u003e|include \"/etc/phpMyAdmin/extra.inc.php\";|g' /etc/phpMyAdmin/config.inc.php"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed -i     -e 's/^Listen/#Listen/g'     -e's/^LoadModule/#LoadModule/g'     /etc/httpd/conf/httpd.conf"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm /etc/httpd/conf.d/welcome.conf /etc/httpd/conf.d/phpMyAdmin.conf"
