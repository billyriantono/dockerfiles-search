{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "node:4-slim",
      "SourceCode": "FROM node:4-slim",
      "Platform": "",
      "From": {
        "Image": "node:4-slim"
      },
      "Commands": [
        {
          "Maintainer": "Lukas Pustina \u003clukas.pustina@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "npm install -g coffee-script yo generator-hubot"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "adduser --home /carl carl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "carl"
        },
        {
          "Name": "workdir",
          "Path": "/carl"
        },
        {
          "Env": [
            {
              "Key": "HUBOT_NAME",
              "Value": "\"Carl\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "HUBOT_OWNER",
              "Value": "\"Lukas Pustina \u003clukas.pustina@gmail.com\u003e\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "HUBOT_DESCRIPTION",
              "Value": "\"Your friendly Ops Minion\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "HUBOT_SLACK_VERSON",
              "Value": "\"3.4.2\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "yo hubot --name=\"$HUBOT_NAME\" --owner=\"$HUBOT_OWNER\" --description=\"$HUBOT_DESCRIPTION\" --adapter=slack@${HUBOT_SLACK_VERSON} --defaults"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "npm install --save hubot-diagnostics"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "npm install --save hubot-auth@1.3.0"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "npm install --save hubot-http-status"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "npm install --save hubot-seen"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "npm install --save hubot-calculator"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "npm install --save hubot-grafana"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "npm install --save hubot-bosun"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "docker/external-scripts.json",
            "/carl/"
          ]
        },
        {
          "CmdLine": [
            "/bin/sh",
            "-c",
            "cd ~; ./bin/hubot -a slack;"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}