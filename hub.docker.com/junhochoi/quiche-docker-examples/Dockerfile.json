          "CmdLine": [
            "git clone --recursive https://github.com/cloudflare/quiche.git \u0026\u0026     curl -q -s https://nginx.org/download/nginx-1.16.1.tar.gz | tar xzf - \u0026\u0026     cd nginx-1.16.1 \u0026\u0026 patch -p1 -i ../quiche/extras/nginx/nginx-1.16.patch"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/build/nginx-1.16.1"
        },
        {
          "CmdLine": [
            "./configure         --build=\"quiche-$(git --git-dir=../quiche/.git rev-parse --short HEAD)\"    \t    --with-http_ssl_module    \t    --with-http_v2_module    \t    --with-http_v3_module    \t    --with-openssl=../quiche/deps/boringssl    \t    --with-quiche=../quiche \u0026\u0026     make -j`nproc` \u0026\u0026     make install"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    },
