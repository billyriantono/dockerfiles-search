{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "builder",
      "BaseName": "golang:1.12-alpine",
      "SourceCode": "FROM golang:1.12-alpine as builder",
      "Platform": "",
      "As": "builder",
      "From": {
        "Image": "golang:1.12-alpine"
      },
      "Commands": [
        {
          "Name": "workdir",
          "Path": "/go/src/github.com/qastub/deepmock"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            ".",
            "/go/src/github.com/qastub/deepmock"
          ]
        },
        {
          "Env": [
            {
              "Key": "GO111MODULE",
              "Value": "on"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GOPROXY",
              "Value": "https://proxy.qastub.com"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "set -e     \u0026\u0026 sed -i \"s/dl-cdn.alpinelinux.org/mirrors.aliyun.com/g\" /etc/apk/repositories     \u0026\u0026 apk update     \u0026\u0026 apk add git     \u0026\u0026 REVISION=`git rev-list -1 HEAD`     \u0026\u0026 go mod download     \u0026\u0026 go build -v -ldflags \"-X main.version=$REVISION\" -o deepmock cmd/main.go"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    },
    {
      "Name": "",
      "BaseName": "alpine",
      "SourceCode": "FROM alpine",
      "Platform": "",
      "From": {
        "Image": "alpine"
      },
      "Commands": [
        {
          "Name": "workdir",
          "Path": "/app"
        },
        {
          "Chown": "",
          "From": "builder",
          "Name": "copy",
          "SourcesAndDest": [
            "/go/src/github.com/qastub/deepmock/deepmock",
            "."
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "entrypoint.sh",
            "/usr/bin/"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/app/log"
          ]
        },
        {
          "Env": [
            {
              "Key": "DEEPMOCK_LOGFILE",
              "Value": "/app/log/deepmock.log"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "expose",
          "Ports": [
            "16600"
          ]
        },
        {
          "CmdLine": [
            "entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "-port",
            ":16600"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}