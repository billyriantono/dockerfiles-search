{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "alpine:latest",
      "SourceCode": "FROM alpine:latest",
      "Platform": "",
      "From": {
        "Image": "alpine:latest"
      },
      "Commands": [
        {
          "Maintainer": "Fabian Stegemann \u003cfst@zetaron.de\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apk --no-cache add     dnsmasq     openssl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "DOCKER_GEN_VERSION",
              "Value": "0.7.3"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DOCKER_HOST",
              "Value": "unix:///var/run/docker.sock"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget -qO- https://github.com/jwilder/docker-gen/releases/download/$DOCKER_GEN_VERSION/docker-gen-alpine-linux-amd64-$DOCKER_GEN_VERSION.tar.gz | tar xvz -C /usr/local/bin"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "config/dnsmasq.tmpl",
            "/etc/dnsmasq.tmpl"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "dnsmasq-reload",
            "/usr/local/bin/dnsmasq-reload"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "entrypoint",
            "/usr/local/bin/entrypoint"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "generate",
            "/usr/local/bin/generate"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/app/"
        },
        {
          "Name": "expose",
          "Ports": [
            "53/udp"
          ]
        },
        {
          "CmdLine": [
            "entrypoint"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "generate"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}