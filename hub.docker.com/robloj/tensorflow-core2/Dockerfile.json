      "SourceCode": "FROM robloj/tensorflow-sources",
      "Platform": "",
      "From": {
        "Image": "robloj/tensorflow-sources"
      },
      "Commands": [
        {
          "Maintainer": "Robert Obloj \u003crobert.obloj@ingbank.pl\u003e",
          "Name": "maintainer"
        },
        {
          "Key": "TF_NEED_JEMALLOC",
          "Name": "arg",
          "Value": "1"
        },
        {
          "Key": "TF_NEED_GCP",
          "Name": "arg",
          "Value": "1"
        },
        {
          "Key": "TF_NEED_HDFS",
          "Name": "arg",
          "Value": "1"
        },
        {
          "Key": "TF_NEED_AWS",
          "Name": "arg",
          "Value": "1"
        },
        {
          "Key": "TF_NEED_S3",
          "Name": "arg",
          "Value": "1"
        },
        {
          "Key": "TF_NEED_KAFKA",
          "Name": "arg",
          "Value": "1"
        },
        {
          "Key": "TF_ENABLE_XLA",
          "Name": "arg",
          "Value": "0"
        },
        {
          "Key": "TF_NEED_GDR",
          "Name": "arg",
          "Value": "0"
        },
        {
          "Key": "TF_NEED_VERBS",
          "Name": "arg",
          "Value": "0"
        },
        {
          "Key": "TF_NEED_MPI",
          "Name": "arg",
          "Value": "0"
        },
        {
          "Key": "TF_NEED_OPENCL_SYCL",
          "Name": "arg",
          "Value": "0"
        },
        {
          "Key": "TF_NEED_CUDA",
          "Name": "arg",
          "Value": "0"
        },
        {
          "Key": "TF_DOWNLOAD_CLANG",
          "Name": "arg",
          "Value": "0"
        },
        {
          "Key": "TF_SET_ANDROID_WORKSPACE",
          "Name": "arg",
          "Value": "0"
        },
        {
          "Env": [
            {
              "Key": "CC_OPT_FLAGS",
              "Value": "\"-mtune=core2\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PYTHON_BIN_PATH",
              "Value": "/usr/bin/python"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PYTHON_LIB_PATH",
              "Value": "/usr/lib64/python3.6/site-packages"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "USE_ABI_SWITCH",
              "Value": "\"5.0.0\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CURRENT_GCC_VER",
              "Value": "\"$(gcc -dumpversion)\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "if [ \"$(printf '%s\\n' \"$USE_ABI_SWITCH\" \"$CURRENT_GCC_VER\" | sort -V | head -n1)\" = \"$USE_ABI_SWITCH\" ]; then     export abi_switch=\"--cxxopt='-D_GLIBCXX_USE_CXX11_ABI=0'\"; fi"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -fs /usr/bin/python3.6 /usr/bin/python"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "python -m pip install --upgrade pip \u0026\u0026     python -m pip install six numpy wheel setuptools"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "python ./configure.py"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bazel build --config=opt       $abi_switch //tensorflow/tools/pip_package:build_pip_package 2\u003e\u00261 \u0026\u0026     bazel-bin/tensorflow/tools/pip_package/build_pip_package /tmp/tensorflow_pkg; sleep infinity"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}