{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "alpine:latest",
      "SourceCode": "FROM alpine:latest",
      "Platform": "",
      "From": {
        "Image": "alpine:latest"
      },
      "Commands": [
        {
          "Maintainer": "ksh2ksk4 \u003cksh2ksk4+docker.com@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "TIMEZONE",
              "Value": "'Asia/Tokyo'"
            },
            {
              "Key": "DISTRIBUTIONS",
              "Value": "\"./distributions\""
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/root"
        },
        {
          "CmdLine": [
            "apk update \u0026\u0026     apk upgrade \u0026\u0026     apk add tzdata \u0026\u0026     cp /usr/share/zoneinfo/${TIMEZONE} /etc/localtime \u0026\u0026     echo \"${TIMEZONE}\" \u003e/etc/timezone \u0026\u0026     apk add --virtual _mariadb-packages_         mariadb         mariadb-client \u0026\u0026     apk add --virtual _base-packages_         bash         file         findutils         less"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "${DISTRIBUTIONS}/root/*",
            "/root/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "${DISTRIBUTIONS}/mariadb/my.cnf",
            "/etc/mysql/"
          ]
        },
        {
          "CmdLine": [
            "mysql_install_db --user=mysql"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/var/lib/mysql"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "3306"
          ]
        },
        {
          "CmdLine": [
            "./entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}