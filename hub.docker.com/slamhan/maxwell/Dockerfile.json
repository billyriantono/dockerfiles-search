{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "maven:3.5-jdk-8",
      "SourceCode": "FROM maven:3.5-jdk-8",
      "Platform": "",
      "From": {
        "Image": "maven:3.5-jdk-8"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "MAXWELL_VERSION",
              "Value": "1.14.3"
            },
            {
              "Key": "KAFKA_VERSION",
              "Value": "1.0.0"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            ".",
            "/workspace"
          ]
        },
        {
          "CmdLine": [
            "apt-get update     \u0026\u0026 apt-get -y upgrade     \u0026\u0026 apt-get install -y build-essential     \u0026\u0026 cd /workspace     \u0026\u0026 KAFKA_VERSION=$KAFKA_VERSION make package MAXWELL_VERSION=$MAXWELL_VERSION     \u0026\u0026 mkdir /app     \u0026\u0026 mv /workspace/target/maxwell-$MAXWELL_VERSION/maxwell-$MAXWELL_VERSION/* /app/     \u0026\u0026 apt-get clean     \u0026\u0026 rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/* /usr/share/doc/* /workspace/     \u0026\u0026 echo \"$MAXWELL_VERSION\" \u003e /REVISION"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/app"
        },
        {
          "CmdLine": [
            "/bin/bash",
            "-c",
            "bin/maxwell --user=$MYSQL_USERNAME --password=$MYSQL_PASSWORD --host=$MYSQL_HOST --producer=kafka --kafka.bootstrap.servers=$KAFKA_BROKERS $MAXWELL_OPTIONS"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}