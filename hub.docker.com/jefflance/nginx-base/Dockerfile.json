{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "jefflance/debian-base:latest",
      "SourceCode": "FROM jefflance/debian-base:latest",
      "Platform": "",
      "From": {
        "Image": "jefflance/debian-base:latest"
      },
      "Commands": [
        {
          "Maintainer": "Jeff LANCE \u003cjeff.lance@mala.fr\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "NGINX_VERSION",
              "Value": "\"1.9.4-1~jessie\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-key adv --keyserver hkp://pgp.mit.edu:80 --recv-keys 573BFD6B3D8FBC641079A6ABABF5BD827BD9BF62"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"deb http://nginx.org/packages/mainline/debian/ jessie nginx\" \u003e\u003e /etc/apt/sources.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026     apt-get install -y ca-certificates nginx=${NGINX_VERSION} \u0026\u0026     apt-get clean"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -sf /dev/stdout /var/log/nginx/access.log     \u0026\u0026 ln -sf /dev/stderr /var/log/nginx/error.log"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "configure.sh",
            "/tmp/configure.sh"
          ]
        },
        {
          "CmdLine": [
            "/bin/bash /tmp/configure.sh \u0026\u0026 rm /tmp/configure.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/srv"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "443",
            "80"
          ]
        },
        {
          "CmdLine": [
            "/usr/sbin/runsvdir-start"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}