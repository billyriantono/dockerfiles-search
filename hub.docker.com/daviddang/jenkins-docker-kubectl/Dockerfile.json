{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "jenkins/jenkins:lts",
      "SourceCode": "FROM jenkins/jenkins:lts",
      "Platform": "",
      "From": {
        "Image": "jenkins/jenkins:lts"
      },
      "Commands": [
        {
          "Maintainer": "David Dang \u003cquyettien.ictu@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Name": "expose",
          "Ports": [
            "50000",
            "8080"
          ]
        },
        {
          "Env": [
            {
              "Key": "DOCKER_VERSION",
              "Value": "17.04.0-ce"
            },
            {
              "Key": "DOCKER_COMPOSE_VERSION",
              "Value": "1.14.0"
            },
            {
              "Key": "KUBECTL_VERSION",
              "Value": "v1.7.12"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "Name": "workdir",
          "Path": "/usr/local/bin"
        },
        {
          "CmdLine": [
            "apt-get update -qq \u0026\u0026 apt-get install -y -qq --no-install-recommends     make     \u0026\u0026 apt-get clean     \u0026\u0026 rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -fsSLO https://get.docker.com/builds/Linux/x86_64/docker-${DOCKER_VERSION}.tgz \t\t\u0026\u0026 tar --strip-components=1 -xvzf docker-${DOCKER_VERSION}.tgz -C /usr/local/bin \t\t\u0026\u0026 chmod -R +x /usr/local/bin/docker"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -L https://github.com/docker/compose/releases/download/${DOCKER_COMPOSE_VERSION}/docker-compose-Linux-x86_64 -o /usr/local/bin/docker-compose     \u0026\u0026 chmod +x /usr/local/bin/docker-compose"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -L https://storage.googleapis.com/kubernetes-release/release/${KUBECTL_VERSION}/bin/linux/amd64/kubectl -o /usr/local/bin/kubectl     \u0026\u0026 chmod +x /usr/local/bin/kubectl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "jenkins"
        },
        {
          "Name": "workdir",
          "Path": "$JENKINS_HOME"
        }
      ]
    }
  ]
}