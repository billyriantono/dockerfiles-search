{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "dartainers/dart-runtime:latest",
      "SourceCode": "FROM  dartainers/dart-runtime:latest",
      "Platform": "",
      "From": {
        "Image": "dartainers/dart-runtime:latest"
      },
      "Commands": [
        {
          "Maintainer": "Anatoly Pulyaevskiy \u003canatoly.pulyaevskiy@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Labels": [
            {
              "Key": "Description",
              "Value": "\"Docker image with Dart runtime and Content Shell\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "DEBIAN_FRONTEND",
              "Value": "noninteractive"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CHANNEL",
              "Value": "stable"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SDK_VERSION",
              "Value": "1.19.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ARCHIVE_URL",
              "Value": "https://storage.googleapis.com/dart-archive/channels/$CHANNEL/release/$SDK_VERSION"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo 'deb http://archive.ubuntu.com/ubuntu/ precise multiverse' \u003e\u003e /etc/apt/sources.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo 'deb-src http://archive.ubuntu.com/ubuntu/ precise multiverse' \u003e\u003e /etc/apt/sources.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo 'deb http://archive.ubuntu.com/ubuntu/ precise-updates multiverse' \u003e\u003e /etc/apt/sources.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo 'deb-src http://archive.ubuntu.com/ubuntu/ precise-updates multiverse' \u003e\u003e /etc/apt/sources.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo ttf-mscorefonts-installer msttcorefonts/accepted-mscorefonts-eula select true | debconf-set-selections"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install --no-install-recommends -y -q chromium-browser libudev0   ttf-kochi-gothic ttf-kochi-mincho ttf-mscorefonts-installer ttf-indic-fonts   ttf-dejavu-core ttf-indic-fonts-core fonts-thai-tlwg msttcorefonts xvfb"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/usr/local/content_shell"
        },
        {
          "CmdLine": [
            "curl $ARCHIVE_URL/dartium/content_shell-linux-x64-release.zip \u003e content_shell.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "unzip content_shell.zip \u003e /dev/null"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm content_shell.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/usr/local/content_shell/drt-lucid64-full-$CHANNEL-$SDK_VERSION.0:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DISPLAY",
              "Value": ":99.0"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/root"
        }
      ]
    }
  ]
}