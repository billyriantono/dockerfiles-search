{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:alpine",
      "SourceCode": "FROM openjdk:alpine",
      "Platform": "",
      "From": {
        "Image": "openjdk:alpine"
      },
      "Commands": [
        {
          "Maintainer": "Wouter Sonneveldt \u003cwouter@sonneveldt.net\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "BASE_URL",
              "Value": "\"http://ftb.cursecdn.com/FTB2/modpacks/FTBPresentsSkyfactory25\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "FTB_VERSION",
              "Value": "\"2_5_6\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SERVER_FILE",
              "Value": "\"FTBPresentsSkyfactory25Server.zip\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SERVER_PORT",
              "Value": "25565"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/minecraft"
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "CheckEula.sh",
            "/minecraft/"
          ]
        },
        {
          "CmdLine": [
            "adduser -D minecraft \u0026\u0026     mkdir -p /minecraft/world \u0026\u0026     apk --no-cache add curl wget \u0026\u0026     curl -SLO ${BASE_URL}/${FTB_VERSION}/${SERVER_FILE}  \u0026\u0026     unzip ${SERVER_FILE} \u0026\u0026     chmod u+x FTBInstall.sh ServerStart.sh CheckEula.sh \u0026\u0026     rm eula.txt \u0026\u0026     sed -i '2i /bin/sh /minecraft/CheckEula.sh' /minecraft/ServerStart.sh \u0026\u0026     chown -R minecraft:minecraft /minecraft"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "minecraft"
        },
        {
          "CmdLine": [
            "/minecraft/FTBInstall.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "${SERVER_PORT}"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/minecraft/world"
          ]
        },
        {
          "CmdLine": [
            "/bin/sh",
            "/minecraft/ServerStart.sh"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}