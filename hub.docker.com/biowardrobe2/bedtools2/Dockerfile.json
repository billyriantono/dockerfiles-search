{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "biowardrobe2/scidap:v0.0.2",
      "SourceCode": "FROM biowardrobe2/scidap:v0.0.2",
      "Platform": "",
      "From": {
        "Image": "biowardrobe2/scidap:v0.0.2"
      },
      "Commands": [
        {
          "Maintainer": "Andrey V Kartashov \"porter@porter.st\"",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "DEBIAN_FRONTEND",
              "Value": "noninteractive"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/tmp"
        },
        {
          "Env": [
            {
              "Key": "VERSION",
              "Value": "2.26.0"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "NAME",
              "Value": "bedtools2"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "URL",
              "Value": "\"https://github.com/arq5x/bedtools2/releases/download/v${VERSION}/bedtools-${VERSION}.tar.gz\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget -q -O - $URL | tar -zxv \u0026\u0026 cd ${NAME} \u0026\u0026 make -j 4 \u0026\u0026 cd .. \u0026\u0026 cp ./${NAME}/bin/bedtools /usr/local/bin/ \u0026\u0026 strip /usr/local/bin/*; true \u0026\u0026 rm -rf ./${NAME}/"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}