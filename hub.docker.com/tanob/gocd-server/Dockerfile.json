{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:8-jre",
      "SourceCode": "FROM java:8-jre",
      "Platform": "",
      "From": {
        "Image": "java:8-jre"
      },
      "Commands": [
        {
          "Maintainer": "Adriano Bonat \u003cadrianob@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "GOCD_VERSION",
              "Value": "16.5.0-3305"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GOCD_SERVER_SHA1",
              "Value": "58b648a0483c6e3edd624e2ccc8563c10cc6bbd6"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GOCD_SERVER_URL",
              "Value": "https://download.go.cd/binaries/${GOCD_VERSION}/deb/go-server-${GOCD_VERSION}.deb"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "set -x  \u0026\u0026 curl -fSL \"$GOCD_SERVER_URL\" -o go-server.deb  \u0026\u0026 echo \"$GOCD_SERVER_SHA1 go-server.deb\" | sha1sum -c -  \u0026\u0026 dpkg -i --debug=10 go-server.deb  \u0026\u0026 rm go-server.deb"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "set -x  \u0026\u0026 ln -sf /dev/stdout /var/log/go-server/go-server.log  \u0026\u0026 rm /etc/default/go-server"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "set-agent-key.sh",
            "/"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/var/lib/go-server",
            "/etc/go",
            "/var/go"
          ]
        },
        {
          "Env": [
            {
              "Key": "JAVA_HOME",
              "Value": "/usr/lib/jvm/java-8-openjdk-amd64/jre"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "expose",
          "Ports": [
            "8153",
            "8154"
          ]
        },
        {
          "CmdLine": [
            "/usr/share/go-server/server.sh"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}