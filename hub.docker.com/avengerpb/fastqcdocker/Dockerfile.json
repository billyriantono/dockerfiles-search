{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:15.04",
      "SourceCode": "FROM ubuntu:15.04",
      "Platform": "",
      "From": {
        "Image": "ubuntu:15.04"
      },
      "Commands": [
        {
          "Maintainer": "conrad stoerker",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "FASTQC_PATH",
              "Value": "http://www.bioinformatics.babraham.ac.uk/projects/fastqc"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "FASTQC_ZIP",
              "Value": "fastqc_v0.11.5.zip"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "STARTFILE",
              "Value": "https://raw.githubusercontent.com/conradstoerker/fastqc/master/start.sh"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir /fastqc"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir /fastqc/data"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/fastqc/data"
          ]
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install --yes     openjdk-7-jre     perl     unzip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install python"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "${FASTQC_PATH}/${FASTQC_ZIP}",
            "/tmp/"
          ]
        },
        {
          "CmdLine": [
            "cd /usr/local ; unzip /tmp/${FASTQC_ZIP}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod 755 /usr/local/FastQC/fastqc"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -s /usr/local/FastQC/fastqc /usr/local/bin/fastqc"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "fastqc.py",
            "/tmp"
          ]
        },
        {
          "CmdLine": [
            "ln -s /tmp/fastqc.py /usr/bin"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod a+x /tmp/fastqc.py"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}