{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "lucee/lucee4:latest",
      "SourceCode": "FROM lucee/lucee4:latest",
      "Platform": "",
      "From": {
        "Image": "lucee/lucee4:latest"
      },
      "Commands": [
        {
          "Maintainer": "Daemonites \u003chello@daemon.com.au\u003e",
          "Name": "maintainer"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "nginx_signing.key",
            "/tmp/nginx_signing.key"
          ]
        },
        {
          "CmdLine": [
            "apt-key add /tmp/nginx_signing.key"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"deb http://nginx.org/packages/debian/ stretch nginx\" \u003e\u003e /etc/apt/sources.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "set -x \u0026\u0026 \tapt-get update \u0026\u0026 \tapt-get install --no-install-recommends --no-install-suggests -y \t\tnginx \t\tsupervisor \u0026\u0026 \tapt-get clean \u0026\u0026 \trm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "nginx.conf",
            "/etc/nginx/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "default.conf",
            "/etc/nginx/conf.d/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "supervisord.conf",
            "/etc/supervisor/conf.d/"
          ]
        },
        {
          "CmdLine": [
            "mkdir -p /var/www"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "index.cfm",
            "/var/www/"
          ]
        },
        {
          "Expression": "RUN rm -rf /var/www/*",
          "Name": "onbuild"
        },
        {
          "Name": "expose",
          "Ports": [
            "443",
            "80"
          ]
        },
        {
          "CmdLine": [
            "supervisord",
            "-c",
            "/etc/supervisor/supervisord.conf"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}