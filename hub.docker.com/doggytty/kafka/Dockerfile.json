{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:alpine",
      "SourceCode": "FROM openjdk:alpine",
      "Platform": "",
      "From": {
        "Image": "openjdk:alpine"
      },
      "Commands": [
        {
          "Maintainer": "doggytty\u003cdoggytty@126.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "SCALA_VERSION",
              "Value": "2.12"
            },
            {
              "Key": "KAFKA_VERSION",
              "Value": "0.10.2.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KAFKA_HOME",
              "Value": "/usr/local/kafka_\"$SCALA_VERSION\"-\"$KAFKA_VERSION\""
            },
            {
              "Key": "PATH",
              "Value": "$PATH:/usr/local/kafka_\"$SCALA_VERSION\"-\"$KAFKA_VERSION\"/bin"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk update \u0026\u0026 apk upgrade \u0026\u0026 apk add bash \u0026\u0026     wget -c http://apache.claz.org/kafka/\"$KAFKA_VERSION\"/kafka_\"$SCALA_VERSION\"-\"$KAFKA_VERSION\".tgz -O /tmp/kafka_\"$SCALA_VERSION\"-\"$KAFKA_VERSION\".tgz \u0026\u0026     tar xfz /tmp/kafka_\"$SCALA_VERSION\"-\"$KAFKA_VERSION\".tgz -C /usr/local  \u0026\u0026     rm /tmp/kafka_\"$SCALA_VERSION\"-\"$KAFKA_VERSION\".tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "docker-entrypoint.sh",
            "/usr/local/bin/docker-entrypoint.sh"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "config/*",
            "${KAFKA_HOME}/config/"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "9092"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/data"
          ]
        },
        {
          "Name": "workdir",
          "Path": "${KAFKA_HOME}"
        },
        {
          "CmdLine": [
            "docker-entrypoint.sh"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}