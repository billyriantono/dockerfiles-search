{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "cagiti/docker-alpine-ide-base:latest",
      "SourceCode": "FROM cagiti/docker-alpine-ide-base:latest",
      "Platform": "",
      "From": {
        "Image": "cagiti/docker-alpine-ide-base:latest"
      },
      "Commands": [
        {
          "Maintainer": "Cai Cooper \"caicooper82@gmail.com\"",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "IDEA_CONFIG_PREFIX",
              "Value": "IdeaIC"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "IDEA_PRODUCT",
              "Value": "ideaIC"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "IDEA_MAJOR",
              "Value": "2017"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "IDEA_MINOR",
              "Value": "1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "IDEA_FLAVOUR",
              "Value": "3"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl -L http://download.jetbrains.com/idea/ideaIC-${IDEA_MAJOR}.${IDEA_MINOR}.${IDEA_FLAVOUR}-no-jdk.tar.gz | tar zx -C /opt \u0026\u0026     mv /opt/idea* /opt/intellij"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "developer"
        },
        {
          "CmdLine": [
            "/opt/intellij/bin/idea.sh"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}