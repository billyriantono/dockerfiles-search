{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8u121-alpine",
      "SourceCode": "FROM openjdk:8u121-alpine",
      "Platform": "",
      "From": {
        "Image": "openjdk:8u121-alpine"
      },
      "Commands": [
        {
          "Key": "JIRA_VERSION",
          "Name": "arg",
          "Value": "7.7.1"
        },
        {
          "Key": "JIRA_HOME",
          "Name": "arg",
          "Value": "/var/atlassian/jira"
        },
        {
          "Key": "JIRA_INSTALL",
          "Name": "arg",
          "Value": "/opt/atlassian/jira"
        },
        {
          "Key": "RUN_USER",
          "Name": "arg",
          "Value": "jira"
        },
        {
          "Key": "RUN_GROUP",
          "Name": "arg",
          "Value": "jira"
        },
        {
          "Key": "JIRA_DOWNLOAD_URI",
          "Name": "arg",
          "Value": "https://www.atlassian.com/software/jira/downloads/binary/atlassian-jira-core-${JIRA_VERSION}.tar.gz"
        },
        {
          "Key": "POSTGRES_DRIVER_VERSION",
          "Name": "arg",
          "Value": "42.1.4"
        },
        {
          "Key": "MYSQL_DRIVER_VERSION",
          "Name": "arg",
          "Value": "5.1.45"
        },
        {
          "Env": [
            {
              "Key": "LC_ALL",
              "Value": "C"
            },
            {
              "Key": "JIRA_HOME",
              "Value": "$JIRA_HOME"
            },
            {
              "Key": "JIRA_INSTALL",
              "Value": "$JIRA_INSTALL"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "addgroup -S \"${RUN_GROUP}\"     \u0026\u0026 adduser -S -s /bin/false -G \"${RUN_GROUP}\" \"${RUN_USER}\"     \u0026\u0026 mkdir -p \"${JIRA_HOME}\"             \"${JIRA_HOME}/caches/index\"              \"${JIRA_INSTALL}/conf/Catalina\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apk add --no-cache --virtual .build-deps         curl         tar     \u0026\u0026 apk add --no-cache         bash         fontconfig         ttf-dejavu"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -Ls \"${JIRA_DOWNLOAD_URI}\"         | tar -xz --directory \"${JIRA_INSTALL}\"             --strip-components=1 --no-same-owner     \u0026\u0026 cd \"${JIRA_INSTALL}/lib\"     \u0026\u0026 rm -f \"${JIRA_INSTALL}/lib/postgresql-9.*\"     \u0026\u0026 curl -Os \"https://jdbc.postgresql.org/download/postgresql-${POSTGRES_DRIVER_VERSION}.jar\"     \u0026\u0026 curl -Ls \"https://dev.mysql.com/get/Downloads/Connector-J/mysql-connector-java-${MYSQL_DRIVER_VERSION}.tar.gz\"         | tar -xz --directory \"${JIRA_INSTALL}/lib\"             --strip-components=1 --no-same-owner             \"mysql-connector-java-${MYSQL_DRIVER_VERSION}/mysql-connector-java-${MYSQL_DRIVER_VERSION}-bin.jar\"     \u0026\u0026 chmod -R 700 \"${JIRA_HOME}\"                     \"${JIRA_INSTALL}/conf\"                     \"${JIRA_INSTALL}/temp\"                     \"${JIRA_INSTALL}/logs\"                     \"${JIRA_INSTALL}/work\"     \u0026\u0026 chown -R ${RUN_USER}:${RUN_GROUP} \"${JIRA_HOME}\"                                          \"${JIRA_INSTALL}/conf\"                                          \"${JIRA_INSTALL}/temp\"                                          \"${JIRA_INSTALL}/logs\"                                          \"${JIRA_INSTALL}/work\"     \u0026\u0026 sed --in-place \"s/java version/openjdk version/g\" \"${JIRA_INSTALL}/bin/check-java.sh\"     \u0026\u0026 echo -e \"\\njira.home=${JIRA_HOME}\" \u003e\u003e \"${JIRA_INSTALL}/atlassian-jira/WEB-INF/classes/jira-application.properties\"     \u0026\u0026 touch -d \"@0\" \"${JIRA_INSTALL}/conf/server.xml\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apk del .build-deps"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "\"${RUN_USER}\":\"${RUN_GROUP}\""
        },
        {
          "Name": "expose",
          "Ports": [
            "8080"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "${JIRA_INSTALL}/logs",
            "${JIRA_INSTALL}/conf",
            "${JIRA_HOME}",
            "${JAVA_HOME}/jre/lib/security/"
          ]
        },
        {
          "Name": "workdir",
          "Path": "\"${JIRA_INSTALL}\""
        },
        {
          "CmdLine": [
            "./bin/catalina.sh",
            "run"
          ],
          "Name": "cmd",
          "PrependShell": false
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "./docker-entrypoint.sh",
            "/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "./entrypoint.d/",
            "/entrypoint.d/"
          ]
        },
        {
          "CmdLine": [
            "/docker-entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "Key": "BUILD_DATE",
          "Name": "arg",
          "Value": null
        },
        {
          "Key": "VCS_REF",
          "Name": "arg",
          "Value": null
        },
        {
          "Key": "VERSION",
          "Name": "arg",
          "Value": null
        },
        {
          "Labels": [
            {
              "Key": "maintainer",
              "Value": "\"LasLabs Inc \u003csupport@laslabs.com\u003e\""
            },
            {
              "Key": "org.label-schema.build-date",
              "Value": "$BUILD_DATE"
            },
            {
              "Key": "org.label-schema.name",
              "Value": "\"JIRA - Alpine\""
            },
            {
              "Key": "org.label-schema.description",
              "Value": "\"Provides a Docker image for JIRA on Alpine Linux.\""
            },
            {
              "Key": "org.label-schema.url",
              "Value": "\"https://laslabs.com/\""
            },
            {
              "Key": "org.label-schema.vcs-ref",
              "Value": "$VCS_REF"
            },
            {
              "Key": "org.label-schema.vcs-url",
              "Value": "\"https://github.com/LasLabs/docker-alpine-jira\""
            },
            {
              "Key": "org.label-schema.vendor",
              "Value": "\"LasLabs Inc.\""
            },
            {
              "Key": "org.label-schema.version",
              "Value": "$VERSION"
            },
            {
              "Key": "org.label-schema.schema-version",
              "Value": "\"1.0\""
            }
          ],
          "Name": "label"
        }
      ]
    }
  ]
}