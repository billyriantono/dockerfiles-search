{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "centos:latest",
      "SourceCode": "FROM centos:latest",
      "Platform": "",
      "From": {
        "Image": "centos:latest"
      },
      "Commands": [
        {
          "Maintainer": "Vincent Palmer \u003cshift-gh@someone.section.me\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "DEBIAN_FRONTEND",
              "Value": "noninteractive"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "yum install -y curl tar"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir /tmp/etcd"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl --silent https://api.github.com/repos/coreos/etcd/releases | sed -n 's|.*\\\"tag_name\\\": \\\"\\(.*\\)\\\".*|\\1|p' | head -n 1 \u003e /tmp/etcd_version"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -L https://github.com/coreos/etcd/releases/download/`cat /tmp/etcd_version`/etcd-`cat /tmp/etcd_version`-linux-amd64.tar.gz | tar -xz --directory /tmp/etcd --strip-components 1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cp /tmp/etcd/etcd /usr/local/bin/ \u0026\u0026 cp /tmp/etcd/etcdctl /usr/local/bin/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rf /tmp/etcd"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}