{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "seansummers/alpine-java:latest",
      "SourceCode": "FROM seansummers/alpine-java:latest",
      "Platform": "",
      "From": {
        "Image": "seansummers/alpine-java:latest"
      },
      "Commands": [
        {
          "Name": "workdir",
          "Path": "/opt"
        },
        {
          "CmdLine": [
            "apk.static add curl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://github.com/dweinstein/analysis-runner/archive/master.zip",
            "/tmp/analysis-runner.zip"
          ]
        },
        {
          "CmdLine": [
            "unzip /tmp/analysis-runner.zip \u0026\u0026     mv analysis-runner-master runner \u0026\u0026     rm -f /tmp/runner.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "APKTOOL_VER",
              "Value": "apktool_2.0.0"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/opt/apktool/"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://bitbucket.org/iBotPeaches/apktool/downloads/${APKTOOL_VER}.jar",
            "/opt/apktool/apktool.jar"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "./unpack.sh",
            "/opt/apktool/unpack.sh"
          ]
        },
        {
          "Env": [
            {
              "Key": "APKTOOL",
              "Value": "/opt/apktool/apktool.jar"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JAVA",
              "Value": "java"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JAVA_OPTS",
              "Value": "-Xmx512M"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "TOOL",
              "Value": "/opt/apktool/unpack.sh"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CONTENT_TYPE",
              "Value": "application/x-gzip"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "/opt/runner/runner.sh",
            "${TOOL}"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
