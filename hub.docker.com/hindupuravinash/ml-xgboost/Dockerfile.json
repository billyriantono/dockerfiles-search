{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "hindupuravinash/ml-base",
      "SourceCode": "FROM hindupuravinash/ml-base",
      "Platform": "",
      "From": {
        "Image": "hindupuravinash/ml-base"
      },
      "Commands": [
        {
          "Maintainer": "Aviansh Hindupur",
          "Name": "maintainer"
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "Env": [
            {
              "Key": "XGB_HOME",
              "Value": "/home"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PYTHONPATH",
              "Value": "$XGB_HOME/xgboost/python-package"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt update \u0026\u0026     apt install -y git make g++ libssl-dev libffi-dev \u0026\u0026     cd $XGB_HOME \u0026\u0026     git clone --recursive https://github.com/dmlc/xgboost \u0026\u0026     cd $XGB_HOME/xgboost \u0026\u0026     make -j4 \u0026\u0026     cd $XGB_HOME/xgboost/python-package \u0026\u0026     pip install scipy scikit-learn matplotlib \u0026\u0026     python setup.py install \u0026\u0026     apt remove -y git make g++ libssl-dev libffi-dev \u0026\u0026     apt-get autoremove -y \u0026\u0026     apt install -y libgomp1 libtk8.6"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "avi"
        }
      ]
    }
  ]
