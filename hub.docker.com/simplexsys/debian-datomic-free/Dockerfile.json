{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "simplexsys/debian-zulu-jdk:latest",
      "SourceCode": "FROM simplexsys/debian-zulu-jdk:latest",
      "Platform": "",
      "From": {
        "Image": "simplexsys/debian-zulu-jdk:latest"
      },
      "Commands": [
        {
          "Maintainer": "qsys \u003ckurt.sys@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get install html2text less"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"Be sure you agree with the license of datomic free:\" \u0026\u0026\tcurl -L https://my.datomic.com/datomic.com/datomic-free-edition-license.html | html2text - | less"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "DATOMIC_VERSION",
              "Value": "0.9.5173"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "(curl -L https://my.datomic.com/downloads/free/${DATOMIC_VERSION} -o /tmp/datomic-free.zip \u0026\u0026     unzip /tmp/datomic-free.zip -d /usr/share \u0026\u0026     rm /tmp/datomic-free.zip)"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -s /usr/share/datomic-free-${DATOMIC_VERSION} /usr/share/datomic \u0026\u0026    ln -s /usr/share/datomic/bin/transactor /usr/bin/transactor \u0026\u0026    ln -s /usr/share/datomic/conf /etc/datomic \u0026\u0026    ln -s /usr/share/datomic/data /var/lib/datomic"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"Be sure you agree with the license of datomic free - see above\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/etc/datomic",
            "/var/lib/datomic"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "4334",
            "4335",
            "4336"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/usr/share/datomic"
        },
        {
          "CmdLine": [
            "/usr/share/datomic/bin/transactor"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}