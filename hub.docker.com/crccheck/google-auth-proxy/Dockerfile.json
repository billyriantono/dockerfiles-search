{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "mini/base",
      "SourceCode": "FROM mini/base",
      "Platform": "",
      "From": {
        "Image": "mini/base"
      },
      "Commands": [
        {
          "Maintainer": "Chris \u003cc@crccheck.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "GAP_VERSION",
              "Value": "1.1.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GO_VERSION",
