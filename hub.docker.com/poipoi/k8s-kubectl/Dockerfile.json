{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "alpine:3.8",
      "SourceCode": "FROM alpine:3.8",
      "Platform": "",
      "From": {
        "Image": "alpine:3.8"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "KUBECTL_VERSION",
              "Value": "\"v1.13.1\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "AWS_IAM_AUTHENTICATOR",
              "Value": "\"1.10.3\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk -v --update add      ca-certificates      python3      less      mailcap      curl      bash      bash-doc      bash-completion"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -L https://storage.googleapis.com/kubernetes-release/release/${KUBECTL_VERSION}/bin/linux/amd64/kubectl -o /usr/local/bin/kubectl \u0026\u0026      curl -L https://amazon-eks.s3-us-west-2.amazonaws.com/${AWS_IAM_AUTHENTICATOR}/2018-07-26/bin/linux/amd64/aws-iam-authenticator -o /usr/local/bin/aws-iam-authenticator \u0026\u0026      chmod +x /usr/local/bin/aws-iam-authenticator \u0026\u0026      chmod +x /usr/local/bin/kubectl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "python3 -m ensurepip \u0026\u0026      rm -r /usr/lib/python*/ensurepip \u0026\u0026      pip3 install --upgrade pip setuptools \u0026\u0026      pip3 install --upgrade awscli==1.16.68 python-magic \u0026\u0026      if [ ! -e /usr/bin/pip ]; then ln -s pip3 /usr/bin/pip ; fi \u0026\u0026      if [[ ! -e /usr/bin/python ]]; then ln -sf /usr/bin/python3 /usr/bin/python; fi"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm /var/cache/apk/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/root/.aws"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/root/.kube"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/work"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/work"
        }
      ]
    }
  ]
}