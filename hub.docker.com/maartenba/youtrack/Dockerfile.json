{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:7",
      "SourceCode": "FROM openjdk:7",
      "Platform": "",
      "From": {
        "Image": "openjdk:7"
      },
      "Commands": [
        {
          "Maintainer": "Maarten Balliauw \u003cmaarten.balliauw@jetbrains.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "YOUTRACK_VERSION",
              "Value": "7.0.28450"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget http://download-cf.jetbrains.com/charisma/youtrack-$YOUTRACK_VERSION.jar -O youtrack.jar"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "80"
          ]
        },
        {
          "CmdLine": [
            "java",
            "-Xmx1g",
            "-XX:MaxPermSize=250m",
            "-Djava.awt.headless=true",
            "-jar",
            "youtrack.jar",
            "80"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}