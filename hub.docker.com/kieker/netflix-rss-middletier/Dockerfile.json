        {
          "CmdLine": [
            "mkdir -p ${KIEKER_AGENT_FOLDER} \u0026\u0026   mkdir -p ${KIEKER_LOGS_FOLDER} \u0026\u0026   mkdir -p ${KIEKER_JAR_FOLDER} \u0026\u0026   git clone ${KIEKER_RECIPESRSS_GIT} ${KIEKER_RECIPESRSS_FOLDER} \u0026\u0026   cd ${KIEKER_RECIPESRSS_FOLDER} \u0026\u0026   ./gradlew -x check -x test clean build \u0026\u0026   cp ${KIEKER_RECIPESRSS_FOLDER}/rss-middletier/build/libs/rss-middletier*SNAPSHOT.jar ${KIEKER_JAR_FOLDER}/${KIEKER_JAVA_JAR} \u0026\u0026   rm ${KIEKER_RECIPESRSS_FOLDER} -r \u0026\u0026   rm /root/.gradle -r"
