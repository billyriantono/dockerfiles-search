{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:8",
      "SourceCode": "FROM java:8",
      "Platform": "",
      "From": {
        "Image": "java:8"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "VERSION",
              "Value": "1.1.2"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KOTLIN_URL",
              "Value": "https://github.com/JetBrains/kotlin/releases/download/v${VERSION}/kotlin-compiler-${VERSION}.zip"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget ${KOTLIN_URL} -O /tmp/kotlin.zip \u0026\u0026     unzip /tmp/kotlin.zip -d /opt \u0026\u0026     rm /tmp/kotlin.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:/opt/kotlinc/bin"
            }
          ],
          "Name": "env"
        }
      ]
    }
  ]
}