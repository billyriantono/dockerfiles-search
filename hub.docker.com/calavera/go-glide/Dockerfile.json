      "Platform": "",
      "From": {
        "Image": "golang:1.8.0-stretch"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "GLIDE_VERSION",
              "Value": "v0.12.3"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
