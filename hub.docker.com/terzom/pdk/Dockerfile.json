{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:16.04",
      "SourceCode": "FROM ubuntu:16.04",
      "Platform": "",
      "From": {
        "Image": "ubuntu:16.04"
      },
      "Commands": [
        {
          "Maintainer": "Mike Terzo \u003cmike@terzo.org\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "URL",
              "Value": "\"https://pm.puppetlabs.com/cgi-bin/pdk_download.cgi?dist=ubuntu\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "VER",
              "Value": "1.2.0.0"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/pdk"
          ]
        },
        {
          "CmdLine": [
            "apt-get -y update                                                                  \u0026\u0026 apt-get install -y curl                                                     \u0026\u0026 curl -Lo /tmp/pdk.deb \"${URL}\u0026rel=16.04\u0026arch=amd64\u0026ver=${VER}\"              \u0026\u0026 dpkg -i /tmp/pdk.deb                                                        \u0026\u0026 rm -f /tmp/pdk.deb                                                          \u0026\u0026 rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/pdk"
        }
      ]
    }
  ]
}