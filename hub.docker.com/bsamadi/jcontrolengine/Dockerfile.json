{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "krallin/ubuntu-tini:trusty",
      "SourceCode": "FROM krallin/ubuntu-tini:trusty",
      "Platform": "",
      "From": {
        "Image": "krallin/ubuntu-tini:trusty"
      },
      "Commands": [
        {
          "Maintainer": "Behzad Samadi \u003cbehzad@mechatronics3d.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "CASADIVERSION",
              "Value": "3.1.0-rc1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CTRLVERSION",
              "Value": "0.7.0"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DL",
              "Value": "/home/jserver/Downloads"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "WS",
              "Value": "/home/jserver/workspace"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PKGS",
              "Value": "\"wget unzip gcc g++ gfortran git cmake liblapack-dev pkg-config swig spyder time\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "Py2_PKGS",
              "Value": "\"python-pip python-numpy python-scipy python-matplotlib\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "Py3_PKGS",
              "Value": "\"python3-pip python3-numpy python3-scipy python3-matplotlib\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JM_PKGS",
              "Value": "\"cython jcc subversion ant openjdk-6-jdk python-dev python-svn python-lxml python-nose zlib1g-dev libboost-dev dpkg-dev build-essential libwebkitgtk-dev libjpeg-dev libtiff-dev libgtk2.0-dev libsdl1.2-dev libgstreamer-plugins-base0.10-dev libnotify-dev freeglut3 freeglut3-dev\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ACADO_PKGS",
              "Value": "\"gnuplot doxygen graphviz\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PIP2",
              "Value": "\"jupyter vpython CVXcanon cvxpy APMonitor\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026     apt-get install -y --install-recommends $PKGS \u0026\u0026     apt-get install -y --install-recommends $Py2_PKGS \u0026\u0026     apt-get install -y --install-recommends $Py3_PKGS \u0026\u0026     apt-get install -y --install-recommends $ACADO_PKGS"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "pip install --upgrade pip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "pip install $PIP2"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "python3 -m pip install ipykernel"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "python3 -m ipykernel install --user"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "adduser \t\t\t--home /home/jserver \t\t\t--disabled-password \t\t\t--shell /bin/bash \t\t\t--gecos \"user for running a jupyter server\" \t\t\t--ingroup root \t\t\t--quiet \t\t\tjserver"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "USER",
              "Value": "jserver"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir $DL"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir $WS"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir $DL/control-$CTRLVERSION"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir $WS/control-$CTRLVERSION"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir $WS/control-$CTRLVERSION/examples"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget https://sourceforge.net/projects/python-control/files/control-$CTRLVERSION.tar.gz/download     -O $DL/control-$CTRLVERSION.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar zxvf $DL/control-$CTRLVERSION.tar.gz -C $DL/control-$CTRLVERSION \u0026\u0026     cd $DL/control-$CTRLVERSION/control-$CTRLVERSION \u0026\u0026 python setup.py install \u0026\u0026     cp -R examples $WS/control-$CTRLVERSION"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd $WS \u0026\u0026 mkdir cvxpy \u0026\u0026 mkdir cvxflow"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chown -R jserver $DL"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chown -R jserver $WS"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "jserver"
        },
        {
          "CmdLine": [
            "git clone https://github.com/cvxgrp/cvx_short_course.git $WS/cvxpy"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "git clone https://github.com/mwytock/cvxflow.git $WS/cvxflow"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget http://sourceforge.net/projects/casadi/files/CasADi/$CASADIVERSION/linux/casadi-py27-np1.9.1-v$CASADIVERSION.tar.gz/download     -O $DL/casadi-py27-np1.9.1-v$CASADIVERSION.tar.gz \u0026\u0026     mkdir $WS/casadi-py27-np1.9.1-v$CASADIVERSION \u0026\u0026     tar -zxvf $DL/casadi-py27-np1.9.1-v$CASADIVERSION.tar.gz     -C $WS/casadi-py27-np1.9.1-v$CASADIVERSION"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PYTHONPATH",
              "Value": "$PYTHONPATH:$WS/casadi-py27-np1.9.1-v$CASADIVERSION"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget http://sourceforge.net/projects/casadi/files/CasADi/$CASADIVERSION/casadi-example_pack-v$CASADIVERSION.zip     -O $DL/casadi-example_pack-v$CASADIVERSION.zip \u0026\u0026     mkdir $WS/casadi_examples \u0026\u0026     unzip $DL/casadi-example_pack-v$CASADIVERSION.zip     -d $WS/casadi_examples"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget http://apmonitor.com/wiki/uploads/Main/apm_python_v0.7.4.zip -O $DL/apm_python_v0.7.4.zip \u0026\u0026     mkdir $WS/APMonitor \u0026\u0026     unzip $DL/apm_python_v0.7.4.zip -d $WS/APMonitor"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/home/jserver/workspace"
        },
        {
          "Name": "expose",
          "Ports": [
            "8888"
          ]
        },
        {
          "CmdLine": [
            "/usr/bin/tini",
            "--"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "jupyter",
            "notebook",
            "--port=8888",
            "--no-browser",
            "--ip=0.0.0.0"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}