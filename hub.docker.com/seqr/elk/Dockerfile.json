{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "seqr/jre:8",
      "SourceCode": "FROM        seqr/jre:8",
      "Platform": "",
      "From": {
        "Image": "seqr/jre:8"
      },
      "Commands": [
        {
          "Maintainer": "Lars Larsson \u003clars.larsson@seamless.se\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "LOGSTASH_VERSION",
              "Value": "1.4.2"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ELASTICSEARCH_VERSION",
              "Value": "1.5.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "BIGDESK_VERSION",
              "Value": "2.5.0"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KIBANA_VERSION",
              "Value": "4.0.1"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl --silent --location --retry 3 --cacert /etc/ssl/certs/GeoTrust_Global_CA.crt     -O https://download.elasticsearch.org/logstash/logstash/logstash-$LOGSTASH_VERSION.tar.gz \u0026\u0026 gunzip logstash-$LOGSTASH_VERSION.tar.gz \u0026\u0026 tar xvf logstash-$LOGSTASH_VERSION.tar \u0026\u0026 mv logstash-$LOGSTASH_VERSION /logstash \u0026\u0026 rm -Rf logstash-$LOGSTASH_VERSION.tar"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "logstash_config",
            "/logstash/config"
          ]
        },
        {
          "CmdLine": [
            "cd /tmp \u0026\u0026   curl --silent --location --retry 3 --cacert /etc/ssl/certs/GeoTrust_Global_CA.crt   -O https://download.elasticsearch.org/elasticsearch/elasticsearch/elasticsearch-$ELASTICSEARCH_VERSION.tar.gz \u0026\u0026   gunzip elasticsearch-$ELASTICSEARCH_VERSION.tar.gz \u0026\u0026   tar xvf elasticsearch-$ELASTICSEARCH_VERSION.tar \u0026\u0026   rm -f elasticsearch-$ELASTICSEARCH_VERSION.tar \u0026\u0026   mv /tmp/elasticsearch-$ELASTICSEARCH_VERSION /elasticsearch"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/elasticsearch/bin/plugin -install lukas-vlcek/bigdesk/$BIGDESK_VERSION"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/elasticsearch/bin/plugin -install mobz/elasticsearch-head"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/elasticsearch/bin/plugin -install royrusso/elasticsearch-HQ"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "elasticsearch_config",
            "/elasticsearch/config"
          ]
        },
        {
          "CmdLine": [
            "cd /tmp     \u0026\u0026 curl --silent --location --retry 3 --cacert /etc/ssl/certs/GeoTrust_Global_CA.crt      -O https://download.elasticsearch.org/kibana/kibana/kibana-$KIBANA_VERSION-linux-x64.tar.gz     \u0026\u0026 gunzip kibana-*.tar.gz     \u0026\u0026 tar xf kibana-*.tar     \u0026\u0026 rm kibana-*.tar     \u0026\u0026 mv kibana-* /opt/kibana"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "opkg-install libstdcpp"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "adduser -D -s /sbin/nologin kibana"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "run.sh",
            "/run.sh"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /run.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/run.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
