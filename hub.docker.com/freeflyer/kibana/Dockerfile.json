{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8u111-jre",
      "SourceCode": "FROM openjdk:8u111-jre",
      "Platform": "",
      "From": {
        "Image": "openjdk:8u111-jre"
      },
      "Commands": [
        {
          "Maintainer": "Nicolas Zagulajew (freeeflyer) \u003cdocker.com@xoop.org\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "KIBANA_VERSION",
              "Value": "5.3.0"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KIBANA_HOME",
              "Value": "/opt/kibana-$KIBANA_VERSION-linux-x86_64"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ES_URL",
              "Value": "http://es:9200"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://artifacts.elastic.co/downloads/kibana/kibana-${KIBANA_VERSION}-linux-x86_64.tar.gz",
            "/tmp/kibana.tgz"
          ]
        },
        {
          "CmdLine": [
            "tar -C /opt -xzf /tmp/kibana.tgz     \u0026\u0026 rm /tmp/kibana.tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "start.sh",
            "/start"
          ]
        },
        {
          "Name": "workdir",
          "Path": "$KIBANA_HOME"
        },
        {
          "Name": "expose",
          "Ports": [
            "5601"
          ]
        },
        {
          "CmdLine": [
            "/start"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}