            }
          ],
          "Name": "env"
        },
        {
          "Name": "expose",
          "Ports": [
            "5000/tcp"
          ]
        },
        {
          "Env": [
            {
              "Key": "ASPNETCORE_URLS",
              "Value": "http://*:5000"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "From": "",
