{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "baselibrary/java:8",
      "SourceCode": "FROM baselibrary/java:8",
      "Platform": "",
      "From": {
        "Image": "baselibrary/java:8"
      },
      "Commands": [
        {
          "Maintainer": "ShawnMa \u003cqsma@thoughtworks.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "ELASTICSEARCH_MAJOR",
              "Value": "1.7"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ELASTICSEARCH_VERSION",
              "Value": "1.7.6"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-key adv --keyserver keyserver.ubuntu.com --recv-keys D88E42B4 \u0026\u0026  echo \"deb http://packages.elastic.co/elasticsearch/1.7 stable main\" \u003e /etc/apt/sources.list.d/elasticsearch.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026  apt-get install -y elasticsearch=$ELASTICSEARCH_VERSION \u0026\u0026  rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/usr/share/elasticsearch/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "docker-entrypoint.sh",
            "/docker-entrypoint.sh"
          ]
        },
        {
          "CmdLine": [
            "chmod a+x            /docker-entrypoint.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "9200",
            "9300"
          ]
        },
        {
          "CmdLine": [
            "/docker-entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "Name": "volume",
          "Volumes": [
            "/usr/share/elasticsearch/data"
          ]
        },
        {
          "CmdLine": [
            "elasticsearch"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}