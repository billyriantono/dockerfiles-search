{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:openjdk-7",
      "SourceCode": "FROM java:openjdk-7",
      "Platform": "",
      "From": {
        "Image": "java:openjdk-7"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "MAXWELL_VERSION",
              "Value": "1.8.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "KAFKA_VERSION",
              "Value": "0.10.1.0"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get -y upgrade"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install build-essential ruby -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            ".",
            "/workspace"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/workspace"
        },
        {
          "CmdLine": [
            "KAFKA_VERSION=$KAFKA_VERSION make package MAXWELL_VERSION=$MAXWELL_VERSION"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/workspace/target/maxwell-$MAXWELL_VERSION/"
        },
        {
          "CmdLine": [
            "mkdir /app   \u0026\u0026 mv ./* /app/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/app"
        },
        {
          "CmdLine": [
            "echo \"$MAXWELL_VERSION\" \u003e /REVISION"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bin/maxwell --user=$MYSQL_USERNAME --password=$MYSQL_PASSWORD --host=$MYSQL_HOST --producer=kafka --kafka.bootstrap.servers=$KAFKA_HOST:$KAFKA_PORT $MAXWELL_OPTIONS"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}