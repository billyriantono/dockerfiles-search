      "Platform": "",
      "From": {
        "Image": "phusion/baseimage:0.9.11"
      },
      "Commands": [
        {
          "Maintainer": "Thorben Stangenberg \u003cthorben@stangenberg.net\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "/root"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "/sbin/my_init"
          ],
          "Name": "cmd",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "locale-gen en_US.UTF-8 \u0026\u0026         echo 'LANG=\"en_US.UTF-8\"' \u003e /etc/default/locale"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rf /etc/service/sshd /etc/my_init.d/00_regen_ssh_host_keys.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-key adv --keyserver keyserver.ubuntu.com --recv-keys B97B0AFCAA1A47F044F244A07FCC7D46ACCC4CF8 \u0026\u0026   echo \"deb http://apt.postgresql.org/pub/repos/apt/ trusty-pgdg main 9.3\" \u003e /etc/apt/sources.list.d/pgdg.list \u0026\u0026   apt-get update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "DEBIAN_FRONTEND=noninteractive     apt-get install -y --force-yes         postgresql-9.3 postgresql-contrib-9.3"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "DEBIAN_FRONTEND=noninteractive apt-get install -y inotify-tools"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "postgresql.conf",
            "/etc/postgresql/9.3/main/postgresql.conf"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "pg_hba.conf",
            "/etc/postgresql/9.3/main/pg_hba.conf"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "scripts",
            "/scripts"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /scripts/start.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir /etc/service/postgresql"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -s /scripts/start.sh /etc/service/postgresql/run"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "5432"
          ]
        },
        {
          "CmdLine": [
            "apt-get clean \u0026\u0026 rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/* /var/lib/postgresql /var/log/postgresql"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
