{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "glerchundi/base:12.04",
      "SourceCode": "FROM glerchundi/base:12.04",
      "Platform": "",
      "From": {
        "Image": "glerchundi/base:12.04"
      },
      "Commands": [
        {
          "Maintainer": "Gorka Lerchundi Osa \u003cglertxundi@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "POSTGRESQL_VERSION",
              "Value": "9.3"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo \"deb http://apt.postgresql.org/pub/repos/apt/ precise-pgdg main\" \u003e /etc/apt/sources.list.d/pgdg.list \u0026\u0026     wget --quiet --no-check-certificate -O - https://www.postgresql.org/media/keys/ACCC4CF8.asc | apt-key add - \u0026\u0026     apt-get update \u0026\u0026     apt-get install -y postgresql-$POSTGRESQL_VERSION postgresql-client-$POSTGRESQL_VERSION"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "assets/",
            "/app/"
          ]
        },
        {
          "CmdLine": [
            "chmod 755 /app/init /app/setup/install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/app/setup/install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "22"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "5432"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "[\"/etc/postgresql\",",
            "/var/lib/postgresql\",",
            "\"/var/log/postgresql\"]"
          ]
        },
        {
          "CmdLine": [
            "/app/init"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "app:start"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}