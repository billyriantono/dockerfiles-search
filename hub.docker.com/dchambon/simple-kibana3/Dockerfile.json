{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "alpine:latest",
      "SourceCode": "FROM alpine:latest",
      "Platform": "",
      "From": {
        "Image": "alpine:latest"
      },
      "Commands": [
        {
          "Maintainer": "dchambon \u003cdamien.chambon.pro@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "KIBANA_VERSION",
              "Value": "3.1.2"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk add --update nginx wget \u0026\u0026 rm -rf /var/cache/apk/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /tmp/nginx/client-body"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget -q --no-check-certificate -O- https://download.elastic.co/kibana/kibana/kibana-${KIBANA_VERSION}.tar.gz | tar zxf -   \u0026\u0026 mv /kibana-${KIBANA_VERSION}/* /usr/share/nginx/html/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "nginx",
            "-g",
            "daemon off;"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}