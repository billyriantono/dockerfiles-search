{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "node:8.9.4",
      "SourceCode": "FROM node:8.9.4",
      "Platform": "",
      "From": {
        "Image": "node:8.9.4"
      },
      "Commands": [
        {
          "Maintainer": "Nicholas Evans \u003cnevans@410labs.com\u003e",
          "Name": "maintainer"
        },
        {
          "Key": "STATSD_RELEASE",
          "Name": "arg",
          "Value": "0.8.0"
        },
        {
          "Key": "STATSD_RELEASE_CHECKSUM",
          "Name": "arg",
          "Value": "ba2c8dcd4f7d863beb26405dd946ac6370a9b012a72c5e575c65c064fa4b4077"
        },
        {
          "Key": "STATSD_FILENAME",
          "Name": "arg",
          "Value": "v${STATSD_RELEASE}.tar.gz"
        },
        {
          "Key": "STATSD_RELEASE_URL",
          "Name": "arg",
          "Value": "https://github.com/etsy/statsd/archive/${STATSD_FILENAME}"
        },
        {
          "CmdLine": [
            "set -ex;     groupadd -r statsd; useradd -r -g statsd statsd;     cd /usr/local;     wget $STATSD_RELEASE_URL;     echo \"$STATSD_RELEASE_CHECKSUM ${STATSD_FILENAME}\" | sha256sum --check -;     tar xzf ${STATSD_FILENAME};     mv statsd-${STATSD_RELEASE} statsd;     sed \"s/process.EventEmitter/require('events').EventEmitter/\" statsd/lib/config.js -i;     rm -f ${STATSD_FILENAME};     mkdir /etc/statsd;     cp /usr/local/statsd/exampleConfig.js /etc/statsd/config.js;     sed -i 's/graphite.example.com/carbon-cache/' /etc/statsd/config.js;     chown -R statsd:statsd /etc/statsd"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "8125"
          ]
        },
        {
          "Name": "user",
          "User": "statsd"
        },
        {
          "Name": "workdir",
          "Path": "/usr/local/statsd"
        },
        {
          "CmdLine": [
            "node stats.js /etc/statsd/config.js"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}