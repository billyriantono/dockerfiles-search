{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "build",
      "BaseName": "ubuntu:16.04",
      "SourceCode": "FROM ubuntu:16.04 AS build",
      "Platform": "",
      "As": "build",
      "From": {
        "Image": "ubuntu:16.04"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "PLATFORM",
              "Value": "linux_amd64"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "TERRAFORM_VERSION",
              "Value": "0.11.11"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PACKER_VERISON",
              "Value": "1.3.4"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "VAULT_VERSION",
              "Value": "1.0.3"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026     apt-get install --no-install-recommends -y     curl     wget     unzip     git     jq     awscli     make \u0026\u0026 rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget -O /tmp/terraform.$$ https://releases.hashicorp.com/terraform/${TERRAFORM_VERSION}/terraform_${TERRAFORM_VERSION}_${PLATFORM}.zip \u0026\u0026     unzip -d /usr/local/bin /tmp/terraform.$$ \u0026\u0026     chmod +x /usr/local/bin"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget -O /tmp/packer.$$ https://releases.hashicorp.com/packer/${PACKER_VERISON}/packer_${PACKER_VERISON}_${PLATFORM}.zip \u0026\u0026     unzip -d /usr/local/bin /tmp/packer.$$ \u0026\u0026     chmod +x /usr/local/bin/packer"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget -O /tmp/vault.$$ https://releases.hashicorp.com/vault/${VAULT_VERSION}/vault_${VAULT_VERSION}_${PLATFORM}.zip \u0026\u0026     unzip -d /usr/local/bin /tmp/vault.$$ \u0026\u0026     chmod +x /usr/local/bin/vault"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}