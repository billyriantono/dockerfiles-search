{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "builder",
      "BaseName": "golang:1.13-alpine3.10",
      "SourceCode": "FROM golang:1.13-alpine3.10 as builder",
      "Platform": "",
      "As": "builder",
      "From": {
        "Image": "golang:1.13-alpine3.10"
      },
      "Commands": [
        {
          "CmdLine": [
            "set -eu;     apk --no-cache add         gcc         git         make         musl-dev"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GOPATH",
              "Value": "\"/go\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SRC_DIR",
              "Value": "\"${GOPATH}/src/code.gitea.io/gitea\""
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "\"$SRC_DIR\""
        },
        {
          "Key": "gitea_version",
          "Name": "arg",
          "Value": null
        },
        {
          "Key": "gitea_checksum",
          "Name": "arg",
          "Value": null
        },
        {
          "Key": "gitea_repo_url",
          "Name": "arg",
          "Value": "\"https://github.com/go-gitea/gitea.git\""
        },
        {
          "CmdLine": [
            "set -eu;     git clone --branch \"v${gitea_version}\" --depth 1 --no-checkout \"$gitea_repo_url\" .;     git checkout \"$gitea_checksum\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Key": "GITEA_BUILD_TAGS",
          "Name": "arg",
          "Value": "\"bindata sqlite\""
        },
        {
          "CmdLine": [
            "set -eu;     TAGS=\"$GITEA_BUILD_TAGS\" make generate build"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    },
    {
      "Name": "",
      "BaseName": "alpine:3.10",
      "SourceCode": "FROM alpine:3.10",
      "Platform": "",
      "From": {
        "Image": "alpine:3.10"
      },
      "Commands": [
        {
          "Key": "source",
          "Name": "arg",
          "Value": null
        },
        {
          "Labels": [
            {
              "Key": "maintainer",
              "Value": "'Piotr Orzechowski [orzechowski.tech]'"
            },
            {
              "Key": "source",
              "Value": "\"$source\""
            }
          ],
          "Name": "label"
        },
        {
          "CmdLine": [
            "set -euo pipefail;     apk --no-cache add         bash         ca-certificates         curl         git         linux-pam         openssh         s6         sqlite         su-exec         tzdata;     addgroup -S -g 1000 git;     adduser -S -D -G git -u 1000 -s '/bin/bash' -H -h '/data/git' git;     echo \"root:$(strings /dev/urandom | tr -d '[:cntrl:]' | head -c 128)\" | chpasswd;     echo \"git:$(strings /dev/urandom | tr -d '[:cntrl:]' | head -c 128)\" | chpasswd"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "files",
            "/"
          ]
        },
        {
          "Chown": "",
          "From": "builder",
          "Name": "copy",
          "SourcesAndDest": [
            "\"/go/src/code.gitea.io/gitea/gitea\"",
            "\"/usr/local/bin/gitea\""
          ]
        },
        {
          "CmdLine": [
            "set -eu;     mkdir '/service';     ln -s '/etc/s6/.s6-svscan' '/service/.s6-svscan';     ln -s '/etc/s6/syslogd' '/service/syslogd';     ln -s '/etc/s6/openssh' '/service/openssh';     ln -s '/etc/s6/gitea' '/service/gitea'"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/data"
          ]
        },
        {
          "Env": [
            {
              "Key": "GITEA_CUSTOM",
              "Value": "\"/data/gitea\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "/bin/s6-svscan",
            "/service"
          ],
          "Name": "cmd",
          "PrependShell": false
        },
        {
          "Name": "expose",
          "Ports": [
            "22",
            "3000"
          ]
        }
      ]
    }
  ]
}