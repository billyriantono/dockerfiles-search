{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "nginx:latest",
      "SourceCode": "FROM nginx:latest",
      "Platform": "",
      "From": {
        "Image": "nginx:latest"
      },
      "Commands": [
        {
          "Maintainer": "HanHor \"hanhor.wu@gmail.com\"",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "TZ",
              "Value": "Asia/Shanghai"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "sed     -e 's@worker_processes .*@worker_processes auto;@'     -e 's@access_log .*@access_log off;@'     -e 's@error_log .*@error_log off;@'     -i /etc/nginx/nginx.conf"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "443",
            "80"
          ]
        },
        {
          "CmdLine": [
            "nginx",
            "-g",
            "daemon off;"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}