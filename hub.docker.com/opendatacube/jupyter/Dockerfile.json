{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "opendatacube/datacube-core",
      "SourceCode": "FROM opendatacube/datacube-core",
      "Platform": "",
      "From": {
        "Image": "opendatacube/datacube-core"
      },
      "Commands": [
        {
          "CmdLine": [
            "pip3 install     jupyter matplotlib folium     \u0026\u0026 rm -rf $HOME/.cache/pip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "NB_USER",
              "Value": "jovyan"
            },
            {
              "Key": "NB_UID",
              "Value": "1000"
            },
            {
              "Key": "NB_GID",
              "Value": "100"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "/notebooks"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir $HOME"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://raw.githubusercontent.com/jupyter/docker-stacks/master/base-notebook/fix-permissions",
            "/usr/local/bin/fix-permissions"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /usr/local/bin/fix-permissions"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "useradd -m -s /bin/bash -N -u $NB_UID $NB_USER \u0026\u0026     chmod g+w /etc/passwd /etc/group \u0026\u0026     fix-permissions $HOME"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "$HOME"
        },
        {
          "Name": "expose",
          "Ports": [
            "8888"
          ]
        },
        {
          "CmdLine": [
            "jupyter",
            "notebook",
            "--ip=\"*\""
          ],
          "Name": "cmd",
          "PrependShell": false
        },
        {
          "Name": "user",
          "User": "$NB_UID"
        }
      ]
    }
  ]
}