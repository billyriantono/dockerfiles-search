            },
            {
              "Key": "description",
              "Value": "\"This is a quickstart image to install Atomic Registry on a single host. It is an open source enterprise registry solution based on the Origin project featuring single sign-on (SSO) user experience, a robust web interface and advanced role-based access control (RBAC).\""
            },
            {
              "Key": "INSTALL",
              "Value": "'docker run -i --rm                 --privileged --net=host                 -v /var/run:/var/run:rw                 -v /sys:/sys                 -v /etc/localtime:/etc/localtime:ro                 -v /var/lib/docker:/var/lib/docker:rw                 -v /var/lib/origin/:/var/lib/origin/                 -v /etc/origin/:/etc/origin/                 -v /:/host                 -e KUBECONFIG=/etc/origin/master/admin.kubeconfig                 --entrypoint /container/bin/install.sh                 $IMAGE'"
            },
            {
              "Key": "RUN",
              "Value": "'docker run -i --rm --privileged                 --net=host                 -v /:/host                 -v /var/lib/docker:/var/lib/docker:rw                 -v /etc/origin:/etc/origin                 -v /var/lib/registry:/var/lib/registry                 -e KUBECONFIG=/etc/origin/master/admin.kubeconfig                 --entrypoint /container/bin/run.sh                 $IMAGE'"
            },
            {
              "Key": "UNINSTALL",
              "Value": "'docker run -i --rm --privileged                 -v /:/host                 --entrypoint /container/bin/uninstall.sh                 $IMAGE'"
            }
