{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu",
      "SourceCode": "FROM ubuntu",
      "Platform": "",
      "From": {
        "Image": "ubuntu"
      },
      "Commands": [
        {
          "Maintainer": "Vishnu kannan \"\u003cvishnuk@google.com\u003e\"",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "INFLUXDB_VERSION",
              "Value": "0.12.2-1"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y curl \u0026\u0026 mkdir /app \u0026\u0026 curl -s -o /app/influxdb_latest_amd64.deb https://s3.amazonaws.com/influxdb/influxdb_${INFLUXDB_VERSION}_amd64.deb \u0026\u0026   dpkg -i /app/influxdb_latest_amd64.deb \u0026\u0026   rm /app/influxdb_latest_amd64.deb"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/opt/influxdb:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "config.toml",
            "/etc/influxdb.toml"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "2003",
            "25826",
            "4242",
            "8083",
            "8086",
            "8086/udp",
            "8088"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/data"
          ]
        },
        {
          "CmdLine": [
            "influxd",
            "--config",
            "/etc/influxdb.toml"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}