{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8",
      "SourceCode": "FROM openjdk:8",
      "Platform": "",
      "From": {
        "Image": "openjdk:8"
      },
      "Commands": [
        {
          "Labels": [
            {
              "Key": "MAINTAINER",
              "Value": "Sean"
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "SDK_URL",
              "Value": "\"https://dl.google.com/android/repository/sdk-tools-linux-4333796.zip\""
            },
            {
              "Key": "ANDROID_HOME",
              "Value": "\"/usr/local/android/sdk\""
            },
            {
              "Key": "ANDROID_VERSION",
              "Value": "28"
            },
            {
              "Key": "ANDROID_BUILD_TOOLS_VERSION",
              "Value": "28.0.3"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p \"${ANDROID_HOME}\" .android     \u0026\u0026 cd \"${ANDROID_HOME}\"     \u0026\u0026 curl -o sdk.zip ${SDK_URL}     \u0026\u0026 unzip sdk.zip     \u0026\u0026 rm sdk.zip     \u0026\u0026 mkdir -p \"/root/.android/\"     \u0026\u0026 touch \"/root/.android/repositories.cfg\"     \u0026\u0026 yes | tools/bin/sdkmanager --licenses"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "$ANDROID_HOME"
          ]
        },
        {
          "Name": "volume",
          "Volumes": [
            "/root/.gradle"
          ]
        },
        {
          "CmdLine": [
            "${ANDROID_HOME}/tools/bin/sdkmanager --update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "${ANDROID_HOME}/tools/bin/sdkmanager \"build-tools;${ANDROID_BUILD_TOOLS_VERSION}\"     \"platforms;android-${ANDROID_VERSION}\"     \"platform-tools\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir /app"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/app"
        }
      ]
    }
  ]
}