{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "debian:jessie",
      "SourceCode": "FROM debian:jessie",
      "Platform": "",
      "From": {
        "Image": "debian:jessie"
      },
      "Commands": [
        {
          "Maintainer": "Dylan Wang \"wanghaoyu@frazil.me\"",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "NGINX_VERSION",
              "Value": "1.9.6"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y ca-certificates build-essential wget libpcre3 libpcre3-dev zlib1g zlib1g-dev libssl-dev"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget http://www.openssl.org/source/openssl-1.0.2d.tar.gz   \u0026\u0026 tar -xvzf openssl-1.0.2d.tar.gz   \u0026\u0026 cd openssl-1.0.2d   \u0026\u0026 ./config     --prefix=/usr     --openssldir=/usr/ssl   \u0026\u0026 make \u0026\u0026 make install   \u0026\u0026 ./config shared     --prefix=/usr/local     --openssldir=/usr/local/ssl   \u0026\u0026 make clean   \u0026\u0026 make \u0026\u0026 make install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget http://nginx.org/download/nginx-${NGINX_VERSION}.tar.gz   \u0026\u0026 tar -xzvf nginx-${NGINX_VERSION}.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "conf",
            "/nginx-${NGINX_VERSION}/auto/lib/openssl/"
          ]
        },
        {
          "CmdLine": [
            "cd nginx-${NGINX_VERSION}   \u0026\u0026 ./configure     --prefix=/usr/local/nginx     --sbin-path=/usr/sbin/nginx     --conf-path=/etc/nginx/nginx.conf     --pid-path=/var/run/nginx.pid     --error-log-path=/var/log/nginx/error.log     --http-log-path=/var/log/nginx/access.log     --with-http_ssl_module     --with-http_v2_module     --with-openssl=/usr     --with-http_realip_module     --with-http_stub_status_module     --with-threads     --with-ipv6   \u0026\u0026 make   \u0026\u0026 make install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get purge build-essential -y   \u0026\u0026 apt-get autoremove -y"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -sf /dev/stdout /var/log/nginx/access.log"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -sf /dev/stderr /var/log/nginx/error.log"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/var/cache/nginx"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "443",
            "80"
          ]
        },
        {
          "CmdLine": [
            "nginx",
            "-g",
            "daemon off;"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}