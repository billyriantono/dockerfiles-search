      "Platform": "",
      "From": {
        "Image": "cismet/cids-java-maven:jdk-1.8u131-1.0"
      },
      "Commands": [
        {
          "Maintainer": "Pascal Dih√© \u003cpascal.dihe@cismet.de\u003e",
          "Name": "maintainer"
        },
        {
          "Labels": [
            {
              "Key": "de.cismet.cids.distribution.name",
              "Value": "\"cids-distribution image\""
            },
            {
              "Key": "de.cismet.cids.distribution.version",
              "Value": "\"5.0\""
            },
            {
              "Key": "de.cismet.cids.distribution.tag.docker",
              "Value": "\"5.0\""
            },
            {
              "Key": "de.cismet.cids.distribution.tag.git",
              "Value": "\"cidsDistribution-5.0\""
            },
            {
              "Key": "de.cismet.cids.distribution.descripton",
              "Value": "\"General abstract cids distribution Runtime Image\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "CIDS_ACCOUNT_EXTENSION",
              "Value": "CidsDistribution"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CIDS_CODEBASE",
              "Value": "http://localhost"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CIDS_DISTRIBUTION_DIR",
              "Value": "${DATA_DIR}"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CIDS_LIB_DIR",
              "Value": "${CIDS_DISTRIBUTION_DIR}/lib"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CIDS_GENERATOR_DIR",
              "Value": "${CIDS_DISTRIBUTION_DIR}/gen"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CIDS_SERVER_DIR",
              "Value": "${CIDS_DISTRIBUTION_DIR}/server"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CIDS_CLIENT_DIR",
              "Value": "${CIDS_DISTRIBUTION_DIR}/client"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CIDSCTL_CHECK_DATABASE_CONNECTION",
              "Value": "true"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CIDSCTL_START_WEBSERVER",
              "Value": "true"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo 'debconf debconf/frontend select Noninteractive' | debconf-set-selections     \u0026\u0026 apt-get update     \u0026\u0026 apt-get -y --no-install-recommends install nginx sysv-rc-conf"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sysv-rc-conf --list nginx     \u0026\u0026 sysv-rc-conf nginx off     \u0026\u0026 sysv-rc-conf --list nginx     \u0026\u0026 apt-get remove --purge --auto-remove -y sysv-rc-conf     \u0026\u0026 rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "${CIDS_DISTRIBUTION_DIR}/"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "cidsDistribution",
            "${CIDS_DISTRIBUTION_DIR}/"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "import/nginx/default",
            "/etc/nginx/sites-available/default"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "import/container_ctl.sh",
            "/"
          ]
        },
        {
          "CmdLine": [
            "chmod -R +x *.sh     \u0026\u0026 chmod +x /container_ctl.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/container_ctl.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "start"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
