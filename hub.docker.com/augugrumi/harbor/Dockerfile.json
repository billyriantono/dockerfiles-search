        {
          "CmdLine": [
            "path=\"$(pwd)\" \u0026\u0026     cd .. \u0026\u0026     git clone --recursive https://github.com/kubernetes-client/java \u0026\u0026     cd java \u0026\u0026     git checkout 8d6ab536f565ee951141b14bfd170629399d8c67 \u0026\u0026     mvn -q install -DskipTests \u0026\u0026     cd $path"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mvn package \u0026\u0026     cd target/ \u0026\u0026     mkdir -p bundle \u0026\u0026     cp ../api_sample.json bundle/ \u0026\u0026     cp $(ls | grep dependencies.jar) bundle/ \u0026\u0026     cd bundle \u0026\u0026     mv *.jar harbor.jar"
