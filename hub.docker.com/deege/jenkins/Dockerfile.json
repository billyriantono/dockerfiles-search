{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "t0nyhays/java8base",
      "SourceCode": "FROM t0nyhays/java8base",
      "Platform": "",
      "From": {
        "Image": "t0nyhays/java8base"
      },
      "Commands": [
        {
          "Maintainer": "DJ Spiess (http://www.deege.com, dj@deege.com)",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y wget git curl zip \u0026\u0026 rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "MAVEN_VERSION",
              "Value": "3.2.3"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GROOVY_VERSION",
              "Value": "2.3.8"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JENKINS_VERSION",
              "Value": "latest"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl -sSL http://archive.apache.org/dist/maven/maven-3/$MAVEN_VERSION/binaries/apache-maven-$MAVEN_VERSION-bin.tar.gz | tar xzf - -C /usr/share   \u0026\u0026 mv /usr/share/apache-maven-$MAVEN_VERSION /usr/share/maven   \u0026\u0026 ln -s /usr/share/maven/bin/mvn /usr/bin/mvn"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "MAVEN_HOME",
              "Value": "/usr/share/maven"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$MAVEN_HOME/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get install -y git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir /opt/groovy/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/opt"
        },
        {
          "CmdLine": [
            "cd /opt"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget http://dl.bintray.com/groovy/maven/groovy-binary-$GROOVY_VERSION.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "unzip groovy-binary-$GROOVY_VERSION.zip \u0026\u0026 mv groovy-$GROOVY_VERSION /groovy"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -s /groovy/bin/{grape,groovy,groovyConsole,groovyc,groovydoc,groovysh,java2groovy,startGroovy} /usr/local/bin/"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GROOVY_HOME",
              "Value": "/opt/groovy/"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$GROOVY_HOME/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "useradd -d /var/jenkins -m -s /bin/bash jenkins"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "http://mirrors.jenkins-ci.org/war/${JENKINS_VERSION}/jenkins.war",
            "/opt/jenkins.war"
          ]
        },
        {
          "CmdLine": [
            "chmod 644 /opt/jenkins.war"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "JENKINS_HOME",
              "Value": "/var/jenkins"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "expose",
          "Ports": [
            "8080"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "50000"
          ]
        },
        {
          "CmdLine": [
            "java",
            "-jar",
            "/opt/jenkins.war"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}