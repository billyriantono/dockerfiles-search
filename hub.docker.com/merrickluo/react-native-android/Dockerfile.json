          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:$ANDROID_HOME/tools"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:$ANDROID_HOME/tools/bin"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "yes | sdkmanager --licenses \u0026\u0026 \t\tsdkmanager \"platform-tools\" \"extras;android;m2repository\" \"platforms;android-23\" \"build-tools;23.0.1\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y lib32stdc++6 lib32z1 jq"
