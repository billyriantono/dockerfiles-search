          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:/opt/node-v7.8.0-linux-x64/bin"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "YARN_VERSION",
              "Value": "0.24.4"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "npm install -g yarn"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "ANDROID_SDK_URL",
              "Value": "https://dl.google.com/android/repository/sdk-tools-linux-3859397.zip"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p /opt/android-sdk \u0026\u0026 \t\tcurl $ANDROID_SDK_URL -o sdk.zip \u0026\u0026 \t\tunzip sdk.zip -d /opt/android-sdk \u0026\u0026 \t\trm sdk.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
