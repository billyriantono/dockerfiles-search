{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:latest",
      "SourceCode": "FROM ubuntu:latest",
      "Platform": "",
      "From": {
        "Image": "ubuntu:latest"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "JELLYFISH_VERSION",
              "Value": "\"2.2.6\""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y \tbuild-essential \ttar"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://github.com/gmarcais/Jellyfish/releases/download/v$JELLYFISH_VERSION/jellyfish-$JELLYFISH_VERSION.tar.gz",
            "jellyfish.tar.gz"
          ]
        },
        {
          "CmdLine": [
            "mkdir /src/ \u0026\u0026 \ttar zxvf jellyfish.tar.gz -C /src/ --strip-components=1 \u0026\u0026 \tcd /src/ \u0026\u0026 \t./configure \u0026\u0026 \tmake \u0026\u0026 \tmake install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "\"/src/bin/:$PATH\""
            },
            {
              "Key": "LD_LIBRARY_PATH",
              "Value": "\"/src/lib/:$LD_LIBRARY_PATH\""
            },
            {
              "Key": "MANPATH",
              "Value": "\"/src/share/man:$MANPATH\""
            },
            {
              "Key": "PKG_CONFIG_PATH",
              "Value": "\"/src/lib/pkgconfig:$PKG_CONFIG_PATH\""
            }
          ],
          "Name": "env"
        }
      ]
    }
  ]
}