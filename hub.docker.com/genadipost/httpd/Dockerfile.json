{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "centos/httpd-24-centos7",
      "SourceCode": "FROM centos/httpd-24-centos7",
      "Platform": "",
      "From": {
        "Image": "centos/httpd-24-centos7"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "SUMMARY",
              "Value": "\"httpd Documentation\""
            },
            {
              "Key": "DESCRIPTION",
              "Value": "\"httpd Documentation as it seen in https://httpd.apache.org/docs/2.4/. The image is based on centos/httpd-24-centos7 to run unprivileged httpd container.\""
            }
          ],
          "Name": "env"
        },
        {
          "Labels": [
            {
              "Key": "summary",
              "Value": "\"$SUMMARY\""
            },
            {
              "Key": "description",
              "Value": "\"$DESCRIPTION\""
            },
            {
              "Key": "io.k8s.description",
              "Value": "\"$DESCRIPTION\""
            },
            {
              "Key": "io.k8s.display-name",
              "Value": "\"httpd Documentation\""
            },
            {
              "Key": "io.openshift.expose-services",
              "Value": "\"8080:http,8443:https\""
            },
            {
              "Key": "io.openshift.tags",
              "Value": "\"documentation,docs,httpd\""
            },
            {
              "Key": "name",
              "Value": "\"genadipost/dockerized-docs-httpd\""
            },
            {
              "Key": "maintainer",
              "Value": "\"Genadi Postrilko \u003cgenadipost@gmail.com\u003e\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "HTTPD_VERSION",
              "Value": "2.4.33"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "CmdLine": [
            "yum -y install wget"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "default"
        },
        {
          "Name": "workdir",
          "Path": "/opt/app-root/src/"
        },
        {
          "CmdLine": [
            "wget http://apache.spd.co.il/httpd/docs/httpd-docs-$HTTPD_VERSION.en.zip  \u0026\u0026 unzip httpd-docs-$HTTPD_VERSION.en.zip  \u0026\u0026 mv httpd-docs-$HTTPD_VERSION.en/* /var/www/html/  \u0026\u0026 rm httpd-docs-$HTTPD_VERSION.en.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/usr/bin/run-httpd"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}