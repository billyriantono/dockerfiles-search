{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "andreptb/oracle-java:8",
      "SourceCode": "FROM andreptb/oracle-java:8",
      "Platform": "",
      "From": {
        "Image": "andreptb/oracle-java:8"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "MULE_HOME",
              "Value": "/opt/mule"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "MULE_VERSION",
              "Value": "3.8.0"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "cd ~ \u0026\u0026 wget https://repository-master.mulesoft.org/nexus/content/repositories/releases/org/mule/distributions/mule-standalone/${MULE_VERSION}/mule-standalone-${MULE_VERSION}.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd /opt \u0026\u0026 tar xvzf ~/mule-standalone-*.tar.gz \u0026\u0026 rm ~/mule-standalone-*.tar.gz \u0026\u0026 mv /opt/mule-standalone-* /opt/mule"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "${MULE_HOME}/logs",
            "${MULE_HOME}/conf",
            "${MULE_HOME}/apps",
            "/${MULE_HOME}/domains"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/opt/mule"
        },
        {
          "CmdLine": [
            "/opt/mule/bin/mule"
          ],
          "Name": "cmd",
          "PrependShell": false
        },
        {
          "Name": "expose",
          "Ports": [
            "8083"
          ]
        }
      ]
    }
  ]
}