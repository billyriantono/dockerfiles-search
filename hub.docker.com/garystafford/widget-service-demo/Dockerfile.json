{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "frolvlad/alpine-oraclejdk8:slim",
      "SourceCode": "FROM frolvlad/alpine-oraclejdk8:slim",
      "Platform": "",
      "From": {
        "Image": "frolvlad/alpine-oraclejdk8:slim"
      },
      "Commands": [
        {
          "Maintainer": "Gary A. Stafford",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "REFRESHED_AT",
              "Value": "2016-09-30"
            }
          ],
          "Name": "env"
        },
        {
          "Key": "BUILD_DATE",
          "Name": "arg",
          "Value": "\"\""
        },
        {
          "Key": "VCS_REF",
          "Name": "arg",
          "Value": "\"\""
        },
        {
          "Key": "VERSION",
          "Name": "arg",
          "Value": "\"\""
        },
        {
          "Labels": [
            {
              "Key": "org.label-schema.build-date",
              "Value": "$BUILD_DATE"
            },
            {
              "Key": "org.label-schema.name",
              "Value": "\"Widget Service Demo\""
            },
            {
              "Key": "org.label-schema.description",
              "Value": "\"Widget Service build using Docker Hub automated build\""
            },
            {
              "Key": "org.label-schema.license",
              "Value": "Apache-2.0"
            },
            {
              "Key": "org.label-schema.url",
              "Value": "\"https://github.com/garystafford/widget-service-demo\""
            },
            {
              "Key": "org.label-schema.vcs-ref",
              "Value": "$VCS_REF"
            },
            {
              "Key": "org.label-schema.vcs-url",
              "Value": "\"https://github.com/garystafford/widget-service-demo\""
            },
            {
              "Key": "org.label-schema.vendor",
              "Value": "\"Gary A. Stafford\""
            },
            {
              "Key": "org.label-schema.version",
              "Value": "$VERSION"
            },
            {
              "Key": "org.label-schema.schema-version",
              "Value": "\"1.0\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "GITHUB_REPO",
              "Value": "https://github.com/garystafford/widget-service-demo.git"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "set -ex   \u0026\u0026 apk --no-cache add --virtual .build-dependencies curl git   \u0026\u0026 mkdir /git_tmp   \u0026\u0026 git clone --branch build-artifacts --single-branch --depth 1 ${GITHUB_REPO} git_tmp   \u0026\u0026 mv git_tmp/*.jar app.jar   \u0026\u0026 rm -rf git_tmp   \u0026\u0026 touch app.jar   \u0026\u0026 touch widget-service.log"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "FILEBEAT_SRC_SHA1",
              "Value": "3fde7f5f5ea837140965a193bbb387c131c16d9c"
            },
            {
              "Key": "FILEBEAT_VERSION",
              "Value": "1.2.3"
            },
            {
              "Key": "FILEBEAT_URL",
              "Value": "http://download.elastic.co/beats/filebeat"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "set -ex   \u0026\u0026 curl -fsSL ${FILEBEAT_URL}/filebeat-${FILEBEAT_VERSION}-x86_64.tar.gz -o /tmp/filebeat.tar.gz   \u0026\u0026 cd /tmp   \u0026\u0026 echo \"${FILEBEAT_SRC_SHA1} *filebeat.tar.gz\" | sha1sum -c -   \u0026\u0026 tar -xzf filebeat.tar.gz   \u0026\u0026 cd filebeat-*   \u0026\u0026 cp filebeat /bin   \u0026\u0026 rm -rf /tmp/filebeat*   \u0026\u0026 mkdir -p /etc/pki/tls/certs"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "filebeat.yml",
            "/etc/filebeat/filebeat.yml"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "filebeat.template.json",
            "/etc/filebeat/filebeat.template.json"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "logstash-beats.crt",
            "/etc/pki/tls/certs/logstash-beats.crt"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "start.sh",
            "/usr/local/bin/start.sh"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /usr/local/bin/start.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/usr/local/bin/start.sh"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}