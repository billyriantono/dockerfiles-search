{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "nimmis/java-centos:oracle-7-jdk",
      "SourceCode": "FROM nimmis/java-centos:oracle-7-jdk",
      "Platform": "",
      "From": {
        "Image": "nimmis/java-centos:oracle-7-jdk"
      },
      "Commands": [
        {
          "Maintainer": "rohitgarg19@gmail.com",
          "Name": "maintainer"
        },
        {
          "Key": "MAVEN_VERSION",
          "Name": "arg",
          "Value": "3.3.9"
        },
        {
          "Key": "MAVEN_HOME",
          "Name": "arg",
          "Value": "\"/usr/share/maven\""
        },
        {
          "Key": "USER_HOME",
          "Name": "arg",
          "Value": "\"/root\""
        },
        {
          "CmdLine": [
            "wget http://www-eu.apache.org/dist/maven/maven-3/$MAVEN_VERSION/binaries/apache-maven-$MAVEN_VERSION-bin.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar -xzf apache-maven-$MAVEN_VERSION-bin.tar.gz -C /"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -s /apache-maven-$MAVEN_VERSION $MAVEN_HOME"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "MAVEN_HOME",
              "Value": "\"$MAVEN_HOME\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "MAVEN_CONFIG",
              "Value": "\"$USER_HOME/.m2\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JAVA_HOME",
              "Value": "/usr"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "ln -s \"$MAVEN_HOME/bin/mvn\" /usr/bin/mvn"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p \"$USER_HOME/.m2\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod 775 \"$USER_HOME/.m2\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mvn"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}