{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8-jdk-alpine",
      "SourceCode": "FROM openjdk:8-jdk-alpine",
      "Platform": "",
      "From": {
        "Image": "openjdk:8-jdk-alpine"
      },
      "Commands": [
        {
          "Maintainer": "MichaÃ«l van de Giessen \u003ctubbynl@github.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "GRAILS_VERSION",
              "Value": "2.5.6"
            }
