{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "php:7.1-alpine",
      "SourceCode": "FROM php:7.1-alpine",
      "Platform": "",
      "From": {
        "Image": "php:7.1-alpine"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "H5AI_VERSION",
              "Value": "0.29.0"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "H5AI_URL",
