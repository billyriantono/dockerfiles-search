{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ljishen/qemu-arm",
      "SourceCode": "FROM ljishen/qemu-arm",
      "Platform": "",
      "From": {
        "Image": "ljishen/qemu-arm"
      },
      "Commands": [
        {
          "Maintainer": "Jianshen Liu \u003cjliu120@ucsc.edu\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "MEMORY",
              "Value": "4G"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SMP",
              "Value": "4"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "VMLINUXZ",
              "Value": "/root/system/vmlinuz-4.9.0-3-armmp-lpae"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "INITRD_IMG",
              "Value": "/root/system/initrd.img-4.9.0-3-armmp-lpae"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "DRIVE_FILE",
              "Value": "/root/system/hda.qcow2"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "sh",
            "-c",
            "qemu-system-aarch64       -M virt       -m $MEMORY       -smp $SMP       -cpu cortex-a15       -kernel $VMLINUXZ       -initrd $INITRD_IMG       -append 'root=/dev/vda1 console=ttyAMA0'       -drive if=none,file=${DRIVE_FILE},format=qcow,id=hd0       -device virtio-blk-device,drive=hd0       -netdev user,id=net0,hostfwd=tcp::22-:22       -device virtio-net-device,netdev=net0       -nographic"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}