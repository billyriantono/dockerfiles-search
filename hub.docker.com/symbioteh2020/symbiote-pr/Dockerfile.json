{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8-jre-alpine",
      "SourceCode": "FROM openjdk:8-jre-alpine",
      "Platform": "",
      "From": {
        "Image": "openjdk:8-jre-alpine"
      },
      "Commands": [
        {
          "Name": "workdir",
          "Path": "/home"
        },
        {
          "Env": [
            {
              "Key": "componentName",
              "Value": "\"PlatformRegistry\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "componentVersion",
              "Value": "3.0.4"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk --no-cache add \tgit \tunzip \twget \tbash         \u0026\u0026 echo \"Downloading $componentName $componentVersion\" \t\u0026\u0026 wget \"https://jitpack.io/com/github/symbiote-h2020/$componentName/$componentVersion/$componentName-$componentVersion-run.jar\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "8203"
          ]
        },
        {
          "CmdLine": [
            "java $JAVA_HTTP_PROXY $JAVA_HTTPS_PROXY $JAVA_NON_PROXY_HOSTS -DSPRING_BOOT_WAIT_FOR_SERVICES=symbiote-aam:8080 -jar $(ls *run.jar)"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}