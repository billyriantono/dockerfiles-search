{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8-jre-slim",
      "SourceCode": "FROM openjdk:8-jre-slim",
      "Platform": "",
      "From": {
        "Image": "openjdk:8-jre-slim"
      },
      "Commands": [
        {
          "Key": "MIRROR",
          "Name": "arg",
          "Value": "\"https://repo1.maven.org/maven2/com/facebook/presto\""
        },
        {
          "Key": "PRESTO_VERSION",
          "Name": "arg",
          "Value": "\"0.216\""
        },
        {
          "Key": "PRESTO_BIN",
          "Name": "arg",
          "Value": "\"${MIRROR}/presto-server/${PRESTO_VERSION}/presto-server-${PRESTO_VERSION}.tar.gz\""
        },
        {
          "Key": "PRESTO_CLI_BIN",
          "Name": "arg",
          "Value": "\"${MIRROR}/presto-cli/${PRESTO_VERSION}/presto-cli-${PRESTO_VERSION}-executable.jar\""
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026     apt-get install -y --allow-unauthenticated curl wget less \u0026\u0026     apt-get clean \u0026\u0026 rm -rf /var/lib/apt/lists/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PRESTO_HOME",
              "Value": "/presto"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PRESTO_USER",
              "Value": "presto"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PRESTO_CONF_DIR",
              "Value": "${PRESTO_HOME}/etc"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:$PRESTO_HOME/bin"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "useradd         --create-home         --home-dir ${PRESTO_HOME}         --shell /bin/bash         $PRESTO_USER"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p $PRESTO_HOME \u0026\u0026     wget --quiet $PRESTO_BIN \u0026\u0026     tar xzf presto-server-${PRESTO_VERSION}.tar.gz \u0026\u0026     rm -rf presto-server-${PRESTO_VERSION}.tar.gz \u0026\u0026     mv presto-server-${PRESTO_VERSION}/* $PRESTO_HOME \u0026\u0026     rm -rf presto-server-${PRESTO_VERSION} \u0026\u0026     mkdir -p ${PRESTO_CONF_DIR}/catalog/ \u0026\u0026     mkdir -p ${PRESTO_HOME}/data \u0026\u0026     cd ${PRESTO_HOME}/bin \u0026\u0026     wget --quiet ${PRESTO_CLI_BIN} \u0026\u0026     mv presto-cli-${PRESTO_VERSION}-executable.jar presto \u0026\u0026     chmod +x presto \u0026\u0026     chown -R ${PRESTO_USER}:${PRESTO_USER} $PRESTO_HOME"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PYTHON2_DEBIAN_VERSION",
              "Value": "2.7.13-2"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y --no-install-recommends \t\tpython=\"${PYTHON2_DEBIAN_VERSION}\" \t\u0026\u0026 rm -rf /var/lib/apt/lists/*     \u0026\u0026 cd /usr/local/bin \t\u0026\u0026 rm -rf idle pydoc python python-config"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "$PRESTO_USER"
        },
        {
          "CmdLine": [
            "launcher",
            "run"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
