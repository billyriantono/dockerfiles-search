{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:openjdk-8-jdk",
      "SourceCode": "FROM java:openjdk-8-jdk",
      "Platform": "",
      "From": {
        "Image": "java:openjdk-8-jdk"
      },
      "Commands": [
        {
          "Maintainer": "Anirban Sen Chowdhary, senchowdhary.anirban@gmail.com",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "MULE_HOME",
              "Value": "/mule"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/Mule"
        },
        {
          "CmdLine": [
            "echo \"------ Unzipping the added zip --------\""
          ],
          "Name": "cmd",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget https://dl.dropboxusercontent.com/s/0oyk3litr1bobmh/mule-ee-distribution-standalone-3.8.5.zip \u0026\u0026             unzip mule-ee-distribution-standalone-3.8.5.zip \u0026\u0026             rm mule-ee-distribution-standalone-3.8.5.zip \u0026\u0026             ln -s /Mule/mule-enterprise-standalone-3.8.5 /mule"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/mule/logs",
            "/mule/domains",
            "/mule/bin",
            "/mule/conf"
          ]
        },
        {
          "CmdLine": [
            "echo \"------ Copy and deploy mule application in runtime --------\""
          ],
          "Name": "cmd",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"------ Deploying mule application in runtime ! --------\""
          ],
          "Name": "cmd",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://dl.dropboxusercontent.com/s/h0xwffhk4yzb6au/TestApplication.zip",
            "."
          ]
        },
        {
          "CmdLine": [
            "chmod 777  TestApplication.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ls -ltr"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cp TestApplication.zip /mule/apps"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm TestApplication.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chmod 777 /mule/apps"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ls -ltr /mule/apps"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
