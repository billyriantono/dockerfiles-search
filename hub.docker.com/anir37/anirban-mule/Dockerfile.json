        {
          "Name": "expose",
          "Ports": [
            "9082"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "5000"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "1098"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "7777"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "9997"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/mule"
        },
        {
          "CmdLine": [
            "/mule/bin/mule"
          ],
          "Name": "cmd",
