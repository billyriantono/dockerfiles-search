{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8",
      "SourceCode": "FROM openjdk:8",
      "Platform": "",
      "From": {
        "Image": "openjdk:8"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "CONFLUENT_URL",
              "Value": "http://packages.confluent.io/archive/5.1/confluent-community-5.1.0-2.11.tar.gz"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CONFLUENT_TMP_DEST",
              "Value": "/opt/confluent.tgz"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "CONFLUENT_WORKDIR",
              "Value": "/opt/confluent"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget $CONFLUENT_URL -O ${CONFLUENT_TMP_DEST} \u0026\u0026     mkdir -p ${CONFLUENT_WORKDIR} \u0026\u0026     tar -xvzpf ${CONFLUENT_TMP_DEST} --strip-components=1 -C ${CONFLUENT_WORKDIR}"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    },
    {
      "Name": "",
      "BaseName": "openjdk:8-jre-alpine",
      "SourceCode": "FROM openjdk:8-jre-alpine",
      "Platform": "",
      "From": {
        "Image": "openjdk:8-jre-alpine"
      },
      "Commands": [
        {
          "CmdLine": [
            "apk add --no-cache bash"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "0",
          "Name": "copy",
          "SourcesAndDest": [
            "/opt/confluent",
            "/opt/confluent"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "run",
            "/opt/confluent/bin/run"
          ]
        },
        {
          "CmdLine": [
            "rm -rf /opt/confluent/share/doc"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rf /opt/confluent/src"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rf /opt/confluent/lib"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/opt/confluent/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "[ \"/opt/confluent/bin\" ]"
        },
        {
          "CmdLine": [
            "tail",
            "-f",
            "/dev/null"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}