{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:bionic",
      "SourceCode": "FROM ubuntu:bionic",
      "Platform": "",
      "From": {
        "Image": "ubuntu:bionic"
      },
      "Commands": [
        {
          "Maintainer": "TheDevMinerTV \u003ctobigames200@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "SPIGOT_VERSION",
              "Value": "1.14.3"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt update \u0026\u0026 apt install -y openjdk-8-jre-headless ca-certificates-java wget nano;     wget -q https://cdn.getbukkit.org/spigot/spigot-${SPIGOT_VERSION}.jar -O /minecraft_server-${SPIGOT_VERSION}.jar;"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/data"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/data"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "25565"
          ]
        },
        {
          "CmdLine": [
            "echo eula=true \u003e /data/eula.txt \u0026\u0026 java -jar /minecraft_server-${SPIGOT_VERSION}.jar"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}