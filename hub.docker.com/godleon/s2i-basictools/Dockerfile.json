            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "INSTALL_PKGS=\"git make jq curl\" \u0026\u0026 export DEBIAN_FRONTEND=noninteractive \u0026\u0026 apt-get -yq update \u0026\u0026 apt-get -yq install $INSTALL_PKGS \u0026\u0026 apt-get clean \u0026\u0026 git clone https://github.com/godleon/s2i-basictools.git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
