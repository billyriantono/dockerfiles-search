{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "centos:centos7",
      "SourceCode": "FROM centos:centos7",
      "Platform": "",
      "From": {
        "Image": "centos:centos7"
      },
      "Commands": [
        {
          "Maintainer": "Camille Meulien \u003ccamille.meulien@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "yum install -y     ansible     epel-release     git     wget;"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "/home/jenkins-slave"
            },
            {
              "Key": "JENKINS_SWARM_VERSION",
              "Value": "2.2"
            },
            {
              "Key": "JENKINS_SWARM_JAR_PATH",
              "Value": "/usr/share/jenkins"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JENKINS_SWARM_JAR",
              "Value": "$JENKINS_SWARM_JAR_PATH/swarm-client-${JENKINS_SWARM_VERSION}-jar-with-dependencies.jar"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "yum install -y java-1.8.0-openjdk-headless     \u0026\u0026 useradd -d $HOME -m jenkins-slave     \u0026\u0026 mkdir -p $JENKINS_SWARM_JAR_PATH     \u0026\u0026 chmod 755 $JENKINS_SWARM_JAR_PATH     \u0026\u0026 curl -sSLo $JENKINS_SWARM_JAR https://repo.jenkins-ci.org/releases/org/jenkins-ci/plugins/swarm-client/$JENKINS_SWARM_VERSION/swarm-client-$JENKINS_SWARM_VERSION-jar-with-dependencies.jar     \u0026\u0026 JENKINS_SWARM_JAR_SHA1=$(curl -s https://repo.jenkins-ci.org/releases/org/jenkins-ci/plugins/swarm-client/$JENKINS_SWARM_VERSION/swarm-client-$JENKINS_SWARM_VERSION-jar-with-dependencies.jar.sha1)     \u0026\u0026 echo \"${JENKINS_SWARM_JAR_SHA1} ${JENKINS_SWARM_JAR}\" | sha1sum -c -"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "entrypoint.sh",
            "/"
          ]
        },
        {
          "CmdLine": [
            "chmod 755 /entrypoint.sh     \u0026\u0026 chown root:root /entrypoint.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "jenkins-slave"
        },
        {
          "Name": "volume",
          "Volumes": [
            "$HOME"
          ]
        },
        {
          "CmdLine": [
            "/entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
}