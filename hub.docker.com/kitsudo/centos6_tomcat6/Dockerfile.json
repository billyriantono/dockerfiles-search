{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "kitsudo/centos6_maven",
      "SourceCode": "FROM kitsudo/centos6_maven",
      "Platform": "",
      "From": {
        "Image": "kitsudo/centos6_maven"
      },
      "Commands": [
        {
          "Maintainer": "Dave Luo \u003ckitsudo163@163.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "TOMCAT_VERSION",
              "Value": "6.0.47"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "TOMCAT_HOME",
              "Value": "/opt/tomcat"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "yum install -y         mysql-server         mysql"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir $TOMCAT_HOME \u0026\u0026 curl -sSL     http://archive.apache.org/dist/tomcat/tomcat-6/v${TOMCAT_VERSION}/bin/apache-tomcat-${TOMCAT_VERSION}.tar.gz |     tar zxv -C $TOMCAT_HOME --strip-components 1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "${TOMCAT_HOME}/bin"
        },
        {
          "Name": "expose",
          "Ports": [
            "8080"
          ]
        },
        {
          "CmdLine": [
            "${TOMCAT_HOME}/bin/catalina.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "run"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}