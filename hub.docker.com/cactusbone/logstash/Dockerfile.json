{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8-jre",
      "SourceCode": "FROM openjdk:8-jre",
      "Platform": "",
      "From": {
        "Image": "openjdk:8-jre"
      },
      "Commands": [
        {
          "Maintainer": "F4 \u003cdev@f4-group.com\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y wget git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget -q https://download.elastic.co/logstash/logstash/logstash-2.4.1.tar.gz -O /tmp/logstash.tar.gz \u0026\u0026     (cd /tmp \u0026\u0026 tar zxf logstash.tar.gz \u0026\u0026 mv logstash-2.4.1 /opt/logstash \u0026\u0026 rm logstash.tar.gz)"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed -i 's|kramdown.*|kramdown (1.14.0)|' /opt/logstash/Gemfile.jruby-1.9.lock"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "run.sh",
            "/usr/local/bin/run"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /usr/local/bin/run"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "git clone -b plugin-api-v1 --depth 1 https://github.com/F4-Group/logstash-input-gelf.git /custom/logstash-input-gelf"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed -i 's|gem \"logstash-input-gelf\".*$|gem \"logstash-input-gelf\", :path =\u003e \"/custom/logstash-input-gelf\"|' /opt/logstash/Gemfile"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "git clone -b plugin-api-v1 --depth 1 https://github.com/F4-Group/logstash-filter-geoip.git /custom/logstash-filter-geoip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "sed -i 's|gem \"logstash-filter-geoip\".*$|gem \"logstash-filter-geoip\", :path =\u003e \"/custom/logstash-filter-geoip\"|' /opt/logstash/Gemfile"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/custom/logstash-filter-geoip"
        },
        {
          "CmdLine": [
            "wget -q https://repo1.maven.org/maven2/org/jruby/jruby-dist/9.2.5.0/jruby-dist-9.2.5.0-bin.tar.gz -O /tmp/jruby.tar.gz \u0026\u0026     (cd /tmp \u0026\u0026 tar zxf jruby.tar.gz \u0026\u0026 mv jruby-9.2.5.0 /opt/jruby \u0026\u0026 rm jruby.tar.gz)"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/opt/jruby/bin/jruby -S gem install bundler"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/opt/jruby/bin/jruby -S bundle install --standalone"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/opt/jruby/bin/jruby -S bundle exec rake vendor"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/opt/jruby/bin/jruby -S bundle exec rake install_jars"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/"
        },
        {
          "CmdLine": [
            "/opt/logstash/bin/logstash-plugin install --no-verify"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "logstash.conf.template",
            "/opt/logstash.conf.template"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "514"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "514/udp"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "12201/udp"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "9500"
          ]
        },
        {
          "CmdLine": [
            "/usr/local/bin/run"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}