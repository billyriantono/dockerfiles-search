        {
          "Name": "workdir",
          "Path": "/go/src/github.com/catkins/statsd-logger"
        },
        {
          "CmdLine": [
            "curl -L https://github.com/golang/dep/releases/download/v0.5.0/dep-linux-amd64 \u003e $GOPATH/bin/dep   \u0026\u0026 chmod +x $GOPATH/bin/dep"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "Gopkg.lock",
            "."
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "Gopkg.toml",
            "."
          ]
        },
        {
          "CmdLine": [
            "dep ensure -v --vendor-only"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            ".",
            "."
          ]
        },
        {
          "Env": [
            {
              "Key": "CGO_ENABLED",
              "Value": "0"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "cd cmd/statsd-logger \u0026\u0026 go install"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    },
    {
      "Name": "",
      "BaseName": "alpine:latest",
      "SourceCode": "FROM alpine:latest",
      "Platform": "",
      "From": {
        "Image": "alpine:latest"
      },
      "Commands": [
        {
          "Key": "BUILD_DATE",
          "Name": "arg",
          "Value": null
        },
        {
          "Key": "VCS_REF",
          "Name": "arg",
          "Value": null
        },
        {
          "Labels": [
            {
              "Key": "org.label-schema.build-date",
              "Value": "$BUILD_DATE"
            },
            {
              "Key": "org.label-schema.vcs-url",
              "Value": "\"https://github.com/catkins/statsd-logger.git\""
