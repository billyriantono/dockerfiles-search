{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "binary-builder",
      "BaseName": "golang:1.11",
      "SourceCode": "FROM golang:1.11 as binary-builder",
      "Platform": "",
      "As": "binary-builder",
      "From": {
        "Image": "golang:1.11"
      },
