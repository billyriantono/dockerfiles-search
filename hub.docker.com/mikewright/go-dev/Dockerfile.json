{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "mikewright/docker-dev-base:latest",
      "SourceCode": "FROM mikewright/docker-dev-base:latest",
      "Platform": "",
      "From": {
        "Image": "mikewright/docker-dev-base:latest"
      },
      "Commands": [
        {
          "Maintainer": "Mike Wright \u003cmkwright@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026     apt-get install -y build-essential libssl-dev \u0026\u0026     /cleanup"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GOLANG_VERSION",
              "Value": "1.6"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl https://storage.googleapis.com/golang/go$GOLANG_VERSION.linux-amd64.tar.gz -o /go.tar.gz \u0026\u0026     tar -C /usr/local -xzf /go.tar.gz \u0026\u0026     rm /go.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:/usr/local/go/bin"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GOPATH",
              "Value": "/src"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "vimrc.bundles.local",
            "/root/.vimrc.bundles.local"
          ]
        },
        {
          "CmdLine": [
            "/update-vim"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}