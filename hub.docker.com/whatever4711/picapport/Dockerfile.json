          "Name": "env"
        },
        {
          "CmdLine": [
            "apk add --update --no-cache openjdk8 curl \u0026\u0026     mkdir -p /opt/picapport \u0026\u0026     curl -SsL -o /opt/picapport/picapport-headless.jar http://picapport.de/prepare_download_headless_jar-${PICAPPORT_VERSION}.php \u0026\u0026     mkdir /opt/picapport/.picapport \u0026\u0026     printf \"%s\\n%s\\n%s\\n\" \"server.port=$PICAPPORT_PORT\" \"irobot.root.0.path=/srv/photos\" \"foto.jpg.usecache=2\" \u003e /opt/picapport/.picapport/picapport.properties"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/opt/picapport"
        },
        {
          "Name": "expose",
          "Ports": [
            "${PICAPPORT_PORT}"
          ]
        },
        {
          "CmdLine": [
            "java",
            "-Xms256m",
            "-Xmx512m",
            "-Duser.home=/opt/picapport",
            "-jar",
            "picapport-headless.jar"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
