      "SourceCode": "FROM ubuntu:trusty",
      "Platform": "",
      "From": {
        "Image": "ubuntu:trusty"
      },
      "Commands": [
        {
          "Key": "SWARM_CLIENT_VERSION",
          "Name": "arg",
          "Value": "3.14"
