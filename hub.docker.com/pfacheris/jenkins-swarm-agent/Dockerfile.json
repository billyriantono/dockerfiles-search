        {
          "Key": "JAVA_PACKAGE",
          "Name": "arg",
          "Value": "openjdk-6-jdk"
        },
        {
          "CmdLine": [
            "useradd -m jenkins"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
