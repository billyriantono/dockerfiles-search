            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SWARM_DISABLE_SSL_VERIFICATION",
              "Value": "false"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SWARM_EXECUTORS",
              "Value": "2"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
