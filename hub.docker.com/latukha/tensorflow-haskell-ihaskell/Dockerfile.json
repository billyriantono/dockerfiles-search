{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "tensorflow/tensorflow:latest-devel-py3",
      "SourceCode": "FROM tensorflow/tensorflow:latest-devel-py3",
      "Platform": "",
      "From": {
        "Image": "tensorflow/tensorflow:latest-devel-py3"
      },
      "Commands": [
        {
          "Maintainer": "Anton Latukha \u003canton.latukha+docker@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "DEBIAN_FRONTEND",
              "Value": "noninteractive"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y         python3-pip git         libtinfo-dev         libzmq3-dev         libcairo2-dev         libpango1.0-dev         libmagic-dev         libblas-dev         liblapack-dev"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "curl -sSL https://get.haskellstack.org/ | sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "PATH=\"$HOME\"/.local/bin:\"$PATH\""
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "\"$HOME\"/git"
        },
        {
          "CmdLine": [
            "git clone --depth 1 https://github.com/gibiansky/IHaskell.git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "\"$HOME\"/git/IHaskell"
        },
        {
          "CmdLine": [
            "pip3 install -r requirements.txt"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "stack setup"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "stack install --ghc-options=-O2 gtk2hs-buildtools"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "stack install --ghc-options=-O2"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "\"$HOME\"/git/IHaskell"
        },
        {
          "CmdLine": [
            "apt-get install -y     libsnappy-dev     libncurses5-dev     libgmp-dev     locales"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install unzip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PROTOBUF_V",
              "Value": "3.3.0"
            },
            {
              "Key": "LIBTFLOWCPU_V",
              "Value": "1.0.0"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl -O -L     https://github.com/google/protobuf/releases/download/v\"$PROTOBUF_V\"/protoc-\"$PROTOBUF_V\"-linux-x86_64.zip \u0026\u0026     unzip -d /usr/local protoc-\"$PROTOBUF_V\"-linux-x86_64.zip bin/protoc \u0026\u0026     chmod 755 /usr/local/bin/protoc \u0026\u0026     curl -L https://storage.googleapis.com/tensorflow/libtensorflow/libtensorflow-cpu-linux-x86_64-\"$LIBTFLOWCPU_V\".tar.gz | tar -xz -C /usr/local \u0026\u0026     ldconfig"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "stack install --ghc-options=-O2 snappy snappy-framing tensorflow-proto tensorflow tensorflow-core-ops tensorflow-ops tensorflow-records tensorflow-records-conduit tensorflow-test tensorflow-opgen tensorflow-logging"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "stack exec ihaskell -- install --stack"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "8888"
          ]
        },
        {
          "Name": "workdir",
          "Path": "\"$HOME\"/git"
        },
        {
          "CmdLine": [
            "stack exec jupyter -- notebook --allow-root"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}