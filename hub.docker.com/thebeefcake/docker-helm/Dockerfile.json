{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "alpine:3.5",
      "SourceCode": "FROM alpine:3.5",
      "Platform": "",
      "From": {
        "Image": "alpine:3.5"
      },
      "Commands": [
        {
          "Maintainer": "Mario Siegenthaler \u003cmario.siegenthaler@linkyard.ch\u003e",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "apk add --update --no-cache ca-certificates git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "VERSION",
              "Value": "v2.5.0"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "FILENAME",
              "Value": "helm-${VERSION}-linux-amd64.tar.gz"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "http://storage.googleapis.com/kubernetes-helm/${FILENAME}",
            "/tmp"
          ]
        },
        {
          "CmdLine": [
            "tar -zxvf /tmp/${FILENAME} -C /tmp   \u0026\u0026 mv /tmp/linux-amd64/helm /bin/helm   \u0026\u0026 rm -rf /tmp"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "/bin/helm"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        }
      ]
    }
  ]
