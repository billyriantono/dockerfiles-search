{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "nukesz/java:latest",
      "SourceCode": "FROM nukesz/java:latest",
      "Platform": "",
      "From": {
        "Image": "nukesz/java:latest"
      },
      "Commands": [
        {
          "Maintainer": "Norbert Benczur, norbert.benczur@gmail.com",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "WILDFLY_VERSION",
              "Value": "10.0.0.Final"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "WILDFLY_HOME",
              "Value": "/opt/wildfly-${WILDFLY_VERSION}"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget http://download.jboss.org/wildfly/${WILDFLY_VERSION}/wildfly-${WILDFLY_VERSION}.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar -zxvf wildfly-${WILDFLY_VERSION}.tar.gz -C /opt \u0026\u0026 rm wildfly-${WILDFLY_VERSION}.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "${WILDFLY_HOME}/bin/standalone.sh -b=0.0.0.0"
          ],
          "Name": "entrypoint",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "8080"
          ]
        }
      ]
    }
  ]
}