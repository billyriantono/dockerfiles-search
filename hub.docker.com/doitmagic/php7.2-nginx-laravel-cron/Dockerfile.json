          "CmdLine": [
            "sed -i -e \"s/;daemonize\\s*=\\s*yes/daemonize = no/g\" /etc/php/7.2/fpm/php-fpm.conf   \u0026\u0026 apt-get autoclean \u0026\u0026 apt-get -y autoremove \u0026\u0026 cp /etc/nginx/sites-available/default /etc/nginx/sites-available/default.bak \u0026\u0026 apt-get autoclean -y --force-yes \u0026\u0026 apt-get clean -y --force-yes"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
