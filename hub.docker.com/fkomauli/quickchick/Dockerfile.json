{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:trusty",
      "SourceCode": "FROM ubuntu:trusty",
      "Platform": "",
      "From": {
        "Image": "ubuntu:trusty"
      },
      "Commands": [
        {
          "Maintainer": "Francesco Komauli \u003cfrancesco.komauli@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "COQBIN",
              "Value": "/usr/local/bin/"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "\"$COQBIN:$PATH\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "COQ_VERSION",
              "Value": "v8.4"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "QUICKCHICK_VERSION",
              "Value": "itp-2015-final"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SSREFLECT_SHA256",
              "Value": "bad978693d1bfd0a89586a34678bcc244e3b7efba6431e0f83d8e1ae8f82a142"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/coq"
          ]
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y     ocaml     camlp5     proofgeneral     proofgeneral-doc     git     vim     wget     make     time"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/tmp"
        },
        {
          "CmdLine": [
            "git clone -b $COQ_VERSION https://github.com/coq/coq.git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/tmp/coq"
        },
        {
          "CmdLine": [
            "./configure -prefix /usr/local  \u0026\u0026 make  \u0026\u0026 make install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/tmp"
        },
        {
          "CmdLine": [
            "wget http://ssr.msr-inria.inria.fr/FTP/ssreflect-1.5.tar.gz  \u0026\u0026 echo \"$SSREFLECT_SHA256  ssreflect-1.5.tar.gz\" | sha256sum -c -  \u0026\u0026 tar -xzf ssreflect-1.5.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/tmp/ssreflect-1.5"
        },
        {
          "CmdLine": [
            "make  \u0026\u0026 make install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/tmp"
        },
        {
          "CmdLine": [
            "git clone https://github.com/QuickChick/QuickChick"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/tmp/QuickChick"
        },
        {
          "CmdLine": [
            "git checkout $QUICKCHICK_VERSION  \u0026\u0026 make  \u0026\u0026 make install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/tmp"
        },
        {
          "CmdLine": [
            "rm -rf coq ssreflect-1.5 QuickChick"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "Name": "workdir",
          "Path": "/coq"
        }
      ]
    }
  ]
}