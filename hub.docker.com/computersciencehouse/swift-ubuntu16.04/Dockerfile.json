{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "computersciencehouse/s2i-base-ubuntu:16.04",
      "SourceCode": "FROM computersciencehouse/s2i-base-ubuntu:16.04",
      "Platform": "",
      "From": {
        "Image": "computersciencehouse/s2i-base-ubuntu:16.04"
      },
      "Commands": [
        {
          "Maintainer": "Steven Mirabito \u003csmirabito@csh.rit.edu\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "SWIFT_VERSION",
              "Value": "3.0"
            }
          ],
          "Name": "env"
        },
        {
          "Labels": [
            {
              "Key": "io.k8s.description",
              "Value": "\"Platform for building Swift applications\""
            },
            {
              "Key": "io.k8s.display-name",
              "Value": "\"Swift 3.0\""
            },
            {
              "Key": "io.openshift.expose-services",
              "Value": "\"8080:http\""
            },
            {
              "Key": "io.openshift.tags",
              "Value": "\"swift,3.0\""
            }
          ],
          "Name": "label"
        },
        {
          "CmdLine": [
            "export DEBIAN_FRONTEND=noninteractive \u0026\u0026 wget -qO- http://dev.iachieved.it/iachievedit.gpg.key | apt-key add - \u0026\u0026 echo \"deb http://iachievedit-repos.s3.amazonaws.com/ xenial main\" \u003e\u003e /etc/apt/sources.list \u0026\u0026 apt-get -yq update \u0026\u0026 apt-get -yq install swift-3.0 \u0026\u0026 apt-get -yq clean \u0026\u0026 export PATH=/opt/swift/swift-3.0/usr/bin:$PATH"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "./.s2i/bin/",
            "/usr/libexec/s2i"
          ]
        },
        {
          "CmdLine": [
            "mkdir -p /bin \u0026\u0026 ln -s /usr/bin/env /bin/env"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "chown -R 1001:1001 /opt/app-root"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "1001"
        },
        {
          "Name": "expose",
          "Ports": [
            "8080"
          ]
        },
        {
          "CmdLine": [
            "/usr/libexec/s2i/usage"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}