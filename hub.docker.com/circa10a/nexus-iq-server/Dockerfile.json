{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "centos:latest",
      "SourceCode": "FROM centos:latest",
      "Platform": "",
      "From": {
        "Image": "centos:latest"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "IQ_VERSION",
              "Value": "1.50.0-01"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JAVA_HOME",
              "Value": "/opt/java"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JAVA_VERSION_MAJOR",
              "Value": "8"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JAVA_VERSION_MINOR",
              "Value": "181"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JAVA_VERSION_BUILD",
              "Value": "13"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SIG",
              "Value": "96a7b8442fe848ef90c96a2fad6ed6d1"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "yum -y update \u0026\u0026 yum clean all"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "mkdir -p /opt   \u0026\u0026 curl --fail --silent --location --retry 3   --header \"Cookie: oraclelicense=accept-securebackup-cookie; \"   http://download.oracle.com/otn-pub/java/jdk/${JAVA_VERSION_MAJOR}u${JAVA_VERSION_MINOR}-b${JAVA_VERSION_BUILD}/\"${SIG}\"/server-jre-${JAVA_VERSION_MAJOR}u${JAVA_VERSION_MINOR}-linux-x64.tar.gz   | gunzip   | tar -x -C /opt   \u0026\u0026 ln -s /opt/jdk1.${JAVA_VERSION_MAJOR}.0_${JAVA_VERSION_MINOR} ${JAVA_HOME}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "SONATYPE_WORK",
              "Value": "/sonatype-work"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir -p /opt/sonatype/iq-server   \u0026\u0026 curl --fail --silent --location --retry 3     https://download.sonatype.com/clm/server/latest.tar.gz   | gunzip   | tar x -C /opt/sonatype/iq-server nexus-iq-server-${IQ_VERSION}.jar config.yml"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "useradd -r -u 201 -m -c \"iq-server role account\" -d ${SONATYPE_WORK} -s /bin/false iq-server"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "${SONATYPE_WORK}"
          ]
        },
        {
          "Env": [
            {
              "Key": "JVM_OPTIONS",
              "Value": "-server"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "expose",
          "Ports": [
            "8070"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "8071"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/opt/sonatype/iq-server"
        },
        {
          "Name": "user",
          "User": "iq-server"
        },
        {
          "CmdLine": [
            "${JAVA_HOME}/bin/java   ${JVM_OPTIONS}   -Ddw.sonatypeWork=${SONATYPE_WORK}   -jar nexus-iq-server-${IQ_VERSION}.jar   server config.yml"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}