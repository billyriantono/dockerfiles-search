{
  "MetaArgs": [
    {
      "Key": "osversion",
      "DefaultValue": "latest",
      "ProvidedValue": null,
      "Value": "latest"
    }
  ],
  "Stages": [
    {
      "Name": "",
      "BaseName": "chloroextractorteam/benchmark-baseimage:${osversion}",
      "SourceCode": "FROM chloroextractorteam/benchmark-baseimage:${osversion}",
      "Platform": "",
      "From": {
        "Image": "chloroextractorteam/benchmark-baseimage:${osversion}"
      },
      "Commands": [
        {
          "Key": "VERSION",
          "Name": "arg",
          "Value": "master"
        },
        {
          "Key": "VCS_REF",
          "Name": "arg",
          "Value": null
        },
        {
          "Key": "BUILD_DATE",
          "Name": "arg",
          "Value": null
        },
        {
          "CmdLine": [
            "echo \"VCS_REF: \"${VCS_REF}\", BUILD_DATE: \"${BUILD_DATE}\", VERSION: \"${VERSION}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Labels": [
            {
              "Key": "maintainer",
              "Value": "\"frank.foerster@ime.fraunhofer.de\""
            },
            {
              "Key": "description",
              "Value": "\"Dockerfile providing an image for GetOrganelle for our benchmark setup\""
            },
            {
              "Key": "version",
              "Value": "${VERSION}"
            },
            {
              "Key": "org.label-schema.vcs-ref",
              "Value": "${VCS_REF}"
            },
            {
              "Key": "org.label-schema.build-date",
              "Value": "${BUILD_DATE}"
            },
            {
              "Key": "org.label-schema.vcs-url",
              "Value": "\"https://github.com/chloroExtractorTeam/benchmark.git\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "GETORGANELLEVERSION",
              "Value": "b1b38da88568a38d97898e67f245019ef62df54b"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "workdir",
          "Path": "/opt"
        },
        {
          "CmdLine": [
            "git clone --recursive https://github.com/Kinggerm/GetOrganelle.git \u0026\u0026     cd GetOrganelle \u0026\u0026     git checkout ${GETORGANELLEVERSION} \u0026\u0026     rm -rf .git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "\"/opt/GetOrganelle:/opt/GetOrganelle/Utilities:$PATH\""
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/data"
          ]
        },
        {
          "Name": "workdir",
          "Path": "/data"
        }
      ]
    }
  ]
}