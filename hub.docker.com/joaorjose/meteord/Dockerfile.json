{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "debian:jessie",
      "SourceCode": "FROM debian:jessie",
      "Platform": "",
      "From": {
        "Image": "debian:jessie"
      },
      "Commands": [
        {
          "Maintainer": "Jesse Rosenberger",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "METEORD_DIR",
              "Value": "/opt/meteord"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "scripts",
            "$METEORD_DIR"
          ]
        },
        {
          "Key": "NODE_VERSION",
          "Name": "arg",
          "Value": null
        },
        {
          "Env": [
            {
              "Key": "NODE_VERSION",
              "Value": "${NODE_VERSION:-4.7.0}"
            }
          ],
          "Name": "env"
        },
        {
          "Expression": "ENV NODE_VERSION ${NODE_VERSION:-4.7.0}",
          "Name": "onbuild"
        },
        {
          "CmdLine": [
            "bash $METEORD_DIR/lib/install_base.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bash $METEORD_DIR/lib/install_node.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bash $METEORD_DIR/lib/install_phantomjs.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "bash $METEORD_DIR/lib/cleanup.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "80"
          ]
        },
        {
          "CmdLine": [
            "bash $METEORD_DIR/run_app.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": true
        }
      ]
    }
  ]
}