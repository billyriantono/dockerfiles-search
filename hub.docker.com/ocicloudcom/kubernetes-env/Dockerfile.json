        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://kubernetes-helm.storage.googleapis.com/helm-v${HELM_VERSION}-linux-amd64.tar.gz",
            "/tmp"
          ]
        },
        {
          "CmdLine": [
            "tar zxvf /tmp/helm-v${HELM_VERSION}-linux-amd64.tar.gz -C /usr/local linux-amd64/helm \u0026\u0026     mv /usr/local/linux-amd64/helm /usr/local/bin/helm \u0026\u0026     rm -f /tmp/helm-v${HELM_VERSION}-linux-amd64.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
