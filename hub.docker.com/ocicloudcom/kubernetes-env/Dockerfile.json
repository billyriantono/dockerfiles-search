        {
          "CmdLine": [
            "tar zxvf /tmp/google-cloud-sdk-${GCLOUD_VERSION}.0.0-linux-x86_64.tar.gz -C /usr/local  \u0026\u0026     rm -f /tmp/google-cloud-sdk-${GCLOUD_VERSION}.0.0-linux-x86_64.tar.gz \u0026\u0026     ln -s /lib /lib64 \u0026\u0026     gcloud config set core/disable_usage_reporting true \u0026\u0026     gcloud config set component_manager/disable_update_check true \u0026\u0026     gcloud config set metrics/environment github_docker_image"
          ],
          "Name": "run",
          "PrependShell": true
