          ]
        },
        {
          "CmdLine": [
            "unzip /tmp/terraform_0.${TERRAFORM_VERSION}_linux_amd64.zip -d /usr/local/bin/ \u0026\u0026     rm -f /tmp/terraform_0.${TERRAFORM_VERSION}_linux_amd64.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
