{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "nginx:latest",
      "SourceCode": "FROM nginx:latest",
      "Platform": "",
      "From": {
        "Image": "nginx:latest"
      },
      "Commands": [
        {
          "Maintainer": "Tomohisa Kusano \u003csiomiz@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "copyables",
            "/"
          ]
        },
        {
          "Env": [
            {
              "Key": "CONFD_VERSION",
              "Value": "0.11.0"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://github.com/kelseyhightower/confd/releases/download/v${CONFD_VERSION}/confd-${CONFD_VERSION}-linux-amd64",
            "/opt/confd/confd"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /opt/confd/confd /entrypoint.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/etc/letsencrypt"
          ]
        },
        {
          "CmdLine": [
            "/entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "Name": "expose",
          "Ports": [
            "443"
          ]
        },
        {
          "CmdLine": [
            "/opt/confd/confd"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}