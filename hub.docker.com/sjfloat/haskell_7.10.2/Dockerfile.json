{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "sjfloat/dev",
      "SourceCode": "from sjfloat/dev",
      "Platform": "",
      "From": {
        "Image": "sjfloat/dev"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "DEBIAN_FRONTEND",
              "Value": "noninteractive"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "CmdLine": [
            "wget -q -O- https://s3.amazonaws.com/download.fpcomplete.com/debian/fpco.key | apt-key add -"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo 'deb http://download.fpcomplete.com/debian/jessie stable main'| tee /etc/apt/sources.list.d/fpco.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y     stack     libtinfo-dev"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get clean"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/*"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "HOME",
              "Value": "/home/$USER"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "stack.yaml",
            "$HOME/.stack/global/stack.yaml"
          ]
        },
        {
          "CmdLine": [
            "chown -R ${USER}: $HOME"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "$USER"
        },
        {
          "Name": "workdir",
          "Path": "$HOME"
        },
        {
          "CmdLine": [
            "stack setup"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "stack install cabal-install"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "stack path --bin-path | path-to-setup stack"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            ". $HOME/.profile \u0026\u0026 cabal update \u0026\u0026 cabal install     ghc-mod     happy     hdevtools     hlint     hoogle     HUnit     parsec     test-framework     test-framework-hunit"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"set path = (\\$path $HOME/.cabal/bin)\" \u003e $HOME/.cshrc.d/z_cabal.csh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"export PATH=\\$PATH:$HOME/.cabal/bin\"  \u003e $HOME/.profile.d/z_cabal.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "haskeline",
            "$HOME/.haskeline"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "hdevtools.vim",
            "$HOME/.vim/plugin/"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "ghc-mod.vim",
            "$HOME/.vim/plugin/"
          ]
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "CmdLine": [
            "chown -R ${USER}:     $HOME/.vim/plugin     $HOME/.haskeline"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "$USER"
        },
        {
          "CmdLine": [
            "git clone https://github.com/Shougo/vimproc.vim.git $HOME/.vim/bundle/vimproc.vim"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "$HOME/.vim/bundle/vimproc.vim"
        },
        {
          "CmdLine": [
            "make"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "$HOME/.vim/bundle/"
        },
        {
          "CmdLine": [
            "wget -O /tmp/ghcmod.vim.zip \"http://www.vim.org/scripts/download_script.php?src_id=23482\"     \u0026\u0026 unzip /tmp/ghcmod.vim.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "$HOME"
        },
        {
          "CmdLine": [
            "$SHELL -l"
          ],
          "Name": "cmd",
          "PrependShell": true
        }
      ]
    }
  ]
}