{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "ubuntu:14.04",
      "SourceCode": "FROM ubuntu:14.04",
      "Platform": "",
      "From": {
        "Image": "ubuntu:14.04"
      },
      "Commands": [
        {
          "Maintainer": "Jérémy SEBAN \u003cjeremy@seban.eu\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "PLEXVERSION",
              "Value": "1.8.1.4139-c789b3fbb"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apt-get -qq update"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get -qq install -y wget"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "locale-gen en_US.UTF-8"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget --quiet https://downloads.plex.tv/plex-media-server/${PLEXVERSION}/plexmediaserver_${PLEXVERSION}_amd64.deb -O plex.deb"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "dpkg -i plex.deb"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm plex.deb"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "volume",
          "Volumes": [
            "/etc/plex"
          ]
        },
        {
          "Env": [
            {
              "Key": "PLEX_MEDIA_SERVER_APPLICATION_SUPPORT_DIR",
              "Value": "/etc/plex"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PLEX_MEDIA_SERVER_HOME",
              "Value": "/usr/lib/plexmediaserver"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PLEX_MEDIA_SERVER_MAX_PLUGIN_PROCS",
              "Value": "6"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PLEX_MEDIA_SERVER_TMPDIR",
              "Value": "/tmp"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LD_LIBRARY_PATH",
              "Value": "/usr/lib/plexmediaserver"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LC_ALL",
              "Value": "en_US.UTF-8"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LANG",
              "Value": "en_US.UTF-8"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "expose",
          "Ports": [
            "32400"
          ]
        },
        {
          "CmdLine": [
            "/usr/lib/plexmediaserver/Plex Media Server"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}