{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:8",
      "SourceCode": "FROM java:8",
      "Platform": "",
      "From": {
        "Image": "java:8"
      },
      "Commands": [
        {
          "Maintainer": "cliffred",
          "Name": "maintainer"
        },
        {
          "CmdLine": [
            "dpkg --add-architecture i386 \u0026\u0026 apt-get update \u0026\u0026 apt-get install -y --force-yes libc6-i386 lib32stdc++6 lib32gcc1 lib32ncurses5 lib32z1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get clean"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "ANDROID_SDK_VERSION",
              "Value": "25.2.5"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_SDK_URL",
              "Value": "https://dl.google.com/android/repository/tools_r${ANDROID_SDK_VERSION}-linux.zip"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_API_LEVELS",
              "Value": "android-25"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_BUILD_TOOLS_VERSION",
              "Value": "25.0.2"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "cd /opt \u0026\u0026 wget --quiet --output-document=sdk.zip ${ANDROID_SDK_URL}"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd /opt \u0026\u0026 unzip -q sdk.zip -d android \u0026\u0026 rm -f sdk.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "ANDROID_HOME",
              "Value": "/opt/android"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "${PATH}:${ANDROID_HOME}/tools"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo y | android update sdk --all --no-ui --filter tools,platform-tools,build-tools-${ANDROID_BUILD_TOOLS_VERSION},${ANDROID_API_LEVELS},extra-android-m2repository"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}