            "echo \"Setting up bash as default shell\" \u0026\u0026     rm /bin/sh \u0026\u0026     ln -s /bin/bash /bin/sh \u0026\u0026     echo \"Setting locale\" \u0026\u0026     locale-gen en_US.UTF-8 \u0026\u0026     echo \"Installing curl\" \u0026\u0026     apt-get update \u0026\u0026     apt-get upgrade -y \u0026\u0026     apt-get install curl -y \u0026\u0026     echo \"Running one-liner\" \u0026\u0026     (curl -s \"$( echo \"$remote_shell_script\" | sed 's/#/%23/g' )\" | bash /dev/stdin \"$toolset_branch\" $server_type $used_modules) \u0026\u0026     echo \"Removing cache\" \u0026\u0026     apt-get clean \u0026\u0026     rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/*"
