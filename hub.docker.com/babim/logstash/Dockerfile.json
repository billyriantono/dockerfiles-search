{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "babim/logstash:base",
      "SourceCode": "FROM babim/logstash:base",
      "Platform": "",
      "From": {
        "Image": "babim/logstash:base"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "LS_VERSION",
              "Value": "7.0.0"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LOGSTASH",
              "Value": "7"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LS_URL",
              "Value": "\"https://artifacts.elastic.co/downloads/logstash\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LS_TARBAL",
              "Value": "\"$LS_URL/logstash-oss-${LS_VERSION}.tar.gz\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SOFT",
              "Value": "logstash"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl -s https://raw.githubusercontent.com/babim/docker-tag-options/master/z%20ElasticStack%20install/${SOFT}_install.sh | bash"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "LOGSTASH_PATH",
              "Value": "/usr/share/logstash/bin"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LS_SETTINGS_DIR",
              "Value": "/usr/share/logstash/config"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$LOGSTASH_PATH:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "LANG",
              "Value": "'en_US.UTF-8'"
            },
            {
              "Key": "LC_ALL",
              "Value": "'en_US.UTF-8'"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/usr/share/logstash/bin:/sbin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Name": "volume",
          "Volumes": [
            "/etc/logstash/conf.d"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "5044",
            "9600"
          ]
        }
      ]
    }
  ]
}