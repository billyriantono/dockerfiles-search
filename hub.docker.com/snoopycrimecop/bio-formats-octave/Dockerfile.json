        "Image": "openmicroscopy/octave:0.3.0"
      },
      "Commands": [
        {
          "Maintainer": "ome-devel@lists.openmicroscopy.org.uk",
          "Name": "maintainer"
        },
        {
          "Key": "VERSION",
          "Name": "arg",
          "Value": "6.0.0-m3"
        },
        {
          "Name": "user",
          "User": "root"
        },
        {
          "CmdLine": [
            "apt-get update     \u0026\u0026 apt-get install -y wget unzip     \u0026\u0026 rm -rf /var/lib/apt/lists/*     \u0026\u0026 apt-get clean"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "user",
          "User": "octave"
        },
        {
          "CmdLine": [
            "wget --user-agent Docker downloads.openmicroscopy.org/bio-formats/$VERSION/artifacts/bioformats-octave-$VERSION.tar.gz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
