          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"/home/octave/bioformats_package.jar\" \u003e\u003e /home/octave/javaclasspath.txt"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo \"pkg install bioformats-octave-$VERSION.tar.gz\" | octave"
          ],
          "Name": "run",
          "PrependShell": true
