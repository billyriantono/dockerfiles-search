{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "joaodubas/nodejs-base:latest",
      "SourceCode": "FROM joaodubas/nodejs-base:latest",
      "Platform": "",
      "From": {
        "Image": "joaodubas/nodejs-base:latest"
      },
      "Commands": [
        {
          "Maintainer": "Joao Paulo Dubas \"joao.dubas@gmail.com\"",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "NODE_VERSION",
              "Value": "0.10.32"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "NODE_FILENAME",
              "Value": "node-v${NODE_VERSION}-linux-x64"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "NODE_TGZ",
              "Value": "${NODE_FILENAME}.tar.gz"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "NODE_SRC",
              "Value": "/opt/src"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "mkdir ${NODE_SRC}     \u0026\u0026 cd ${NODE_SRC}     \u0026\u0026 wget http://nodejs.org/dist/v${NODE_VERSION}/${NODE_TGZ}     \u0026\u0026 tar -xzf ${NODE_TGZ}     \u0026\u0026 ln -s ${NODE_FILENAME} nodejs     \u0026\u0026 ln -s ${NODE_SRC}/nodejs/bin/* /usr/local/bin"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rf ${NODE_SRC}/${NODE_TGZ}"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}