{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "java:8-jdk",
      "SourceCode": "FROM java:8-jdk",
      "Platform": "",
      "From": {
        "Image": "java:8-jdk"
      },
      "Commands": [
        {
          "Maintainer": "Dmitry Rymarev",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_VERSION",
              "Value": "24.4.1"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_API_LEVEL",
              "Value": "23"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ANDROID_BUILD_TOOLS_VERSION",
              "Value": "23.0.3"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl -s -o android-sdk.tgz https://dl.google.com/android/android-sdk_r$ANDROID_VERSION-linux.tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "tar -xvzf android-sdk.tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -f android-sdk.tgz"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "ANDROID_HOME",
              "Value": "/android-sdk-linux"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:$ANDROID_HOME/tools:$ANDROID_HOME/platform-tools"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo y | android update sdk --no-ui --all --filter platform-tools"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | android update sdk --no-ui --all --filter android-$ANDROID_API_LEVEL"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | android update sdk --no-ui --all --filter build-tools-$ANDROID_BUILD_TOOLS_VERSION"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | android update sdk --no-ui --all --filter extra-android-m2repository"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "echo y | android update sdk --no-ui --all --filter extra-android-support"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026 apt-get install -y \tlib32stdc++6 \tlib32z1"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "GRADLE_VERSION",
              "Value": "2.13"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "GRADLE_HOME",
              "Value": "/gradle-$GRADLE_VERSION"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "curl -s -o gradle-bin.zip http://downloads.gradle.org/distributions/gradle-${GRADLE_VERSION}-bin.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "unzip gradle-bin.zip -d /"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -f gradle-bin.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:$GRADLE_HOME/bin"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "BUILD_DIR",
              "Value": "/build"
            }
          ],
          "Name": "env"
        },
        {
          "Expression": "COPY . $BUILD_DIR",
          "Name": "onbuild"
        },
        {
          "Expression": "WORKDIR $BUILD_DIR",
          "Name": "onbuild"
        }
      ]
    }
  ]
}