{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "alpine:3.4",
      "SourceCode": "FROM alpine:3.4",
      "Platform": "",
      "From": {
        "Image": "alpine:3.4"
      },
      "Commands": [
        {
          "Maintainer": "Greg Feigenson \u003cgreg.feigenson@8x8.com\u003e",
          "Name": "maintainer"
        },
        {
          "Env": [
            {
              "Key": "KIBANA_VERSION",
              "Value": "4.6.3"
            },
            {
              "Key": "KIBANA_HOME",
              "Value": "/usr/share/kibana"
            },
            {
              "Key": "PATH",
              "Value": "${PATH}:${KIBANA_HOME}/bin"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk update \u0026\u0026 apk upgrade \u0026\u0026 apk add --no-cache curl nodejs bash \u0026\u0026     curl -Ls https://download.elastic.co/kibana/kibana/kibana-$KIBANA_VERSION-linux-x86_64.tar.gz | tar -xz -C /usr/share \u0026\u0026     ln -s /usr/share/kibana-$KIBANA_VERSION-linux-x86_64 $KIBANA_HOME \u0026\u0026     rm $KIBANA_HOME/node/bin/npm \u0026\u0026     rm $KIBANA_HOME/node/bin/node \u0026\u0026     ln -s /usr/bin/npm $KIBANA_HOME/node/bin/npm \u0026\u0026     ln -s /usr/bin/node $KIBANA_HOME/node/bin/node \u0026\u0026     $KIBANA_HOME/bin/kibana plugin --install elasticsearch/marvel/latest \u0026\u0026     $KIBANA_HOME/bin/kibana plugin --install elastic/sense \u0026\u0026     rm -rf /tmp/* /var/cache/apk/* \u0026\u0026 apk del curl"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "config",
            "$KIBANA_HOME/config"
          ]
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "docker-entrypoint.sh",
            "/"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "5601"
          ]
        },
        {
          "CmdLine": [
            "/docker-entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "kibana"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}