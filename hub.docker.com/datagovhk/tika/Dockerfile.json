{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:9-jre",
      "SourceCode": "FROM openjdk:9-jre",
      "Platform": "",
      "From": {
        "Image": "openjdk:9-jre"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "TIKA_VERSION",
              "Value": "1.17"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "TIKA_PORT",
              "Value": "9998"
            }
          ],
          "Name": "env"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "https://www.apache.org/dist/tika/tika-server-${TIKA_VERSION}.jar",
            "/var/lib/tika/"
          ]
        },
        {
          "Name": "expose",
          "Ports": [
            "${TIKA_PORT}"
          ]
        },
        {
          "CmdLine": [
            "java -jar /var/lib/tika/tika-server-${TIKA_VERSION}.jar                 --host 0.0.0.0                 --port ${TIKA_PORT}"
          ],
          "Name": "entrypoint",
          "PrependShell": true
        }
      ]
    }
  ]
}