{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "google/dart:1.24.3",
      "SourceCode": "FROM google/dart:1.24.3",
      "Platform": "",
      "From": {
        "Image": "google/dart:1.24.3"
      },
      "Commands": [
        {
          "Maintainer": "John Ryan \u003cjohn.p.ryan4@gmail.com\u003e",
          "Name": "maintainer"
        },
        {
          "Labels": [
            {
              "Key": "Description",
              "Value": "\"Dart SDK with content-shell\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "CHANNEL",
              "Value": "stable"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "SDK_VERSION",
              "Value": "1.24.3"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "ARCHIVE_URL",
              "Value": "https://storage.googleapis.com/dart-archive/channels/$CHANNEL/release/$SDK_VERSION"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$PATH:/usr/lib/dart/bin"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "echo \"deb http://us.archive.ubuntu.com/ubuntu precise main multiverse\"     \u003e\u003e /etc/apt/sources.list"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update; echo 'true'"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y --force-yes git wget unzip xvfb chromedriver libgconf-2-4 gdb"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get install -y --force-yes gdb"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get clean"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget $ARCHIVE_URL/sdk/dartsdk-linux-x64-release.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "unzip dartsdk-linux-x64-release.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cp -r dart-sdk/* /usr/local"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rf dartsdk-linux-x64-release.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget $ARCHIVE_URL/dartium/content_shell-linux-x64-release.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "unzip content_shell-linux-x64-release.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm content_shell-linux-x64-release.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/drt-lucid64-full-$CHANNEL-$SDK_VERSION.0:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "ln -s /drt-linux-x64-stable-1.$SDK_VERSION.0/content_shell /usr/local/bin/content_shell"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "wget $ARCHIVE_URL/dartium/dartium-linux-x64-release.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "unzip dartium-linux-x64-release.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "ln -s /dartium-linux-x64-stable-$SDK_VERSION.0/chrome /usr/local/bin/dartium"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "cd / \u0026\u0026 git clone -b dev https://github.com/flutter/flutter.git"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "/flutter/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "flutter doctor"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get  -y --force-yes install libstdc++6 fonts-droid"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "flutter doctor"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "rm -rfv /flutter/bin/cache/artifacts/gradle_wrapper/"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}