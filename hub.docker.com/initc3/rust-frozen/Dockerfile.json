{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "buildpack-deps:buster",
      "SourceCode": "FROM buildpack-deps:buster",
      "Platform": "",
      "From": {
        "Image": "buildpack-deps:buster"
      },
      "Commands": [
        {
          "Env": [
            {
              "Key": "BUILD_DATE",
              "Value": "\"2019-11-21\""
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "RUSTUP_HOME",
              "Value": "/usr/local/rustup"
            },
            {
              "Key": "CARGO_HOME",
              "Value": "/usr/local/cargo"
            },
            {
              "Key": "PATH",
              "Value": "/usr/local/cargo/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "set -eux;         url=\"https://static.rust-lang.org/rustup/dist/x86_64-unknown-linux-gnu/rustup-init\";     wget \"$url\";     chmod +x rustup-init;     ./rustup-init -y --no-modify-path --default-toolchain nightly-$BUILD_DATE;     rm rustup-init;     chmod -R a+w $RUSTUP_HOME $CARGO_HOME;     rustup --version;     cargo --version;     rustc --version;"
          ],
          "Name": "run",
          "PrependShell": true
        }
      ]
    }
  ]
}