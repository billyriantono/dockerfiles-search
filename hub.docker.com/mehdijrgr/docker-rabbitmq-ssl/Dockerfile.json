{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "debian:jessie",
      "SourceCode": "FROM       debian:jessie",
      "Platform": "",
      "From": {
        "Image": "debian:jessie"
      },
      "Commands": [
        {
          "Maintainer": "mehdijrgr",
          "Name": "maintainer"
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "ssl",
            "/ssl"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "scripts",
            "/scripts"
          ]
        },
        {
          "Chown": "",
          "Name": "add",
          "SourcesAndDest": [
            "rabbitmq.config",
            "/etc/rabbitmq/rabbitmq.config"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /scripts/*.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "CmdLine": [
            "apt-get update \u0026\u0026     apt-get install adduser wget init-system-helpers openssl logrotate socat erlang erlang-nox -y \u0026\u0026     apt-get autoclean \u0026\u0026     apt-get autoremove"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Env": [
            {
              "Key": "RABBITMQ_VERSION",
              "Value": "3.6.12"
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "wget https://www.rabbitmq.com/releases/rabbitmq-server/v$RABBITMQ_VERSION/rabbitmq-server_$RABBITMQ_VERSION-1_all.deb \u0026\u0026     dpkg -i rabbitmq-server_$RABBITMQ_VERSION-1_all.deb \u0026\u0026     rm -rf rabbitmq-server_$RABBITMQ_VERSION-1_all.deb \u0026\u0026     rabbitmq-plugins enable rabbitmq_web_stomp rabbitmq_stomp rabbitmq_management"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "expose",
          "Ports": [
            "15672",
            "5671",
            "5672",
            "61613",
            "61614"
          ]
        },
        {
          "CmdLine": [
            "/scripts/run.sh"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}