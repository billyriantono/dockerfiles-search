{
  "MetaArgs": null,
  "Stages": [
    {
      "Name": "",
      "BaseName": "openjdk:8-alpine",
      "SourceCode": "FROM openjdk:8-alpine",
      "Platform": "",
      "From": {
        "Image": "openjdk:8-alpine"
      },
      "Commands": [
        {
          "Labels": [
            {
              "Key": "maintainer",
              "Value": "\"David Sperling \u003cdsperling@smithmicro.com\u003e\""
            }
          ],
          "Name": "label"
        },
        {
          "Env": [
            {
              "Key": "JMETER_VERSION",
              "Value": "apache-jmeter-3.2"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JMETER_HOME",
              "Value": "/opt/$JMETER_VERSION"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "PATH",
              "Value": "$JMETER_HOME/bin:$PATH"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "RESULTS_LOG",
              "Value": "results.jtl"
            }
          ],
          "Name": "env"
        },
        {
          "Env": [
            {
              "Key": "JMETER_FLAGS",
              "Value": ""
            }
          ],
          "Name": "env"
        },
        {
          "CmdLine": [
            "apk add --update --no-cache     curl     openssh-client     unzip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/opt"
        },
        {
          "CmdLine": [
            "curl -O https://archive.apache.org/dist/jmeter/binaries/$JMETER_VERSION.tgz   \u0026\u0026 tar -xvf $JMETER_VERSION.tgz   \u0026\u0026 rm $JMETER_VERSION.tgz   \u0026\u0026 rm -rf $JMETER_VERSION/docs $JMETER_VERSION/printable_docs   \u0026\u0026 curl -O https://jmeter-plugins.org/files/JMeterPlugins-Standard-1.4.0.zip     -O https://jmeter-plugins.org/files/JMeterPlugins-Extras-1.4.0.zip     -O https://jmeter-plugins.org/files/JMeterPlugins-ExtrasLibs-1.4.0.zip   \u0026\u0026 unzip -n '*.zip'   \u0026\u0026 rm *.zip"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Chown": "",
          "From": "",
          "Name": "copy",
          "SourcesAndDest": [
            "entrypoint.sh",
            "/opt/jmeter/"
          ]
        },
        {
          "CmdLine": [
            "chmod +x /opt/jmeter/entrypoint.sh"
          ],
          "Name": "run",
          "PrependShell": true
        },
        {
          "Name": "workdir",
          "Path": "/logs"
        },
        {
          "Name": "expose",
          "Ports": [
            "1099",
            "4445/udp",
            "50000",
            "51000"
          ]
        },
        {
          "CmdLine": [
            "/opt/jmeter/entrypoint.sh"
          ],
          "Name": "entrypoint",
          "PrependShell": false
        },
        {
          "CmdLine": [
            "minion"
          ],
          "Name": "cmd",
          "PrependShell": false
        }
      ]
    }
  ]
}